<mconfig name="Debugger gui functional test config">

    <testbag testattribs="validation" executor="ide" name="Debugger overall" resultsprocessor="qa-functional">
       <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/debuggercore/StartDebugger.class"/>
                <include name="gui/debuggercore/Breakpoints.class"/>
                <include name="gui/debuggercore/LocalVariables.class"/>
                <include name="gui/debuggercore/Watches.class"/>
                <include name="gui/debuggercore/Views.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="start" executor="ide" name="Start Debugger" resultsprocessor="qa-functional">
       <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/debuggercore/StartDebugger.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="breakpoints" executor="ide" name="Breakpoints" resultsprocessor="qa-functional">
       <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/debuggercore/Breakpoints.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="local" executor="ide" name="Local Variables" resultsprocessor="qa-functional">
       <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/debuggercore/LocalVariables.class"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="watches" executor="ide" name="Watches" resultsprocessor="qa-functional">
       <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/debuggercore/Watches.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="views" executor="ide" name="Views" resultsprocessor="qa-functional">
       <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/debuggercore/Views.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <executor name="ide" antfile="build-qa-functional.xml" target="runidetest"/>

    <compiler name="default-compiler" default="true" antfile="build-qa-functional.xml" target="qa-functional-compiler"/>
    
</mconfig>
