<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
  <folder name="Services">
    <folder name="Browsers">
      <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/webbrowsercateg.gif" />

      <file name="SimpleExtBrowser.ser" url="SimpleExtBrowser.ser">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
      </file>
      <file name="UnixWebBrowser.ser" url="UnixWebBrowser.ser">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.UnixWebBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
      </file>
      <file name="WinWebBrowser.ser" url="WinWebBrowser.ser">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.WinWebBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
      </file>
      <attr name="IceBrowser.ser/SimpleExtBrowser.ser" boolvalue="true"/>
      <attr name="IceBrowser.ser/UnixWebBrowser.ser" boolvalue="true"/>
      <attr name="IceBrowser.ser/WinWebBrowser.ser" boolvalue="true"/>
    </folder>
  </folder>

  <folder name="ServiceTemplates">
    <folder name="Browsers">
      <file name="SimpleExtBrowser[org-netbeans-modules-extbrowser-SimpleExtBrowser].instance">
        <attr name="template" boolvalue="true"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
      </file>
    </folder>
  </folder>
</filesystem>
