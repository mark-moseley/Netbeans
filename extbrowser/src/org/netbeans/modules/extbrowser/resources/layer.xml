<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
  <folder name="Services">
    <folder name="Browsers">
      <file name="External Browser (Command Line).ser" url="SimpleExtBrowser.ser"/>
      <!--<file name="External Web Browser.ser" url="ExtWebBrowser.ser"/>-->
      <file name="External Browser (UNIX).ser" url="UnixWebBrowser.ser">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.UnixWebBrowser.isHidden"/>
      </file>
      <file name="External Browser (Windows).ser" url="WinWebBrowser.ser">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.WinWebBrowser.isHidden"/>
      </file>
      <attr name="IceBrowser.ser/External Browser (Command Line).ser" boolvalue="true"/>
      <attr name="IceBrowser.ser/External Browser (Windows).ser" boolvalue="true"/>
      <attr name="IceBrowser.ser/External Browser (UNIX).ser" boolvalue="true"/>
    </folder>
  </folder>

  <folder name="ServiceTemplates">
    <folder name="Browsers">
      <file name="External Browser (Command Line)[org-netbeans-modules-extbrowser-SimpleExtBrowser].instance">
        <attr name="template" boolvalue="true"/>
      </file>
    </folder>
  </folder>
</filesystem>
