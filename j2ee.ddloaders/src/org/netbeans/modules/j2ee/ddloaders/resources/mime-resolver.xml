<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">

<!-- See #9582. -->

<MIME-resolver>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-dd-servlet2.2">
            <xml-rule>
            	<doctype public-id="-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-dd-servlet2.3">
            <xml-rule>
            	<doctype public-id="-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-dd-servlet2.4">
            <xml-rule>
            	<element name="web-app" ns="http://java.sun.com/xml/ns/j2ee">
                    <attr name="version" text="2.4"/>
                </element>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-dd-ejbjar2.0">
            <xml-rule>
            	<doctype public-id="-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-dd-ejbjar2.1">
            <xml-rule>
            	<element name="ejb-jar" ns="http://java.sun.com/xml/ns/j2ee">
                    <attr name="version" text="2.1"/>
                </element>
            </xml-rule>
        </resolver>
    </file>

</MIME-resolver>
