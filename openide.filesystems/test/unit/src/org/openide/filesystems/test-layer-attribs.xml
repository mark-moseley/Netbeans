<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="foo">
        <file name="bar">
            <attr name="x" stringvalue="val"/>
            <attr name="a" methodvalue="org.openide.filesystems.XMLFileSystemTestHid.method"/>
            <attr name="map1" methodvalue="org.openide.filesystems.XMLFileSystemTestHid.map1"/>
            <attr name="map2" methodvalue="org.openide.filesystems.XMLFileSystemTestHid.map2"/>
            <attr name="mapImage" methodvalue="org.openide.filesystems.XMLFileSystemTestHid.mapImage"/>
            <attr name="mapDisplayName" methodvalue="org.openide.filesystems.XMLFileSystemTestHid.mapDisplayName"/>
            <attr name="displayName" bundlevalue="org.openide.filesystems.TestBundle#foobar"/>
