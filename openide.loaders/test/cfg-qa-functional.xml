<mconfig name="openide qa-functional test config">

    <!--
    Stable tests are enumerated bellow
    -->

    <testbag testattribs="validation or full or ds or i18n or dspool or stable" executor="code" name="DO1" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="DataLoaderTests/LoaderPoolTest/DataLoaderPoolTest_explore.class"/>
                <include name="DataLoaderTests/LoaderPoolTest/DataLoaderPoolTest_create.class"/>
                <include name="DataLoaderTests/LoaderPoolTest/DataLoaderPoolTest_find.class"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="validation or full or ds or i18n or dsmanipulation or stable" executor="ide" name="DO2" resultsprocessor="unit">        
        <testset dir="qa-functional/src">
            <patternset>
                <include name="DataLoaderTests/DataObjectTest/Manipulation.class"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="validation or full or ds or i18n or dsmodify or stable" executor="ide" name="DO3" resultsprocessor="unit">        
        <testset dir="qa-functional/src">
            <patternset>
                <include name="DataLoaderTests/DataObjectTest/Modify.class"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="validation or full or ds or i18n or dsvalidity or stable" executor="ide" name="DO4" resultsprocessor="unit">        
        <testset dir="qa-functional/src">
            <patternset>
                <include name="DataLoaderTests/DataObjectTest/Validity.class"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="validation or full or ds or i18n or dsother or stable" executor="ide" name="DO5" resultsprocessor="unit">        
        <testset dir="qa-functional/src">
            <patternset>
                <include name="DataLoaderTests/DataObjectTest/Others.class"/>
            </patternset>
        </testset>
    </testbag>

    

    <testbag testattribs="fs or full or validation or attribs or i18n or stable" executor="ide" name="FileObject attributes test" resultsprocessor="unit">
          <testset dir="qa-functional/src"> 
            <patternset>
                <include name="FileSystemTest/AttrTest.class"/>
            </patternset>
        </testset>
    </testbag>
                     

    <testbag testattribs="full or fsunit or i18n" executor="ide" name="Filesystem unit" resultsprocessor="unit">        
        <testset dir="qa-functional/src">
            <patternset>
                <include name="unit/org/openide/filesystems/FilesystemBugs.class"/>
            </patternset>
        </testset>
    </testbag>

    
    
    
    <testbag testattribs="full or explorer or i18n" executor="ide" name="Explorer tests" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
		<include name="gui/explorer/CopyCutPasteRenameTest.class"/>
        </patternset>
        </testset>        
    </testbag>
    
   <testbag testattribs="validation or ds or stable" executor="ide" name="DO6">
    <testset dir="qa-functional/src">
            <patternset>
                <include name="DataLoaderTests/DataObjectTest/SomeTests.class"/>
            </patternset>     
    </testset>        
    </testbag>


  <executor name="ide" antfile="build-qa-functional.xml" target="runidetest" default="true"/>    
    <executor name="code" antfile="build-qa-functional.xml" target="run-code-test"/>
  <compiler name="qa-functional-compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>

</mconfig>
