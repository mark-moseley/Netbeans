<project name="core/swing/plaf" default="netbeans" basedir=".">
   <import file="../../../nbbuild/templates/projectized.xml"/>

    
    <target name="demo" depends="compile" description="Run a demo">
        <echo message="Found plaf lib at: ${plaf-lib}"/>
        <echo>And I am &#x05D9;&#x05E9;&#x05D9; &#x05D1;&#x05E0; &#x05DC;&#x05D6;&#x05E8; or similar.</echo>
        <echo message="Demo args: ${application.args}"/>
        <java fork="true" classname="org.netbeans.swing.plaf.gtk.GtkToolbarUI" args="${application.args}">
            <classpath>
                <pathelement location="${build.classes.dir}"/>
                <pathelement path="${module.classpath}"/>
                <pathelement location="${plaf-lib}"/>
            </classpath>
        </java>
    </target>
</project>
