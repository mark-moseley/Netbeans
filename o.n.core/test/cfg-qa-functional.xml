    <executor name="code" antfile="build-qa-functional.xml" target="runcodetest"/>
    
    <executor name="ide" antfile="build-qa-functional.xml" target="runidetest"/>

    <executor name="ide_firststart" antfile="build-qa-functional.xml" target="runidetest_firststart"/>
    
    <executor name="ide_secondstart" antfile="build-qa-functional.xml" target="runidetest_secondstart"/>
    
    <executor name="platform" antfile="build-qa-functional.xml" target="runplatformtest"/>

    <compiler name="QAcompiler" antfile="build-qa-functional.xml" target="QAcompiler" default="true"/>
    
    <resultsprocessor name="unit" antfile="build.xml" target="test_report" default="true"/>

</mconfig>
