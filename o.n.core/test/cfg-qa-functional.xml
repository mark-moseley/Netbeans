<mconfig name="core qa-functional test config">
<!-- START Window System testbags -->
    <testbag testattribs="validation or full or ide or ws" executor="ide_firststart" prio="1" name="Initial position of windows after first start" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>                
                <include name="gui/windowsystem/validation/Validation_Init.class"/>		               
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="validation or full or ide or ws" executor="ide_secondstart" prio="2" name="Initial position of windows after restart" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>                
                <include name="gui/windowsystem/validation/Validation_Init_1.class"/>		               
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="validation or full or ide or ws" executor="ide" name="Docking, Attaching, Resize, Move windows in IDE" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/windowsystem/validation/Validation_Docking.class"/>		
                <include name="gui/windowsystem/validation/Validation_ResizeMove.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="full or ide or ws" executor="ide" name="Project switching" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/windowsystem/validation/Validation_ProjectSwitching.class"/>		
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(validation and not full) or ide" executor="ide" name="Main Window tests - validation" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testMainMenuMnemonicsCollision"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testMainMenuShortCutCollision"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testMenuItemsWhenFilesystemSelected"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testMenuItemsWhenJavaSelected"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testMenuItemsWhenFormSelected"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testPopupMenuItemsWhenFilesystemSelected"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testPopupMenuItemsWhenJavaSelected"/>
                <include name="gui/windowsystem/mainwindow/MainMenu.class/testPopupMenuItemsWhenFormSelected"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="full or ide or ws" executor="ide" name="Main Window tests" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/windowsystem/mainwindow/MainMenu.class"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="(validation and not full) or ide" executor="ide" name="MDI Workspaces tests - validation" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/windowsystem/mdi/WorkspacesTest.class/testSwitchingWorkspacesByMenu"/>
                <include name="gui/windowsystem/mdi/WorkspacesTest.class/testSwitchingWorkspacesByClick"/>
                <include name="gui/windowsystem/mdi/WorkspacesTest.class/testSwitchingWorkspacesByPopup"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="full or ide or ws" executor="ide" name="MDI Workspaces tests" resultsprocessor="unit">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/windowsystem/mdi/WorkspacesTest.class"/>		
            </patternset>
        </testset>
    </testbag>

<!-- END Window System testbags -->


