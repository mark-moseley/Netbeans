<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

  <folder name="Menu">
	<folder name="View">
            <file name="org-netbeans-modules-javadoc-search-SearchDocAction.instance"/>
            <attr name="org-netbeans-core-actions-HTMLViewAction.instance/org-netbeans-modules-javadoc-search-SearchDocAction.instance" boolvalue="true" />
            <attr name="org-netbeans-modules-form-actions-ComponentInspectorAction.instance/org-netbeans-modules-javadoc-search-SearchDocAction.instance" boolvalue="true" />
            <attr name="org-netbeans-modules-objectbrowser-ShowBrowserAction.instance/org-netbeans-modules-javadoc-search-SearchDocAction.instance" boolvalue="true" />
            <attr name="org-netbeans-modules-javadoc-search-SearchDocAction.instance/Separator2[javax-swing-JSeparator].instance" boolvalue="true" />
        </folder>
  </folder>

  <folder name="Actions">
    <folder name="Help">
      <file name="Search Doc Action[org-netbeans-modules-javadoc-search-SearchDocAction].instance" />
    </folder>
    <folder name="Build">
      <file name="Generate Javadoc Action[org-netbeans-modules-javadoc-GenerateDocAction].instance" />
    </folder>
    <folder name="Tools">
      <file name="Auto Comment Action[org-netbeans-modules-javadoc-comments-AutoCommentAction].instance" />
      <file name="Correct Javadoc Action[org-netbeans-modules-javadoc-comments-CorrectJavaDocAction].instance" />
    </folder>
  </folder>

  <folder name="Shortcuts">
	<file name="C-F1[org-netbeans-modules-javadoc-search-SearchDocAction].instance" />
  </folder> 

  <folder name="xml">
    <folder name="entities">
        <folder name="NetBeans_IDE">
            <!-- public ID defined by LibraryProcessor -->
            <file name="DTD_JavadocLibrary" url="nbresloc:/org/netbeans/modules/javadoc/search/environment/library.dtd"/>
        </folder>    
    </folder>
  </folder>

  <folder name="xml">
    <folder name="lookups">
        <folder name="NetBeans_IDE">
            <!-- public ID defined by LibraryProcessor -->
            <file name="DTD_JavadocLibrary.instance">
                <attr name="instanceCreate" newvalue="org.netbeans.modules.javadoc.search.environment.JavadocProcessor" />
            </file>
        </folder>    
    </folder>
  </folder>

<!--  Services part -->

  <folder name="Services">
    <file name="org.netbeans.modules.javadoc.settings.DocumentationSettings.settings" url="DocumentationSettings.settings"/>

      <folder name="JavadocExecutors">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.javadoc.settings.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/javadoc/resources/JavaDoc.gif"/>

        <file name="javadoc_external_executor.settings" url="javadoc_external_executor.settings"/>
        <file name="javadoc_internal_executor.settings" url="javadoc_internal_executor.settings"/>
      </folder>  

      <folder name="JavadocDoclets">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.javadoc.settings.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/javadoc/resources/JavaDoclet.gif"/>

        <file name="javadoc_doclets.settings" url="javadoc_doclets.settings"/>
      </folder>

      <folder name="JavadocSearchTypes">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.javadoc.search.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/javadoc/resources/searchDoc.gif"/>

        <file name="javadoc_search.settings" url="javadoc_search.settings"/>
        <file name="javadoc_japan_search.settings" url="javadoc_japan_search.settings"/>
      </folder>  
  </folder>

<!--  Templates part -->

  <folder name="Templates">
      <folder name="Services">
          <folder name="JavadocExecutors">
            <file name="javadoc_external_executor.settings" url="javadoc_external_executor.settings">
              <attr name="template" boolvalue="true"/>
            </file>    
            <file name="javadoc_internal_executor.settings" url="javadoc_internal_executor.settings">
              <attr name="template" boolvalue="true"/>
            </file>
          </folder>  

          <folder name="JavadocDoclets">
            <file name="javadoc_doclets.settings" url="javadoc_doclets.settings">
              <attr name="template" boolvalue="true"/>
            </file>
          </folder>  

          <folder name="JavadocSearchTypes">
            <file name="javadoc_search.settings" url="javadoc_search.settings">
              <attr name="template" boolvalue="true"/>
            </file>
            <file name="javadoc_japan_search.settings" url="javadoc_japan_search.settings">
              <attr name="template" boolvalue="true"/>
            </file>
          </folder>  
      </folder>
   </folder>   
</filesystem>
