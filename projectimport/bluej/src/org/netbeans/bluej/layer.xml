<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="OptionsDialog">
            <file name="BlueJ.instance">
                <attr name="instanceClass" stringvalue="org.netbeans.bluej.options.BluejAdvancedOption"/>
            </file>
   </folder>
  <folder name="Services">
    
    <folder name="AutoupdateType">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>

      <file name="bluej.settings" url="autoupdate_xml_type.settings">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
        <attr name="url_key" stringvalue="URL_Default_N"/>
        <attr name="enabled" boolvalue="true"/>
      </file>
    </folder>
    
      <folder name="JavaHelp">
            <file name="bluej-helpset.xml" url="bluej-helpset.xml"/>
        </folder>
        <file name="org-netbeans-bluej-options-BlueJSettings.settings" url="options/BlueJSettings.xml">
      </file>
  </folder>
    <folder name="Templates">
        <folder name="Bluej">
            <file name="Abstract.java" url="resources/templates/newclass/abstract.tmpl">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="bluej"/>
            </file>
            <file name="Enum.java" url="resources/templates/newclass/enum.tmpl">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="bluej"/>
            </file>
            <file name="Interface.java" url="resources/templates/newclass/interface.tmpl">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="bluej"/>
            </file>
            <file name="StdClass.java" url="resources/templates/newclass/stdclass.tmpl">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="bluej"/>
            </file>
            <file name="UnitTest.java" url="resources/templates/newclass/unittest.tmpl">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="bluej"/>
            </file>
            <attr name="UnitTest.java/Package.shadow" boolvalue="true"/>
            <file name="Package.shadow">
                <attr name="originalFile" stringvalue="Templates/Classes/Package"/>
            </file>
            
        </folder>
        <!-- override the default definition with our wrapper that adds a bluej.pkg file into the newly created directory. -->
        <folder name="Classes">
            <file name="Package" url="resources/Package.template">
                <attr name="instantiatingIterator" methodvalue="org.netbeans.bluej.packagewizard.PackageWizardIterator.createWizard"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/spi/java/project/support/ui/package.gif"/>
                <attr name="templateCategory" stringvalue="java-classes, java-classes-basic"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.packagewizard.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/bluej/packagewizard/Package.html"/>
            </file>        
            
        </folder>
        <folder name="Project">
            <folder name="BlueJ">
                <file name="EmptyBluejProject.zip" url="EmptyBluejProject.zip">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.bluej.Bundle"/>
                    <attr name="instantiatingIterator" methodvalue="org.netbeans.bluej.wizard.EmptyBluejWizardIterator.createIterator"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/netbeans/bluej/wizard/EmptyBluejDescription.html"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/bluej/resources/bluejproject.png"/>
                    <attr name="template" boolvalue="true"/>
                </file>
            </folder>
        </folder>
    </folder>

</filesystem>
