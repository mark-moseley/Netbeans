<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Buildsystem">
        <folder name="org.netbeans.modules.kjava.j2meproject">
            <folder name="nbproject">
                <folder name="build-impl.xml">
                    <folder name="init-subtargets">
                        <attr name="semc-init/savaje-init" boolvalue="true"/>
                        <file name="savaje-init" url="savaje-init"/>
                    </folder>
                    <folder name="jar-subtargets">
                        <attr name="ricoh-build-dalp/savaje-build-jnlp" boolvalue="true"/>
                        <file name="savaje-build-jnlp" url="savaje-build-jnlp"/>
                    </folder>
                    <folder name="run-subtargets">
                        <attr name="semc-run/savaje-run" boolvalue="true"/>
                        <file name="savaje-run" url="savaje-run"/>
                    </folder>
                    <folder name="debug-subtargets">
                        <attr name="semc-debug/savaje-debug" boolvalue="true"/>
                        <file name="savaje-debug" url="savaje-debug"/>
                    </folder>
                    <folder name="clean-subtargets">
                        <attr name="clean-semc/clean-savaje" boolvalue="true"/>
                        <file name="clean-savaje" url="clean-savaje"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Customizer">
        <folder name="org.netbeans.modules.kjava.j2meproject">
            <folder name="Jad">
                <attr name="CDCGeneral/SavaJe" boolvalue="true"/>
                <file name="SavaJe">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.j2me.cdc.project.savaje.Bundle"/>
                    <attr name="customizerPanelClass" newvalue="org.netbeans.modules.j2me.cdc.project.savaje.SavaJeProjectCategoryCustomizer"/>
                    <attr name="triggerPropertyName" stringvalue="platform.type"/>
                    <attr name="triggerPropertyValue" stringvalue="savaje"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="MainTemplates">
        <folder name="org.netbeans.modules.kjava.j2meproject">
            <file name="savaje">
                <attr name="templateName" stringvalue="Templates/GUIForms/JFrame.java"/>
                <attr name="templateType" stringvalue="main"/>
            </file>
        </folder>
    </folder>    
    <folder name="org-netbeans-api-project-libraries">
        <folder name="Libraries">
            <file name="savaje-ant-utils.xml" url="Utils.xml"/>
        </folder>
    </folder>    
</filesystem>
