<!-- 
    The following validators are defined	
        javaid:		A legal java identifier (allows java keywords though)
        package:	A legal java package name, e.g. javaid's separated by periods.
        memorysize:	A number followed by kb or mb (case insensitive)
        domain:		A domain.  This is probably similar to javaid + represents a server domain
        directory:	A directory path specification
        url:		A URL string
        classid:	A windows classid (GUID)
-->
    <validator>
        <validator-name>javaidentifier</validator-name>
        <validator-pattern>[a-zA-Z_$][$\w]*</validator-pattern>
    </validator>
    <validator>
        <validator-name>javapackage</validator-name>
        <validator-pattern>([A-Za-z_$][$\w]*)(\.[$A-Za-z_][$\w]*)*</validator-pattern>
    </validator>
    <validator>
        <validator-name>memorysize</validator-name>
        <validator-pattern>\d*([kKmM][bB])?</validator-pattern>
    </validator>
    <validator>
        <validator-name>domain</validator-name>
        <validator-pattern>.*</validator-pattern> <!-- FIXME - not defined yet -->
    </validator>
    <validator>
        <validator-name>directory</validator-name>
        <validator-pattern>.*</validator-pattern> <!-- FIXME - not defined yet -->
    </validator>
    <validator>
        <validator-name>url</validator-name>
        <validator-pattern>.*</validator-pattern> <!-- FIXME - not defined yet -->
    </validator>
    <validator>
        <validator-name>classid</validator-name>
        <validator-pattern>(?i)clsid:[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}</validator-pattern>
    </validator>
</dynamic-properties>
