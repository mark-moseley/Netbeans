gunzip -c SUNWnetbeans-platform4.tar.gz | tar xf -
gunzip -c SUNWnetbeans-ide4-modules.tar.gz | tar xf -
gunzip -c SUNWnetbeans-ide.tar.gz | tar xf -

pkgadd -d ./ SUNWnetbeans-platform4 SUNWnetbeans-ide4-modules SUNWnetbeans-ide

rm -Rf SUNWnetbeans-platform4 SUNWnetbeans-ide4-modules SUNWnetbeans-ide

ls -l /opt/netbeans
ls -l /usr/bin/netbeans
ls -l /etc/netbeans.conf
ls -l /usr/share/applications/NetBeans-IDE-4.0.desktop
ls -l /usr/share/pixmaps/nb48x48.png
