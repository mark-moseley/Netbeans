    <choice id="glassfish" title="@GF_TITLE@" description="Free, open source, production-quality application server that is also the reference implementation for Java EE 5 specification." start_selected="true" start_enabled="true" start_visible="true">
        <pkg-ref id="ident.pkg.glassfish"></pkg-ref>
    </choice>
    <choice id="tomcat" title="@TC_TITLE@" description="Free, open source web server that is the reference implementation for Java servlet and JSP specifications." start_selected="false" start_enabled="true" start_visible="true">
        <pkg-ref id="ident.pkg.tomcat"></pkg-ref>
    </choice>
    <choice id="openesb" title="@OESB_TITLE@" description="Open ESB implements an Enterprise Service Bus (ESB) runtime using Java Business Integration (JBI) as the foundation. Open ESB is an open standard, open-source product that offers a platform to build integration solutions based on service-oriented architecture (SOA)." start_selected="true" start_enabled="true" start_visible="true" selected="choices.openesb.selected &amp;&amp; choices.glassfish.selected" enabled="choices.glassfish.selected">
        <pkg-ref id="ident.pkg.openesb"></pkg-ref>
    </choice>
    <choice id="netbeans" title="@NB_TITLE@" description="NetBeans IDE" start_selected="true" start_enabled="true" start_visible="true"></choice>
    <choice id="baseide" title="Base IDE" description="Core components of an integrated development environment." start_selected="true" start_enabled="true" start_visible="true">
        <pkg-ref id="org.netbeans.ide"></pkg-ref>
    </choice>
    <choice id="javase" title="Java SE" description="Essential tools for programming in Java, including an editor, debugger, profiler, refactoring support, and award winning drag-and-drop GUI design tool (formerly known as Project Matisse)." start_selected="true" start_enabled="true" start_visible="true" selected="choices.javase.selected &amp;&amp; choices.baseide.selected" enabled="choices.baseide.selected">
        <pkg-ref id="ident.pkg.javase"></pkg-ref>
    </choice>
    <choice id="webjavaee" title="Web &amp; Java EE" description="Tools for creating Java web and enterprise applications compatible with the J2EE 1.3, J2EE 1.4, and Java EE 5 specifications. Includes support for servlets, JSPs, tag libraries, Struts, Java Persistence API, Enterprise Java Beans, JAX-WS, and Java Server Faces. Provides visual drag-and-drop application development environment." start_selected="true" start_enabled="true" start_visible="true" selected="choices.webjavaee.selected &amp;&amp; choices.javase.selected" enabled="choices.javase.selected">
        <pkg-ref id="ident.pkg.javaee"></pkg-ref>
    </choice>
    <choice id="cpp" title="C/C++" description="Tools for developing C and C++ applications. Includes project templates, support for existing projects, advanced editor, debug support, and makefile wizard for configuration management." start_selected="true" start_enabled="true" start_visible="true" selected="choices.cpp.selected &amp;&amp; choices.baseide.selected" enabled="choices.baseide.selected">
        <pkg-ref id="ident.pkg.cpp"></pkg-ref>
    </choice>
    <choice id="ruby" title="Ruby" description="Complete tool set for creating Ruby and Ruby on Rails applications. Includes JRuby interpreter, Ruby on Rails framework, powerful editor, debugger, gem manager, and interactive Ruby shell. Supports easily creating, modifying, and running Ruby on Rails applications." start_selected="true" start_enabled="true" start_visible="true" selected="choices.ruby.selected &amp;&amp; choices.baseide.selected" enabled="choices.baseide.selected">
        <pkg-ref id="ident.pkg.ruby"></pkg-ref>
    </choice>
    <choice id="soa" title="SOA" description="Tools for creating composite applications as part of a Service Oriented Architecture. Provides comprehensive BPEL support. Includes graphical editor, debugger, and refactoring support." start_selected="true" start_enabled="true" start_visible="true" selected="choices.soa.selected &amp;&amp; choices.webjavaee.selected &amp;&amp; choices.openesb.selected" enabled="choices.webjavaee.selected &amp;&amp; choices.openesb.selected">
        <pkg-ref id="ident.pkg.soa"></pkg-ref>
    </choice>
