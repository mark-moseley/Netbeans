    </choice>  
    <choice id="openesb" title="openesb-title" description="openesb-description" start_selected="true" start_enabled="true" start_visible="true" selected="choices.openesb.selected &amp;&amp; choices.glassfish.selected" enabled="choices.glassfish.selected">
