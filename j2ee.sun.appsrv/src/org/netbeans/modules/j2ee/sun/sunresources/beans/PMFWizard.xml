<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE wizard>
<wizard>
    <name>persistence-manager-factory-resource</name>
    <field-group>
        <name>general</name>
        <field field-type="string" required="true">
            <name>jndi-name</name>
            <field-value>
                <default-field-value>jdo/</default-field-value>
            </field-value>
        </field>
        <field field-type="string">
            <name>factory-class</name>
            <field-value>
                <default-field-value>com.sun.jdo.spi.persistence.support.sqlstore.impl.PersistenceManagerFactoryImpl</default-field-value>
            </field-value>
        </field>
<!--   
        <field field-type="string">
            <name>jdbc-resource-jndi-name</name>
            <field-value>
                <default-field-value></default-field-value>
            </field-value>
        </field>
-->
        <field field-type="list">
            <name>enabled</name>
            <field-value>
                <default-field-value>true</default-field-value>
            </field-value>
            <tag>
                <tag-item>true</tag-item>
                <tag-item>false</tag-item>
            </tag>
        </field>
        <field field-type="textarea">
            <name>description</name>
            <field-value>
                <default-field-value></default-field-value>
            </field-value>
        </field>
    </field-group>
    <field-group>
        <name>properties</name>
        <field field-type="string">
        </field>
    </field-group>
</wizard>

