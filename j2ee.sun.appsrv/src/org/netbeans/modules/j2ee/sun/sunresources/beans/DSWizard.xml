<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE wizard>
<wizard>
    <name>jdbc-resource</name>
    <field-group>
        <name>general</name>
        <field field-type="string" required="true">
            <name>jndi-name</name>
            <field-value>
                <default-field-value>jdbc/</default-field-value>
            </field-value>
        </field>
        <field field-type="list">
            <name>object-type</name>
            <field-value>
                <default-field-value>user</default-field-value>
            </field-value>
            <tag> 
                <tag-item>user</tag-item>
                <tag-item>system-all</tag-item>
                <tag-item>system-admin</tag-item>
                <tag-item>system-instance</tag-item>
            </tag>            
        </field>
        <field field-type="list">
            <name>enabled</name>
            <field-value>
                <default-field-value>true</default-field-value>
            </field-value>
            <tag>
                <tag-item>true</tag-item>
                <tag-item>false</tag-item>
            </tag>
        </field>
        <field field-type="textarea">
            <name>description</name>
            <field-value>
                <default-field-value></default-field-value>
            </field-value>
        </field>        
    </field-group>
    <field-group>
        <name>properties</name>
        <field field-type="string">
        </field>
    </field-group>
</wizard>

