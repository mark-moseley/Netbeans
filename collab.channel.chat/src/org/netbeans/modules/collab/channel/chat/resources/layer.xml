<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<filesystem>
    <folder name="Services">
        <folder name="Hidden">
            <file name="chat-Chat.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.collab.channel.chat.resources.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:org/netbeans/modules/collab/channel/chat/resources/chat_bubble.png"/>
                <attr name="contentType" stringvalue="text"/>
                <attr name="instanceOf" stringvalue="org.netbeans.modules.collab.channel.chat.messagetype.CollabContentType"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.collab.channel.chat.messagetype.CollabContentTypeImpl.create"/>
            </file>
        </folder>
        <folder name="Collaboration">
            <folder name="Channels">
                <file name="org-netbeans-modules-collab-channel-chat-ChatCollabletFactory.instance"/>
                <file name="org-netbeans-modules-collab-channel-chat-ChatCollabletFactorySettings.settings" url="ChatCollabletFactorySettings.settings">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.collab.channel.chat.Bundle"/>
                    <attr name="SystemFileSystem.layer" stringvalue="session"/>
                </file>
            </folder>
        </folder>
        <folder name="JavaHelp">
            <!-- Merge it after the main online help: -->
            <attr name="org-netbeans-modules-usersguide-above-regular.txt/org-netbeans-modules-collab-channel-chat-helpset.xml"
                    boolvalue="true"/>
            <!-- This DTD gives an instance of HelpSet: -->
            <file name="org-netbeans-modules-collab-channel-chat-helpset.xml"
                    url="org-netbeans-modules-collab-channel-chat-helpset.xml" />
            <!-- Merge before additional helpsets such as the Ant Manual. -->
            <attr name="org-netbeans-modules-collab-channel-chat-helpset.xml/org-netbeans-modules-usersguide-below-regular.txt"
                    boolvalue="true"/>
        </folder>                
    </folder>

</filesystem>
