	<script type="text/javascript" src="../../resources/libs/dojo/v0.4.3/djd43.js"></script>
        <script type="text/javascript" src="./Support.js"></script>
	<script type="text/javascript">
		function loadResourcesTable() {
			var restStore = new rdj.data.__CONTAINER_NAME__Store({serviceUrl:"http://localhost:8080/__PROJECT_NAME__/resources/__CONTAINER_PATH_NAME__/"});
			var tableElement = djd43.byId("content");
			var query = null;
			rdj.displayResourcesTable(restStore, query, tableElement);
		}		
		djd43.addOnLoad(loadResourcesTable);
	</script>
</head>
<body>
	<h1>__CONTAINER_NAME__ table widget showing RestStore Dojo Component</h1>
