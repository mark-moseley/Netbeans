<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="WebServices">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.websvc.rest.wizard.Bundle"/>
            <file name="RESTServicesFromEntities">         
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="web-types"/>
                <attr name="templateWizardIterator" newvalue="org.netbeans.modules.websvc.rest.wizard.RESTServicesFromEntitiesIterator" />
                <!--attr name="templateCategory" stringvalue="web-services"/-->
                <attr name="templateWizardURL"
                      urlvalue="nbresloc:/org/netbeans/modules/websvc/rest/wizard/resources/RESTServicesFromEntities.html"/>
                <attr name="SystemFileSystem.localizingBundle"
                      stringvalue="org.netbeans.modules.websvc.rest.wizard.Bundle"/>
                <attr name="SystemFileSystem.icon"
                      urlvalue="nbresloc:/org/netbeans/modules/websvc/rest/resources/RESTServiceIcon.png"/> 
            </file>
            <file name="GenericRESTServices">
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardIterator" newvalue="org.netbeans.modules.websvc.rest.wizard.GenericRESTServicesIterator" />
                <attr name="templateWizardURL"
                      urlvalue="nbresloc:/org/netbeans/modules/websvc/rest/wizard/resources/RESTServicesFromPatterns.html"/>
                <attr name="SystemFileSystem.localizingBundle"
                      stringvalue="org.netbeans.modules.websvc.rest.wizard.Bundle"/>
                <attr name="SystemFileSystem.icon"
                      urlvalue="nbresloc:/org/netbeans/modules/websvc/rest/resources/RESTServiceIcon.png"/> 
            </file>
            <file name="GenericResource.java" url="/org/netbeans/modules/websvc/rest/codegen/resources/GenericResource.template"/>
            <file name="UriResolver.java" url="/org/netbeans/modules/websvc/rest/codegen/resources/UriResolver.template"/>
            <file name="PersistenceService.java" url="/org/netbeans/modules/websvc/rest/codegen/resources/PersistenceService.template"/>
            <file name="GoogleMapResource.java" url="/org/netbeans/modules/websvc/rest/codegen/resources/GoogleMapResource.template"/>
            <file name="GoogleMapRefConverter.java" url="/org/netbeans/modules/websvc/rest/codegen/resources/GoogleMapRefConverter.template"/>
        </folder>
    </folder>
    
    <folder name="Editors">
        <folder name="text">
            <folder name="x-java">                
                <file name="PaletteFactory.instance">                    
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.websvc.rest.component.palette.RestPaletteFactory.createPalette"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.spi.palette.PaletteController"/>
                </file>
            </folder>
        </folder>
    </folder>   
    
    <folder name="RestPalette">
        <folder name="Google">
            <attr name="SystemFileSystem.localizingBundle" 
                stringvalue="org.netbeans.modules.websvc.rest.component.palette.Bundle"/>
            <file name="googlemap.xml" url="/org/netbeans/modules/websvc/rest/component/palette/googlemap.xml" />
        </folder>
        <folder name="Component">
            <attr name="SystemFileSystem.localizingBundle" 
                stringvalue="org.netbeans.modules.websvc.rest.component.palette.Bundle"/>
            <file name="component.xml" url="/org/netbeans/modules/websvc/rest/component/palette/component.xml" />
        </folder>
    </folder>    
    
</filesystem>
