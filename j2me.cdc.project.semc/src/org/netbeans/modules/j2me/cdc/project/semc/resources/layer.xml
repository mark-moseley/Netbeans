<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Buildsystem">
        <folder name="org.netbeans.modules.kjava.j2meproject">
            <folder name="nbproject">
                <folder name="build-impl.xml">
                    <folder name="init-subtargets">
                        <attr name="ricoh-init/semc-init" boolvalue="true"/>
                        <file name="semc-init" url="semc-init"/>
                    </folder>
                    <folder name="jar-subtargets">
                        <attr name="nokiaS80-create-manifest/semc-build-j9" boolvalue="true"/>
                        <file name="semc-build-j9" url="semc-build-j9"/>
                        <attr name="semc-build-j9/do-jar" boolvalue="true"/>
                    </folder>
                   <folder name="build-subtargets">
                        <attr name="override-jad/semc-sis" boolvalue="true"/>
                        <file name="semc-sis" url="semc-sis"/>
                        <attr name="semc-sis/increment-app-version" boolvalue="true"/>
                    </folder>
                    <folder name="run-subtargets">
                        <attr name="ricoh-run/semc-run" boolvalue="true"/>
                        <file name="semc-run" url="semc-run"/>
                    </folder>
                    <folder name="debug-subtargets">
                        <attr name="ricoh-debug/semc-debug" boolvalue="true"/>
                        <file name="semc-debug" url="semc-debug"/>
                    </folder>
                    <folder name="clean-subtargets">
                        <attr name="clean-ricoh/clean-semc" boolvalue="true"/>
                        <file name="clean-semc" url="clean-semc"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>    
    <folder name="Customizer">
        <folder name="org.netbeans.modules.kjava.j2meproject">
            <folder name="Jad">
                <attr name="CDCGeneral/SEMC" boolvalue="true"/>
                <file name="SEMC">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.j2me.cdc.project.semc.Bundle"/>
                    <attr name="customizerPanelClass" newvalue="org.netbeans.modules.j2me.cdc.project.semc.SemcProjectCategoryCustomizer"/>
                    <attr name="triggerPropertyName" stringvalue="platform.type"/>
                    <attr name="triggerPropertyValue" stringvalue="semc"/>
                </file>
            </folder>
        </folder>
    </folder>

    <folder name="org-netbeans-api-project-libraries">
        <folder name="Libraries">
            <file name="semc-ant-utils.xml" url="Utils.xml"/>
        </folder>
    </folder>
</filesystem>
