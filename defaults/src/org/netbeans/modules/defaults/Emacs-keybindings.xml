<?xml version="1.0"?>
<!DOCTYPE bindings PUBLIC "-//NetBeans//DTD Editor KeyBindings settings 1.0//EN"
                          "http://www.netbeans.org/dtds/EditorKeyBindings-1_0.dtd">

<bindings>
    <bind actionName="abbrev-expand" key="S-SPACE"/>    
    <bind actionName="adjust-window-center" key="C-U$C-L"/>
    <bind actionName="annotations-cycling" key="DS-C"/>
    <bind actionName="caret-backward" key="C-B"/>
    <bind actionName="caret-backward" key="LEFT"/>
    <bind actionName="caret-backward" key="KP_LEFT"/>
    <bind actionName="caret-begin" key="AS-COMMA"/>
    <bind actionName="caret-begin" key="C-HOME"/>
    <bind actionName="caret-begin" key="ESCAPE$S-COMMA"/>
    <bind actionName="caret-begin-line" key="HOME"/>
    <bind actionName="caret-down" key="C-N"/>
    <bind actionName="caret-down" key="DOWN"/>
    <bind actionName="caret-down" key="KP_DOWN"/>
    <bind actionName="caret-end" key="AS-PERIOD"/>
    <bind actionName="caret-end" key="C-END"/>
    <bind actionName="caret-end" key="ESCAPE$S-PERIOD"/>
    <bind actionName="caret-end-line" key="C-E"/>
    <bind actionName="caret-end-line" key="END"/>
    <bind actionName="caret-forward" key="C-F"/>
    <bind actionName="caret-forward" key="RIGHT"/>
    <bind actionName="caret-line-first-column" key="C-A"/>
    <bind actionName="caret-next-word" key="A-F"/>
    <bind actionName="caret-next-word" key="C-RIGHT"/>
    <bind actionName="caret-previous-word" key="A-B"/>
    <bind actionName="caret-previous-word" key="C-LEFT"/>
    <bind actionName="caret-up" key="C-P"/>
    <bind actionName="caret-up" key="UP"/>
    <bind actionName="caret-up" key="KP_UP"/>
    <bind actionName="comment" key="C-C$C-C"/>
    <bind actionName="collapse-fold" key="D-MINUS"/>
    <bind actionName="collapse-all-folds" key="DS-SUBTRACT"/>
    <bind actionName="collapse-fold" key="D-SUBTRACT"/>
    <bind actionName="completion-show" key="C-SPACE"/>
    <bind actionName="completion-show" key="D-BACK_SLASH"/>
    <bind actionName="copy-to-clipboard" key="COPY"/>
    <bind actionName="copy-to-clipboard" key="A-W"/>    
    <bind actionName="cut-to-clipboard" key="CUT"/>
    <bind actionName="cut-to-clipboard" key="C-W"/>    
    <bind actionName="cut-to-line-begin" key="A-0$C-K"/>
    <bind actionName="cut-to-line-end" key="C-K"/>
    <bind actionName="delete-next" key="DELETE"/>
    <bind actionName="delete-next" key="C-D"/> 
    <bind actionName="delete-previous" key="BACK_SPACE"/>
    <bind actionName="expand-all-folds" key="DS-ADD"/>
    <bind actionName="expand-all-folds" key="DS-EQUALS"/>
    <bind actionName="expand-all-folds" key="DS-PLUS"/>
    <bind actionName="expand-fold" key="D-ADD"/>
    <bind actionName="expand-fold" key="D-EQUALS"/>
    <bind actionName="expand-fold" key="D-PLUS"/>
    <bind actionName="find" key="FIND"/>
    <bind actionName="find" key="C-S"/>    
    <bind actionName="find-next" key="A-S"/>
    <bind actionName="find-next" key="F3"/>
    <bind actionName="find-previous" key="A-R"/>
    <bind actionName="find-previous" key="C-R"/>
    <bind actionName="find-previous" key="S-F3"/>
    <bind actionName="goto-declaration" key="A-PERIOD"/>
    <bind actionName="goto-declaration" key="CA-G"/>
    <bind actionName="insert-break" key="ENTER"/>
    <bind actionName="insert-tab" key="TAB"/>
    <bind actionName="page-down" key="C-V"/>
    <bind actionName="page-down" key="PAGE_DOWN"/>
    <bind actionName="page-up" key="A-V"/>
    <bind actionName="page-up" key="PAGE_UP"/>
    <bind actionName="paste-from-clipboard" key="PASTE"/>
    <bind actionName="paste-from-clipboard" key="C-Y"/>    
    <bind actionName="remove-tab" key="S-TAB"/>
    <bind actionName="remove-word-next" key="A-D"/>
    <bind actionName="remove-word-previous" key="A-BACK_SPACE"/>
    <bind actionName="remove-word-previous" key="C-BACK_SPACE"/>
    <bind actionName="replace" key="AS-5"/>
    <bind actionName="scroll-down" key="C-UP"/>
    <bind actionName="scroll-up" key="C-DOWN"/>
    <bind actionName="select-all" key="C-X$H"/>
    <bind actionName="select-word" key="CA-W"/>
    <bind actionName="selection-backward" key="S-LEFT"/>
    <bind actionName="selection-begin" key="DS-HOME"/>
    <bind actionName="selection-begin-line" key="S-HOME"/>
    <bind actionName="selection-down" key="S-DOWN"/>
    <bind actionName="selection-end" key="DS-END"/>
    <bind actionName="selection-end-line" key="S-END"/>
    <bind actionName="selection-forward" key="S-RIGHT"/>
    <bind actionName="selection-next-word" key="DS-RIGHT"/>
    <bind actionName="selection-page-down" key="S-PAGE_DOWN"/>
    <bind actionName="selection-page-up" key="S-PAGE_UP"/>
    <bind actionName="selection-previous-word" key="DS-LEFT"/>
    <bind actionName="selection-up" key="S-UP"/>
    <bind actionName="shift-line-left" key="A-0$C-X$TAB"/>
    <bind actionName="shift-line-right" key="C-X$TAB"/>
    <bind actionName="split-line" key="D-ENTER"/>
    <bind actionName="start-macro-recording" key="C-X$S-9"/>
    <bind actionName="start-new-line" key="S-ENTER"/>
    <bind actionName="stop-macro-recording" key="C-X$S-0"/>
    <bind actionName="to-lower-case" key="C-X$C-L"/>
    <bind actionName="to-upper-case" key="C-X$C-U"/>
    <bind actionName="toggle-case-identifier-begin" key="A-C"/>
    <bind actionName="toggle-typing-mode" key="INSERT"/>
    <bind actionName="undo" key="UNDO"/>
    <bind actionName="undo" key="C-X$U"/>
    <bind actionName="undo" key="C-SLASH"/>    
    <bind actionName="uncomment" key="A-0$C-C$C-C"/>
    <bind actionName="word-match-next" key="A-SLASH"/>
</bindings>
