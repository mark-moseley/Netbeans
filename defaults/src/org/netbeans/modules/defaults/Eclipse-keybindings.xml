<?xml version="1.0"?>

<bindings>
    <bind actionName="caret-next-word" key="C-RIGHT"/>
    <bind actionName="caret-previous-word" key="C-LEFT"/>
    <bind actionName="collapse-fold" key="C-SUBTRACT"/>
    <bind actionName="completion-show" key="C-SPACE"/>
    <bind actionName="expand-all-folds" key="C-MULTIPLY"/>
    <bind actionName="expand-fold" key="C-ADD"/>
    <bind actionName="fast-import" key="CS-M"/>
    <bind actionName="find-next" key="C-K"/>
    <bind actionName="find-previous" key="CS-K"/>
    <bind actionName="fix-imports" key="CS-O"/>
    <bind actionName="format" key="CS-F"/>
    <bind actionName="goto" key="C-L"/>
    <bind actionName="goto-declaration" key="F3"/>
    <bind actionName="goto-source" key="C-F3"/>
    <bind actionName="jump-list-next" key="A-RIGHT"/>
    <bind actionName="jump-list-prev" key="A-LEFT"/>
    <bind actionName="match-brace" key="CAS-P"/>
    <bind actionName="remove-line" key="C-D"/>
    <bind actionName="remove-word-next" key="C-DELETE"/>
    <bind actionName="remove-word-previous" key="C-BACK_SPACE"/>
    <bind actionName="replace" key="CS-F"/>
    <bind actionName="scroll-down" key="C-UP"/>
    <bind actionName="scroll-up" key="C-DOWN"/>
    <bind actionName="select-all" key="C-A"/>
    <bind actionName="select-element-next" key="AS-UP"/>
    <bind actionName="select-element-previous" key="AS-DOWN"/>
    <bind actionName="selection-backward" key="CS-LEFT"/>
    <bind actionName="selection-next-word" key="CS-RIGHT"/>
    <bind actionName="to-lower-case" key="CS-Y"/>
    <bind actionName="to-upper-case" key="CS-X"/>
    <bind actionName="toggle-typing-mode" key="INSERT"/>
    <bind actionName="word-match-next" key="A-SLASH"/>
</bindings>
