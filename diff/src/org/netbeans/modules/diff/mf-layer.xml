<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<filesystem>
  <folder name="Services">
    <folder name="Diffs">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.diff.Bundle" />
      <file name="org-netbeans-modules-diff-builtin-DefaultDiff.settings" url="DefaultDiff.settings"/>
    </folder>
    <folder name="DiffProviders">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.diff.Bundle" />
      <file name="org-netbeans-modules-diff-cmdline-CmdlineDiffProvider.settings" url="CmdlineDiffProvider.settings"/>
      <file name="org-netbeans-modules-diff-builtin-provider-BuiltInDiffProvider.settings" url="BuiltInDiffProvider.settings"/>
    </folder>
    <folder name="DiffVisualizers">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.diff.Bundle" />
      <file name="org-netbeans-modules-diff-builtin-visualizer-GraphicalDiffVisualizer.settings" url="GraphicalDiffVisualizer.settings"/>
      <file name="org-netbeans-modules-diff-builtin-visualizer-TextDiffVisualizer.settings" url="TextDiffVisualizer.settings"/>
    </folder>
    <folder name="MergeVisualizers">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.merge.Bundle" />
      <file name="org-netbeans-modules-diff-builtin-visualizer-GraphicalMergeVisualizer.settings" url="GraphicalMergeVisualizer.settings"/>
    </folder>

    <folder name="Hidden"> 
      <file name="org-netbeans-modules-diff-DiffSettings.settings" url="DiffSettings.settings">
        <attr name="SystemFileSystem.layer" stringvalue="project"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.diff.Bundle" />
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/diff/diffSettingsIcon.gif" />
      </file>
    </folder>

    <!--
    <file name="org-netbeans-modules-diff-DiffSettingsNode.instance">
