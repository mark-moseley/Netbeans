<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 EJB 2.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_2_1-1.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>RemoteSessionBean</ejb-name>
      <jndi-name>ejb/RemoteSessionBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>MLRSBean</ejb-name>
      <jndi-name>ejb/MLRSBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>MRSSBean</ejb-name>
      <jndi-name>ejb/MRSSBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>MLRSSBean</ejb-name>
      <jndi-name>ejb/MLRSSBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>RemoteEntityBean</ejb-name>
      <jndi-name>ejb/RemoteEntityBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>MLREBean</ejb-name>
      <jndi-name>ejb/MLREBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>QueueMdbBean</ejb-name>
      <jndi-name>jms/QueueMdb</jndi-name>
      <mdb-connection-factory>
        <jndi-name>jms/QueueMdbFactory</jndi-name>
      </mdb-connection-factory>
    </ejb>
    <ejb>
      <ejb-name>TopicMdbBean</ejb-name>
      <jndi-name>jms/TopicMdb</jndi-name>
      <mdb-connection-factory>
        <jndi-name>jms/TopicMdbFactory</jndi-name>
      </mdb-connection-factory>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
