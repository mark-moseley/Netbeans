<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 Servlet 2.4//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_4-1.dtd">
<sun-web-app error-url="">
  <context-root>/SampleApp-WebModule</context-root>
  <servlet>
    <servlet-name>WSServlet_SampleWebService</servlet-name>
    <webservice-endpoint>
      <port-component-name>SampleWebService</port-component-name>
      <endpoint-address-uri>SampleWebService</endpoint-address-uri>
    </webservice-endpoint>
  </servlet>
  <ejb-ref>
    <ejb-ref-name>ejb/SampleSessionBean</ejb-ref-name>
    <jndi-name>ejb/SampleSessionBean</jndi-name>
  </ejb-ref>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="classdebuginfo" value="true">
      <description>Enable debug info compilation in the generated servlet class</description>
    </property>
    <property name="mappedfile" value="true">
      <description>Maintain a one-to-one correspondence between static content and the generated servlet class' java code</description>
    </property>
  </jsp-config>
</sun-web-app>
