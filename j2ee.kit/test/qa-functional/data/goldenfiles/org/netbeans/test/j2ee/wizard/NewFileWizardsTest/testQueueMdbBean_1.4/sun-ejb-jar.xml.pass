<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 EJB 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_0-0.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>RemoteSessionBean</ejb-name>
      <jndi-name>ejb/RemoteSessionBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>LRSBean</ejb-name>
      <jndi-name>ejb/LRSBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>RSSBean</ejb-name>
      <jndi-name>ejb/RSSBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>LRSSBean</ejb-name>
      <jndi-name>ejb/LRSSBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>RemoteEntityBean</ejb-name>
      <jndi-name>ejb/RemoteEntityBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>LREBean</ejb-name>
      <jndi-name>ejb/LREBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>QueueMdbBean</ejb-name>
      <jndi-name>jms/QueueMdb</jndi-name>
      <mdb-connection-factory>
        <jndi-name>jms/QueueMdbFactory</jndi-name>
      </mdb-connection-factory>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
