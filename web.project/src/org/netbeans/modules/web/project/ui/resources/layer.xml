<?xml-stylesheet type="text/xml" href="http://openide.netbeans.org/fs/filesystem.xsl"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Project">
            <attr name="Web/Standard" boolvalue="true"/>
            <attr name="Web/Mobile" boolvalue="true"/>

            <folder name="Web">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.project.ui.wizards.Bundle"/>
                <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/project/ui/resources/web.html"/>

                <attr name="emptyWeb.xml/importWeb.xml" boolvalue="true"/>

                <!-- Add template for the J2SE project -->
                <file name="emptyWeb.xml">
                    <attr name="template" boolvalue="true"/>
                    <attr name="templateWizardIterator" newvalue="org.netbeans.modules.web.project.ui.wizards.NewWebProjectWizardIterator"/>
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.project.ui.wizards.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/web/project/ui/resources/webProjectIcon.gif"/>
                    <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/project/ui/resources/emptyProject.html"/>
                </file>
                <file name="importWeb.xml">
                   <attr name="template" boolvalue="true"/>
                   <attr name="templateWizardIterator" newvalue="org.netbeans.modules.web.project.ui.wizards.ImportWebProjectWizardIterator"/>
                   <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.project.ui.wizards.Bundle"/>
                   <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/web/project/ui/resources/webProjectIcon.gif"/>
                   <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/project/ui/resources/importProject.html"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="org-netbeans-api-project-libraries">
        <folder name="Libraries">
            <file name="copyfiles.xml" url="copyfiles.xml"/>
        </folder>
    </folder>
    <folder name="org-netbeans-modules-web-project">
        <file name="web-2.3.xml" url="web-2.3.xml"/>
        <file name="web-2.4.xml" url="web-2.4.xml"/>
    </folder>
</filesystem>
