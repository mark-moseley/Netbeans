<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

    <folder name="Actions">
        <folder name="Window">
            <file name="org-netbeans-modules-palette-ShowPaletteAction.instance" />
        </folder>
    </folder>

    <folder name="Menu">
        <folder name="Window">
            <file name="ShowPaletteAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-palette-ShowPaletteAction.instance"/>
            </file>
            <attr name="oldRootsEnd.txt/ShowPaletteAction.shadow" boolvalue="true" />
            <attr name="ShowPaletteAction.shadow/Form" boolvalue="true" />
        </folder>
    </folder>

    
    <!-- default workspaces, frames and their layout defined by core -->
    <folder name="Windows2">
        <folder name="Modes">
            <file name="commonpalette.wsmode" url="commonpalette.wsmode" />
            <folder name="commonpalette">
                <file name="CommonPalette.wstcref" url="CommonPalette.wstcref" />
            </folder>
        </folder>

        <folder name="Components">
            <file name="CommonPalette.settings" url="CommonPalette.settings" />
        </folder>

        <folder name="Groups">
            <file name="commonpalette.wsgrp" url="commonpalette.wsgrp" />
            <folder name="commonpalette">
                <file name="CommonPalette.wstcgrp" url="CommonPalette.wstcgrp" />
            </folder>
        </folder>
    </folder>
    
    <folder name="PaletteSettings">
        <!-- Palette stores its non-public settings here -->
    </folder>

    <folder name="xml">
        <folder name="entities">
            <folder name="NetBeans" >
                <file name="Editor_Palette_Item_1_0" url="editor-palette-item-1_0.dtd">
                    <attr name="hint.originalPublicID" stringvalue="-//NetBeans//Editor Palette Item 1.0//EN"/>
                </file>
            </folder>
        </folder>
        
        <folder name="lookups">
            <folder name="NetBeans">
                <file name="Editor_Palette_Item_1_0.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.palette.PaletteEnvironmentProvider"/>
                    <attr name="instanceOf" stringvalue="org.openide.loaders.Environment.Provider"/>
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.palette.PaletteEnvironmentProvider.createProvider"/>
                </file>
            </folder>
        </folder>

    </folder>

</filesystem>
