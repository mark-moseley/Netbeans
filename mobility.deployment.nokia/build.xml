<project name="mobility/deployment/nokia" default="netbeans" basedir=".">
    <import file="../../../nbbuild/templates/projectized.xml"/>
    <description>Builds, tests, and runs the project org.netbeans.modules.mobility.deployment.nokia.</description>
    <target name="release" depends="init">
        <mkdir dir="${cluster}/modules/scr"/>
        <copy todir="${cluster}/modules/scr">
            <fileset dir="scripts"/>
        </copy>
        <mkdir dir="${cluster}/modules/lib"/>
        <copy todir="${cluster}/modules/lib">
            <fileset dir="release/modules/lib"/>
        </copy>
    </target>
</project>
