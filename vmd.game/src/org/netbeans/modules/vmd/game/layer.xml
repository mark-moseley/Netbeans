<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    
     <folder name="Navigator">
        <folder name="Panels">
            <folder name="midpgame">
                <file name="org-netbeans-modules-vmd-game-GameBuilderNavigator.instance"/>
            </folder>
        </folder>
    </folder>

    <folder name="Templates">        
        <folder name="MIDP">
            <file name="GameDesign.java" url="integration/res/emptygame_java.template">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vmd.game.integration.res.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/vmd/game/integration/res/gamer_16.png"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/vmd/game/integration/res/emptygame.html"/>
                <attr name="template" boolvalue="true"/>
        		<attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
                <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.mobility.project.ui.wizard.FileWizardIterator.singleton"/>
                <attr name="MIDP" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="MIDP"/>
                <attr name="position" intvalue="550"/>
            </file>
            <file name="GameDesign.vmd" url="integration/res/emptygame_vmd.template">
                <attr name="template" boolvalue="true"/>
            </file>
        </folder>
    </folder>

    <folder name="vmd-midp-game">
        
        <folder name="components">
            
            <file name="org-netbeans-modules-vmd-game-model-AnimatedTileCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-GameRootCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-ImageResourceCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-LayerCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-SceneCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-SceneItemCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-SequenceCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-SpriteCD.instance"/>
            <file name="org-netbeans-modules-vmd-game-model-TiledLayerCD.instance"/>
            
        </folder>
        
    </folder>
    
</filesystem>
