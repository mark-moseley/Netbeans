#!/bin/sh
source nbbuild/hudson/ruby && \
rm -f nbbuild/nbproject/private/scan-cache-full.ser nbbuild/nbproject/private/scan-cache-standard.ser && \
ant -f nbbuild/build.xml all-xtest -Dverify.checkout=false && \
ant -f lexer/test/build.xml buildtests  -Dverify.checkout=false && \
ant -f scripting/ruby/build.xml runtests && \
ant -f scripting/ruby/build.xml tests-junit-format

