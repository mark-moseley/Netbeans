<project name="nbbuild" default="depucelee" basedir=".">

  <taskdef name="cleanall" classname="org.netbeans.nbbuild.CleanAll"/>
  <taskdef name="nbmerge" classname="org.netbeans.nbbuild.NbMerge"/>
  <taskdef name="configmods" classname="org.netbeans.nbbuild.ConfigureModules"/>
  <taskdef name="makeupdatedesc" classname="org.netbeans.nbbuild.MakeUpdateDesc"/>

  <target name="init">
    <!-- Override this with the Ant command, e.g. -Dmoduleconfig=superslim -->
    <property name="moduleconfig" value="stable"/>
    <!-- You can also explicitly set a particular list of modules if you prefer: -->
    <!-- -Dmodules=foo,bar,... (do not include core or openide or openidex) -->
    <configmods property="modules" selectedconfig="${moduleconfig}">
      <config name="superslim" modules="clazz,editor,html,icebrowser,image,java,projects,text,utilities"/>
      <config name="slim" modules="autoupdate,beans,clazz,debuggercore,debuggerjpda,debuggertools,editor,form,html,httpserver,icebrowser,image,java,objectbrowser,projects,text,utilities"/>
      <config name="stable" modules="ant,applet,autoupdate,beans,clazz,debuggercore,debuggerjpda,debuggertools,editor,form,html,httpserver,i18n,icebrowser,image,jarpackager,java,javadoc,objectbrowser,projects,properties,text,usersguide,utilities,vcscore,vcscvs,web"/>
      <config name="stable-with-apisupport" modules="ant,apisupport,applet,autoupdate,beans,clazz,debuggercore,debuggerjpda,debuggertools,editor,form,html,httpserver,i18n,icebrowser,image,jarpackager,java,javadoc,objectbrowser,projects,properties,text,usersguide,utilities,vcscore,vcscvs,web"/>
      <config name="ide-hacker" modules="apisupport,autoupdate,beans,clazz,debuggercore,debuggerjpda,debuggertools,editor,form,html,i18n,icebrowser,image,jarpackager,java,javadoc,objectbrowser,projects,properties,serialversion,sysprops,text,utilities,vcscvs"/>
      <config name="jesses-pick" modules="ant,apisupport,beans,clazz,editor,html,icebrowser,image,jarpackager,java,javadoc,projects,properties,serialversion,sysprops,text,utilities"/>
      <config name="bloated" modules="ant,apisupport,applet,autoupdate,beans,clazz,crosscompile,debuggercore,debuggerjpda,debuggertools,editor,form,html,httpserver,i18n,icebrowser,image,innertesters,jarpackager,java,javadoc,javaembeddedserver,makefile,multicompile,objectbrowser,projects,properties,remotefs,serialversion,sysprops,text,usersguide,utilities,vcscore,vcscvs,web"/>
      <config name="sandwich" modules="apisupport,applet,autoupdate,beans,clazz,debuggercore,debuggerjpda,debuggertools,editor,form,html,httpserver,i18n,icebrowser,image,innertesters,jarpackager,java,javadoc,objectbrowser,properties,text,utilities,vcscvs,web"/>
      <!-- You are on your own for getting the binaries for some of these: -->
      <config name="bloated-plus-extrabins" modules="ant,apisupport,applet,autoupdate,beans,clazz,crosscompile,debuggercore,debuggerjpda,debuggertools,editor,form,html,httpserver,i18n,icebrowser,image,innertesters,jarpackager,java,javadoc,javaembeddedserver,jini,makefile,multicompile,objectbrowser,projects,properties,remotefs,scripting,serialversion,sysprops,text,usersguide,utilities,vcscore,vcscvs,web"/>
    </configmods>
  </target>

  <!-- Ditto (note: need not necessarily be a number at all): -->
  <property name="buildnumber" value="dev-000"/>

  <property name="binroot" value=".."/>

  <!-- Best to leave this alone, though, unless you know what you are doing: -->
  <property name="firststart.args" value=" -J-Dnetbeans.close=true -J-Dnetbeans.debug.exceptions=true"/>
  <!-- But this it is OK to change: -->
  <property name="tryme.args" value=" -J-Dnetbeans.debug.exceptions=true"/>

  <target name="look-for-binaries">
    <available property="have-binaries-already" file="../core/release/lib/ext/jh.jar"/>
  </target>
  <target name="check-for-binaries" depends="look-for-binaries" unless="have-binaries-already">
    <echo message="ERROR: you seem to be missing the required download of"/>
    <echo message="       binaries which are required to build the IDE."/>
    <echo message="       Please see: http://www.netbeans.org/downloads.html"/>
  </target>

  <!-- How to build the IDE core: -->
  <target name="all-openide" depends="check-for-binaries">
    <echo message="Building Open APIs..."/>
    <ant dir="../openide" target="netbeans"/>
  </target>
  <target name="all-openidex" depends="all-openide">
    <echo message="Building module openidex..."/>
