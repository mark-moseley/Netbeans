  <target name="build-messaging" depends="init">
    <echo message="Starting a build for NetBeans (build ${buildnumber}, config '${moduleconfig}') ..."/>
    <echo message="Selected modules: ${modules}"/>
    <echo message="OS: ${os.name} ${os.version}  Locale: ${user.language}_${user.region}"/>
    <echo message="Java: ${java.vendor} ${java.version} in ${java.home}"/>
    <echo message="Classpath: ${java.class.path}"/>
  </target>

  <!-- Use of nbmerge target has the effect of automatically depending on all-X for every module. -->
  <!-- It also runs deltree(dir=wherever) and copydir(src=../X/netbeans,dest=wherever) to create the IDE install. -->

  <!-- XXX zip task not well suited to making final build, as it does not set +x bits -->
  <!-- should also create empty lib/patches/ dir (but again zip task will ignore it) -->
  <!-- or just make lib/patches/.exists which is otherwise harmless -->

  <target name="virgin" depends="init,build-messaging"
          description="Create a virgin build that has never been started.">
    <!-- Automatically include openide and core, these are mandatory -->
    <mkdir dir="virgin"/>
    <nbmerge dest="virgin" topdir=".." modules="openide,openidex,core,${modules}" targetprefix="all-"/>
    <zip zipfile="NetBeans-${buildnumber}-virgin.zip" basedir="virgin"/>
    <echo message="Virgin build created; see NetBeans-${buildnumber}-virgin.zip (in nbbuild/)."/>
  </target>

  <target name="firststart" depends="init,build-messaging"
          description="Build the IDE and start it, but do not create a build archive.">
    <mkdir dir="firststart"/>
    <nbmerge dest="firststart" topdir=".." modules="openide,openidex,core,${modules}" targetprefix="all-"/>
    <chmod file="firststart/bin/runide.sh" perm="ugo+x"/>
    <chmod file="firststart/bin/runide_multiuser.sh" perm="ugo+x"/>
    <chmod file="firststart/bin/fastjavac/fastjavac.linux" perm="ugo+x"/>
    <chmod file="firststart/bin/fastjavac/fastjavac.sun" perm="ugo+x"/>
    <chmod file="firststart/bin/fastjavac/fastjavac.sun.intel" perm="ugo+x"/>
    <echo message="Starting the IDE..."/>
    <echo message="(This both acts as a test, and primes some files"/>
    <echo message="to make the first user start faster and better.)"/>
    <!-- XXX should perhaps preset JAVA_PATH to ${java.home} or something? -->
    <!-- XXX better would be to scan first start log for stacktraces and just show those; -->
    <!-- possibly after filtering for duplicates or something like this. -->
    <!-- WARNING: W2K fails with Ant 1.1 release, use a newer Ant without this bug: -->
    <!-- missing initial W intentional, to make it case-insensitive: -->
    <exec os="indows" dir="firststart/bin" command="runide.exe ${firststart.args}" failonerror="yes"/>
    <exec os="Linux" dir="firststart/bin" command="sh runide.sh ${firststart.args}" failonerror="yes"/>
    <exec os="SunOS" dir="firststart/bin" command="sh runide.sh ${firststart.args}" failonerror="yes"/>
    <exec os="Solaris" dir="firststart/bin" command="sh runide.sh ${firststart.args}" failonerror="yes"/>
    <!-- XXX ideal would be a timeout after which the process would get SIGQUIT then SIGINT then SIGKILL -->
    <!-- This is possible in newer Ants but not in 1.1 -->
    <echo message="Successfully started, cleaning up detritus..."/>
    <move file="firststart/system/ide.log" tofile="NetBeans-${buildnumber}.log"/>
    <delete file="firststart/system/project.basic"/>
    <delete file="firststart/system/project.last"/>
    <delete file="firststart/beans/libs.properties"/>
    <mkdir dir="firststart/lib/patches"/>
    <echo file="firststart/build_info">NetBeans dev build
Build number: ${buildnumber}
</echo>
  </target>

  <target name="depucelee" depends="firststart"
          description="Create a complete build, including first start and final ZIP file.">
    <zip zipfile="NetBeans-${buildnumber}.zip" basedir="firststart"/>
    <echo message="Build created; see NetBeans-${buildnumber}.zip (in nbbuild/)."/>
    <echo message="If you like, you may run the IDE straight from"/>
    <echo message="the nbbuild/firststart/bin/ directory."/>
    <echo message="(For example, type: ant tryme)"/>
  </target>

  <target name="check-for-firststart">
    <available property="have-firststart" file="firststart/lib/openide.jar"/>
  </target>
  <target name="maybe-firststart" depends="check-for-firststart" unless="have-firststart">
    <ant dir="." target="firststart"/>
  </target>
  <target name="tryme" depends="maybe-firststart"
          description="Try running the IDE interactively (build it first if needed).">
    <exec os="indows" dir="firststart/bin" executable="runide.exe" failonerror="yes">
      <arg line="${tryme.args}"/>
    </exec>
    <exec os="Linux" dir="firststart/bin" executable="sh" failonerror="yes">
      <arg line="runide.sh ${tryme.args}"/>
    </exec>
    <exec os="SunOS" dir="firststart/bin" executable="sh" failonerror="yes">
      <arg line="runide.sh ${tryme.args}"/>
    </exec>
    <exec os="Solaris" dir="firststart/bin" executable="sh" failonerror="yes">
      <arg line="runide.sh ${tryme.args}"/>
    </exec>
  </target>

