<project name="core/windows/test-unit" basedir="." default="all">

   <!-- imports default-compiler which have all dependent jars on classpath.
        Other required jars add to nbproject/project.properties -->
   <import file="../../../nbbuild/templates/xtest-unit.xml"/>

    <!-- ================ -->
    <!-- Executors -->
    <!-- ================ -->

    <target name="runidetest">
        <property name="xtest.ide.use.classloader" value="org.netbeans.core.windows"/>
        <property name="xtest.ide.commandline.suffix" value="--nogui"/>
        <executeTests  pluginName="ide">
            <classpath/>
        </executeTests>
    </target>
</project>
