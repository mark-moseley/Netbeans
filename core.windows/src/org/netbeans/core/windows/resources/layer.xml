<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<!-- XML Layer for core-windows -->

<filesystem>
   
    <!--
    Use this folder and subfolders in your XML layer to define
    desktop layout, modes and groups.
    -->
    <folder name="Windows2">
        <file name="WindowManager.wswmgr" url="nbres:/org/netbeans/core/windows/resources/windowmanager/windowmanager.wswmgr"/>
    </folder>

    <folder name="Actions">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
        
        <folder name="Window">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <file name="org-openide-actions-CloneViewAction.instance"/>
            <file name="org-openide-actions-PreviousTabAction.instance"/>
            <file name="org-openide-actions-NextTabAction.instance"/>
            <file name="org-openide-actions-UndockAction.instance"/>
        </folder>    
    </folder>
    
    <folder name="Menu">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
        
        <attr name="Edit/View" boolvalue="true" />
        <folder name="View">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <attr name="org-netbeans-core-actions-HTMLViewAction.instance/Separator2.instance" boolvalue="true" />
        </folder>

        <attr name="Tools/Window" boolvalue="true" />
        <folder name="Window">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <attr name="org-netbeans-core-actions-HTMLViewAction.instance/Separator3.instance" boolvalue="true" />
            <file name="Separator3.instance">
	        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator3.instance/org-netbeans-core-windows-actions-RecentViewListAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-RecentViewListAction.instance" />
            <attr name="org-netbeans-core-windows-actions-RecentViewListAction.instance/org-netbeans-core-windows-actions-SwitchToRecentDocumentAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-SwitchToRecentDocumentAction.instance" />
            <attr name="org-netbeans-core-windows-actions-SwitchToRecentDocumentAction.instance/Separator4.instance" boolvalue="true" />
            <file name="Separator4.instance">
	        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator4.instance/CloseWindowAction.instance" boolvalue="true" />
            <file name="CloseWindowAction.instance">
                <attr name="instanceClass" stringvalue="javax.swing.Action"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.core.windows.actions.ActionFactory.createGlobalCallback"/>
                <attr name="actionKey" stringvalue="closeWindow"/>
                <attr name="localizingBundle" stringvalue="org.netbeans.core.windows.actions.Bundle"/>
                <!--<attr name="iconResource" stringvalue="org/openide/resources/actions/clone.gif"/>-->
            </file>
            <attr name="CloseWindowAction.instance/org-netbeans-core-windows-actions-MaximizeWindowAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-MaximizeWindowAction.instance" />
            <attr name="org-netbeans-core-windows-actions-MaximizeWindowAction.instance/Separator5.instance" boolvalue="true" />
            <file name="Separator5.instance">
	        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator5.instance/CloneWindowAction.instance" boolvalue="true" />
            <file name="CloneWindowAction.instance">
                <attr name="instanceClass" stringvalue="javax.swing.Action"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.core.windows.actions.ActionFactory.createGlobalCallback"/>
                <attr name="actionKey" stringvalue="cloneWindow"/>
                <attr name="localizingBundle" stringvalue="org.netbeans.core.windows.actions.Bundle"/>
            </file>
            <attr name="CloneWindowAction.instance/org-netbeans-core-windows-actions-CloseAllDocumentsAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-CloseAllDocumentsAction.instance" />
            <!--<attr name="CloneWindowAction.instance/Separator5.instance" boolvalue="true" />
            <file name="Separator5.instance">
	        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator5.instance/org-netbeans-core-windows-actions-AttachWindowAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-AttachWindowAction.instance" />
            <attr name="org-netbeans-core-windows-actions-AttachWindowAction.instance/Separator6.instance" boolvalue="true" />
            <file name="Separator6.instance">
                <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
            </file>-->
            <attr name="org-netbeans-core-windows-actions-CloseAllDocumentsAction.instance/org-netbeans-core-windows-actions-DocumentsAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-DocumentsAction.instance" />
        </folder>
    </folder>

    <folder name="Toolbars">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:org/netbeans/core/resources/toolbarsOption.gif"/>
    </folder>
    
    <folder name="Shortcuts">
        <file name="A-LEFT.instance">
            <attr name="instanceClass" stringvalue="org.netbeans.core.windows.actions.PreviousTabAction" />
        </file>
        <file name="A-RIGHT.instance">
	    <attr name="instanceClass" stringvalue="org.netbeans.core.windows.actions.NextTabAction"/>
	</file>
        
        <file name="C-F4.shadow">
	    <attr name="originalFile" stringvalue="Menu/Window/CloseWindowAction.instance"/>
	</file>
        <file name="S-ESCAPE.shadow">
	    <attr name="originalFile" stringvalue="Menu/Window/org-netbeans-core-windows-actions-MaximizeWindowAction.instance"/>
	</file>
        <file name="C-BACK_QUOTE.shadow">
            <attr name="originalFile" stringvalue="Menu/Window/org-netbeans-core-windows-actions-RecentViewListAction.instance"/>
        </file>
        <file name="C-0.shadow">
            <attr name="originalFile" stringvalue="Menu/Window/org-netbeans-core-windows-actions-SwitchToRecentDocumentAction.instance"/>
        </file>
        <file name="CS-F4.shadow">
            <attr name="originalFile" stringvalue="Menu/Window/org-netbeans-core-windows-actions-CloseAllDocumentsAction.instance"/>
        </file>
        <file name="S-F4.shadow">
	    <attr name="originalFile" stringvalue="Menu/Window/org-netbeans-core-windows-actions-DocumentsAction.instance"/>
	</file>
    </folder>

    <folder name="xml">
        <folder name="lookups">
            <folder name="NetBeans_IDE">
                <!-- public ID defined by XMLDataObject -->
                <file name="DTD_toolbar.instance">
                    <attr name="instanceClass" stringvalue="org.openide.loaders.XMLDataObject$Processor"/>
                    <attr name="instanceCreate" newvalue="org.netbeans.core.windows.view.ui.toolbars.ToolbarProcessor" />
                </file>
            </folder>
        </folder>

        <folder name="entities">
            <folder name="NetBeans_IDE">
                <!-- public ID defined by XMLDataObject -->
                <file name="DTD_toolbar" url="nbres:/org/netbeans/core/windows/resources/toolbar.dtd" >
                    <attr name="hint.originalPublicID" stringvalue="-//NetBeans IDE//DTD toolbar//EN"/>
                </file>
            </folder>

            <folder name="NetBeans">
