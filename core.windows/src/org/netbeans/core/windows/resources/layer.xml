<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<!-- XML Layer for core-windows -->

<filesystem>
    
    <folder name="Actions">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
        
        <folder name="Window">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <file name="org-openide-actions-CloneViewAction.instance"/>
            <file name="org-netbeans-core-actions-DockingAction.instance"/>
            <file name="org-openide-actions-PreviousTabAction.instance"/>
            <file name="org-openide-actions-NextTabAction.instance"/>
            <file name="org-netbeans-core-actions-OpenedWindowsAction.instance"/>
            <file name="org-openide-actions-UndockAction.instance"/>
        </folder>    
    </folder>
    
    <folder name="Menu">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
        
        <attr name="Edit/View" boolvalue="true" />
        <folder name="View">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <attr name="org-netbeans-core-actions-HTMLViewAction.instance/Separator2[javax-swing-JSeparator].instance" boolvalue="true" />
            <file name="Separator2[javax-swing-JSeparator].instance"/>
            <attr name="Separator2[javax-swing-JSeparator].instance/org-openide-actions-WorkspaceSwitchAction.instance" boolvalue="true" />
            <file name="org-openide-actions-WorkspaceSwitchAction.instance"/>
        </folder>

        <attr name="Tools/Window" boolvalue="true" />
        <folder name="Window">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <file name="org-netbeans-core-actions-AttachFrameAction.instance"/>
            <attr name="org-netbeans-core-actions-AttachFrameAction.instance/org-netbeans-core-actions-DetachFrameAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-actions-DetachFrameAction.instance"/>
            <attr name="org-netbeans-core-actions-DetachFrameAction.instance/org-netbeans-core-actions-FrameResidenceAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-actions-FrameResidenceAction.instance"/>
            <attr name="org-netbeans-core-actions-FrameResidenceAction.instance/org-netbeans-core-actions-MakeDominantAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-actions-MakeDominantAction.instance"/>
            <attr name="org-netbeans-core-actions-MakeDominantAction.instance/Separator1[javax-swing-JSeparator].instance" boolvalue="true" />
            <file name="Separator1[javax-swing-JSeparator].instance"/>
            <attr name="Separator1[javax-swing-JSeparator].instance/org-openide-actions-UndoAction.instance" boolvalue="true" />
            <file name="org-openide-actions-UndockAction.instance"/>
            <attr name="org-openide-actions-UndockAction.instance/org-netbeans-core-actions-DockingAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-actions-DockingAction.instance"/>
            <attr name="org-netbeans-core-actions-DockingAction.instance/Separator2[javax-swing-JSeparator].instance" boolvalue="true" />
            <file name="Separator2[javax-swing-JSeparator].instance"/>
            <attr name="Separator2[javax-swing-JSeparator].instance/org-openide-actions-CloneViewAction.instance" boolvalue="true" />
            <file name="org-openide-actions-CloneViewAction.instance"/>
            <attr name="org-openide-actions-CloneViewAction.instance/Separator3[javax-swing-JSeparator].instance" boolvalue="true" />
            <file name="Separator3[javax-swing-JSeparator].instance"/>
            <attr name="Separator3[javax-swing-JSeparator].instance/org-openide-actions-NextTabAction.instance" boolvalue="true" />
            <file name="org-openide-actions-NextTabAction.instance"/>
            <attr name="org-openide-actions-NextTabAction.instance/org-openide-actions-PreviousTabAction.instance" boolvalue="true" />
            <file name="org-openide-actions-PreviousTabAction.instance"/>
            <attr name="org-openide-actions-PreviousTabAction.instance/Separator4[javax-swing-JSeparator].instance" boolvalue="true" />
            <file name="Separator4[javax-swing-JSeparator].instance"/>
            <attr name="Separator4[javax-swing-JSeparator].instance/org-netbeans-core-actions-OpenedWindowsAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-actions-OpenedWindowsAction.instance"/>
        </folder>
        
    </folder>
    
</filesystem>
