<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<!-- XML Layer for core-windows -->

<filesystem>
    
    <folder name="Windows">
        <attr name="SystemFileSystem.layer" stringvalue="project"/>
        <file name="WindowManager.wswmgr" url="nbres:/org/netbeans/core/windows/resources/windowmanager/windowmanager.wswmgr"/>
        <folder name="WindowManager"/>
        
        <!-- 
        Use this folder and subfolders in your XML layer to define
        workspaces, frames, and their layout.
        -->
        
    </folder>

    <folder name="Actions">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
        
        <folder name="Window">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <file name="org-openide-actions-CloneViewAction.instance"/>
            <file name="org-netbeans-core-windows-actions-DockingAction.instance"/>
            <file name="org-openide-actions-PreviousTabAction.instance"/>
            <file name="org-openide-actions-NextTabAction.instance"/>
            <file name="org-netbeans-core-windows-actions-OpenedWindowsAction.instance"/>
            <file name="org-openide-actions-UndockAction.instance"/>
        </folder>    
    </folder>
    
    <folder name="Menu">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
        
        <attr name="Edit/View" boolvalue="true" />
        <folder name="View">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <attr name="org-netbeans-core-actions-HTMLViewAction.instance/Separator2.instance" boolvalue="true" />
            <attr name="org-netbeans-modules-javadoc-search-IndexOverviewAction.instance/Separator2.instance" boolvalue="true" />
            <file name="Separator2.instance">
        	<attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator2.instance/org-openide-actions-WorkspaceSwitchAction.instance" boolvalue="true" />
            <file name="org-openide-actions-WorkspaceSwitchAction.instance"/>
        </folder>

        <attr name="Tools/Window" boolvalue="true" />
        <folder name="Window">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle" />
            <file name="org-netbeans-core-windows-actions-AttachFrameAction.instance"/>
            <attr name="org-netbeans-core-windows-actions-AttachFrameAction.instance/org-netbeans-core-windows-actions-DetachFrameAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-DetachFrameAction.instance"/>
            <attr name="org-netbeans-core-windows-actions-DetachFrameAction.instance/org-netbeans-core-windows-actions-FrameResidenceAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-FrameResidenceAction.instance"/>
            <attr name="org-netbeans-core-windows-actions-FrameResidenceAction.instance/org-netbeans-core-windows-actions-MakeDominantAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-MakeDominantAction.instance"/>
            <attr name="org-netbeans-core-windows-actions-MakeDominantAction.instance/Separator1.instance" boolvalue="true" />
            <file name="Separator1.instance">
	        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator1.instance/org-openide-actions-UndoAction.instance" boolvalue="true" />
            <file name="org-openide-actions-UndockAction.instance"/>
            <attr name="org-openide-actions-UndockAction.instance/org-netbeans-core-windows-actions-DockingAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-DockingAction.instance"/>
            <attr name="org-netbeans-core-windows-actions-DockingAction.instance/Separator2.instance" boolvalue="true" />
            <file name="Separator2.instance">
        	<attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator2.instance/org-openide-actions-CloneViewAction.instance" boolvalue="true" />
            <file name="org-openide-actions-CloneViewAction.instance"/>
            <attr name="org-openide-actions-CloneViewAction.instance/Separator3.instance" boolvalue="true" />
            <file name="Separator3.instance">
        	<attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator3.instance/org-openide-actions-NextTabAction.instance" boolvalue="true" />
            <file name="org-openide-actions-NextTabAction.instance"/>
            <attr name="org-openide-actions-NextTabAction.instance/org-openide-actions-PreviousTabAction.instance" boolvalue="true" />
            <file name="org-openide-actions-PreviousTabAction.instance"/>
            <attr name="org-openide-actions-PreviousTabAction.instance/Separator4.instance" boolvalue="true" />
            <file name="Separator4.instance">
        	<attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
	    </file>
            <attr name="Separator4.instance/org-netbeans-core-windows-actions-OpenedWindowsAction.instance" boolvalue="true" />
            <file name="org-netbeans-core-windows-actions-OpenedWindowsAction.instance"/>
        </folder>
        
    </folder>

    <folder name="Toolbars">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:org/netbeans/core/resources/toolbarsOption.gif"/>
    </folder>

    <folder name="xml">
        <folder name="entities">
            <folder name="NetBeans_IDE">
            	<!-- public ID defined by XMLDataObject -->
            	<file name="DTD_toolbar" url="nbres:/org/netbeans/core/windows/resources/toolbar.dtd" >
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans IDE//DTD toolbar//EN"/>
            	</file>
            </folder>

            <folder name="NetBeans">
            	<!-- Core window system -->
            	<file name="DTD_Workspace_Properties_1_0" url="nbres:/org/netbeans/core/windows/resources/workspace-properties1_0.dtd">
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Workspace Properties 1.0//EN"/>
            	</file>
            	<file name="DTD_Workspace_Properties_1_1" url="nbres:/org/netbeans/core/windows/resources/workspace-properties1_1.dtd">
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Workspace Properties 1.1//EN"/>
            	</file>
            	<file name="DTD_Mode_Properties_1_0" url="nbres:/org/netbeans/core/windows/resources/mode-properties1_0.dtd">
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Mode Properties 1.0//EN"/>
            	</file>
            	<file name="DTD_Mode_Properties_1_1" url="nbres:/org/netbeans/core/windows/resources/mode-properties1_1.dtd">
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Mode Properties 1.1//EN"/>
            	</file>
                <file name="DTD_Mode_Properties_1_2" url="nbres:/org/netbeans/core/windows/resources/mode-properties1_2.dtd">
                        <attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Mode Properties 1.2//EN"/>
                </file>
            	<file name="DTD_Top_Component_in_Mode_Properties_1_0" url="nbres:/org/netbeans/core/windows/resources/tc-ref1_0.dtd">
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Top Component in Mode Properties 1.0//EN"/>
            	</file>
            	<file name="DTD_Window_Manager_1_0" url="nbres:/org/netbeans/core/windows/resources/windowmanager-properties1_0.dtd">
                	<attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Window Manager 1.0//EN"/>
            	</file>
                <file name="DTD_Window_Manager_1_1" url="nbres:/org/netbeans/core/windows/resources/windowmanager-properties1_1.dtd">
                        <attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Window Manager 1.1//EN"/>
                </file>
            </folder>
    	</folder>

        <!-- XMLDataObject.Info processors -->
        <folder name="lookups">
            <!-- PENDING moved to core/windows-->
            <folder name="NetBeans_IDE">
                <!-- public ID defined by XMLDataObject -->
                <file name="DTD_toolbar.instance">
                    <attr name="instanceClass" stringvalue="org.openide.loaders.XMLDataObject$Processor"/>
                    <attr name="instanceCreate" newvalue="org.netbeans.core.windows.toolbars.ToolbarProcessor"/>
                </file>
            </folder>
        </folder>

    </folder>
        
</filesystem>
