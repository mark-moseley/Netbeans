        </folder>

    </folder> <!-- Actions -->
  
    <!-- Lines marked as erroneous: -->
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="org-netbeans-modules-xml-core-error.xml" url="error-annotation.xml"/>
        </folder>
        <folder name="text">
            <folder name="xml">    
