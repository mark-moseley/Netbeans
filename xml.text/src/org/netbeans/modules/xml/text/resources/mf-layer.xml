                </folder>
                <folder name="x-ant+xml">
                    <file name="org-netbeans-modules-xml-text-navigator-XMLNavigatorPanel.instance"/>
                </folder>
                <folder name="x-dd-servlet2.4">
                    <file name="org-netbeans-modules-xml-text-navigator-XMLNavigatorPanel.instance"/>
                </folder>
                <folder name="x-dd-sjsas-servlet2.41">
