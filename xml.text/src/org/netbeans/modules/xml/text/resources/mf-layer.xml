    <folder name="Services">
        <folder name="IndentEngine">
            <file name="org-netbeans-modules-xml-text-settings-XMLIndentEngine.settings" url="XMLIndentEngine.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.xml.text.indent.Bundle"/>
            </file>
            <file name="org-netbeans-modules-xml-text-settings-DTDIndentEngine.settings" url="DTDIndentEngine.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.xml.text.indent.Bundle"/>
            </file>
        </folder> <!-- IndentEngine -->
        
        
        

        <folder name="Hidden">
            <file name="org-netbeans-modules-xml-text-completion-ReloadActionPerformer.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.modules.xml.core.actions.XMLUpdateDocumentAction$Performer" />
            </file>
        </folder> <!-- Hidden -->
    </folder> <!-- Services -->

    <folder name="Editors">

        <folder name="text">
            <folder name="xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.xml.text.resources.Bundle"/>
                <folder name="NetBeans">
                    <folder name="Defaults">
                        <file name="coloring.xml" url="XML-fontsColors.xml">
                            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.xml.text.resources.Bundle"/>
                        </file>
                    </folder>
                </folder>
                
                <folder name="DocumentModel">
                    <file name="org-netbeans-modules-xml-text-structure-XMLDocumentModelProvider.instance"/>
                </folder>

                <folder name="FoldManager">
                    <file name="org-netbeans-modules-xml-text-folding-XmlFoldManagerFactory.instance"/>
                </folder>

                <folder name="SideBar">
                    <file name="org-netbeans-modules-xml-text-folding-XmlCodeFoldingSideBarFactory.instance"/>
                    <attr name="org-netbeans-editor-GlyphGutter.instance/org-netbeans-modules-xml-text-folding-XmlCodeFoldingSideBarFactory.instance" boolvalue="true"/>
                </folder>

                <folder name="Popup">
                    <file name="org-netbeans-modules-xml-text-syntax-XMLViewActions.instance"/>
                    <attr name="org-netbeans-modules-xml-text-syntax-XMLViewActions.instance/org-netbeans-modules-xml-core-actions-CollectXMLAction.instance" boolvalue="true"/>
                    <!-- implicit separator provided by above action group -->
                    <file name="org-netbeans-modules-xml-core-actions-CollectXMLAction.instance"/>

                    <file name="format"/>
                    <file name="SeparatorAfterFormat.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                    </file>

                    <attr name="org-netbeans-modules-xml-core-actions-CollectXMLAction.instance/SeparatorAfterFormat.instance" boolvalue="true"/>
                    <attr name="format/SeparatorAfterFormat.instance" boolvalue="true"/>

                    <attr name="SeparatorAfterFormat.instance/org-openide-actions-CutAction.instance" boolvalue="true"/>
                    <!-- Cut, Copy, Paste actions block -->
                    <attr name="org-openide-actions-PasteAction.instance/ToolsSeparator.instance" boolvalue="true"/>

                    <file name="ToolsSeparator.instance"><attr name="instanceClass" stringvalue="javax.swing.JSeparator"/></file>
                    <attr name="ToolsSeparator.instance/org-openide-actions-ToolsAction.instance" boolvalue="true"/>

                    <file name="org-openide-actions-ToolsAction.instance"/>
                </folder>

                <folder name="Defaults">
                    <file name="abbreviations.xml" url="XMLEditor-abbreviations.xml"/>
                </folder>

                <file name="Settings.settings" url="XMLEditor-options.xml"/>

                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.xml.text.syntax.XMLKit"/>
                </file>

                <!-- <folder name="Toolbars">
                    <folder name="Default">
                        <attr name="stop-macro-recording/XMLSeparator1.instance" boolvalue="true"/>
                        <file name="XMLSeparator1.instance">
                            <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        </file>

                        <attr name="XMLSeparator1.instance/xml-comment" boolvalue="true"/>
                        <file name="xml-comment"/>
                        <attr name="xml-comment/xml-uncomment" boolvalue="true"/>
                        <file name="xml-uncomment"/>
                    </folder> <!__ Default __>
                </folder> -->
            </folder> <!-- xml -->

            <folder name="xml-external-parsed-entity">
                <!-- It reuses kit only -->
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.xml.text.syntax.XMLKit"/>
                </file>
            </folder> <!-- xml-external-parsed-entity -->
        </folder> <!-- text -->

        <folder name="application">
            <folder name="xml-dtd">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.xml.text.resources.Bundle"/>
                <folder name="NetBeans">
                    <folder name="Defaults">
                        <file name="coloring.xml" url="DTD-fontsColors.xml">
                            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.xml.text.resources.Bundle"/>
                        </file>
                    </folder>
                </folder>
                <folder name="Popup">

                    <attr name="org-openide-actions-PasteAction.instance/org-netbeans-modules-xml-core-actions-CollectDTDAction.instance" boolvalue="true"/>
                    <!-- implicit separator provided by below action group -->    
                    <file name="org-netbeans-modules-xml-core-actions-CollectDTDAction.instance"/>
                    <attr name="org-netbeans-modules-xml-core-actions-CollectDTDAction.instance/ToolsSeparator.instance" boolvalue="true"/>

                    <file name="ToolsSeparator.instance"><attr name="instanceClass" stringvalue="javax.swing.JSeparator"/></file>
                    <attr name="ToolsSeparator.instance/org-openide-actions-ToolsAction.instance" boolvalue="true"/>

                    <file name="org-openide-actions-ToolsAction.instance"/>
                </folder>

                <folder name="Defaults">
                    <file name="abbreviations.xml" url="DTDEditor-abbreviations.xml"/>
                <!--    <file name="fontsColors.xml" url="DTDEditor-fontsColors.xml"/>-->
                </folder>

                <file name="Settings.settings" url="DTDEditor-options.xml"/>
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.xml.text.syntax.DTDKit"/>
                </file>
            </folder> <!-- xml-dtd -->
        </folder> <!-- application -->
    </folder> <!-- Editors -->
    
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="application">
                <file name="xml-dtd" url="DTDExample"/>
            </folder>
            <folder name="text">
                <file name="xml" url="XMLExample"/>
            </folder>
        </folder>
    </folder>

</filesystem>
