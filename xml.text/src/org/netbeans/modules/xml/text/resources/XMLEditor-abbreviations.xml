<!DOCTYPE codetemplates PUBLIC  "-//NetBeans//DTD Editor Code Templates settings 1.0//EN"
                                "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">

<codetemplates>
    <codetemplate abbreviation="?xm"><code><![CDATA[<?xml version="1.0" encoding="UTF-8"?>]]></code></codetemplate>
    <codetemplate abbreviation="!do"><code><![CDATA[<!DOCTYPE ]]></code></codetemplate>
    <codetemplate abbreviation="!cd"><code>&lt;![CDATA[${cursor}]]&gt;</code></codetemplate>

    <!-- DTD abbrevs subset useful for internal DTD -->
    <codetemplate abbreviation="!at"><code><![CDATA[<!ATTLIST ${cursor}>]]></code></codetemplate>
    <codetemplate abbreviation="!el"><code><![CDATA[<!ELEMENT ${cursor}>]]></code></codetemplate>
    <codetemplate abbreviation="!en"><code><![CDATA[<!ENTITY ${cursor}>]]></code></codetemplate>
    <codetemplate abbreviation="pu"><code><![CDATA[PUBLIC "${cursor}"]]></code></codetemplate>
    <codetemplate abbreviation="sy"><code><![CDATA[SYSTEM "${cursor}"]]></code></codetemplate>
</codetemplates>
