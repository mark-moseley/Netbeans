<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
    
    <folder name="Databases">
        <folder name="Connections"/>
        <folder name="JDBCDrivers">
            <file name="JDBCODBC.xml" url="JDBCODBC.xml"/>
        </folder>
        <folder name="Runtimes"/>
    </folder>

    <folder name="Services">
        <file name="org-netbeans-modules-db-explorer-DatabaseOption.settings" url="DatabaseOption.settings">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.db.resources.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/db/resources/optionIcon.gif"/>
        </file>

        <folder name="JavaHelp"> 
            <attr name="org-netbeans-modules-usersguide-above-regular.txt/org-netbeans-modules-db-helpset.xml" boolvalue="true"/> 
            <file name="org-netbeans-modules-db-helpset.xml" url="org-netbeans-modules-db-helpset.xml"/> 
            <attr name="org-netbeans-modules-db-helpset.xml/org-netbeans-modules-usersguide-below-regular.txt" boolvalue="true"/> 
        </folder>

    </folder>

    <!-- UI -->
    <folder name="UI">
        <folder name="Services">
            <folder name="IDEConfiguration">
                <folder name="ServerAndExternalToolSettings">
                    <file name="DatabaseOption.shadow">
                        <attr name="originalFile" stringvalue="Services/org-netbeans-modules-db-explorer-DatabaseOption.settings"/>
                    </file>                    
                </folder> <!-- ServerAndExternalToolSettings -->
            </folder> <!-- IDEConfiguration -->
        </folder> <!-- Services -->

        <folder name="Runtime">
           <file name="org-netbeans-modules-db-explorer-nodes-RootNode.instance">
             <attr name="instanceClass" stringvalue="org.netbeans.modules.db.explorer.nodes.RootNode"/>
             <attr name="instanceCreate" methodvalue="org.netbeans.modules.db.explorer.nodes.RootNode.getInstance"/>
           </file>
        </folder> <!-- Runtime -->

    </folder> <!-- UI -->

    <folder name="xml">
        <folder name="entities">
            <folder name="NetBeans" >
                <file name="DTD_JDBC_Driver_1_0" url="jdbc-driver-1_0.dtd">
                    <attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD JDBC Driver 1.0//EN"/>
                </file>
                <file name="DTD_JDBC_Driver_1_1" url="jdbc-driver-1_1.dtd">
                    <attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD JDBC Driver 1.1//EN"/>
                </file>
                <file name="DTD_Database_Connection_1_0" url="connection-1_0.dtd">
                    <attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Database Connection 1.0//EN"/>
                </file>
            </folder>
        </folder>
        
        <folder name="lookups">
            <folder name="NetBeans">
                <file name="DTD_JDBC_Driver_1_0.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.db.explorer.driver.JDBCDriverConvertor"/>
                    <attr name="instanceOf" stringvalue="org.openide.loaders.Environment.Provider"/>
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.db.explorer.driver.JDBCDriverConvertor.createProvider"/>
                </file>
                <file name="DTD_JDBC_Driver_1_1.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.db.explorer.driver.JDBCDriverConvertor"/>
                    <attr name="instanceOf" stringvalue="org.openide.loaders.Environment.Provider"/>
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.db.explorer.driver.JDBCDriverConvertor.createProvider"/>
                </file>
                <file name="DTD_Database_Connection_1_0.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.db.explorer.DatabaseConnectionConvertor"/>
                    <attr name="instanceOf" stringvalue="org.openide.loaders.Environment.Provider"/>
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.db.explorer.DatabaseConnectionConvertor.createProvider"/>
                </file>
            </folder>
        </folder>
    </folder>
    
</filesystem>
