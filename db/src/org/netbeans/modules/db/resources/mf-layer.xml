<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

    <folder name="Menu">
        <folder name="Help">
            <folder name="HelpShortcuts">
                <attr name="org-netbeans-modules-usersguide-sep.instance/org-netbeans-modules-db-mainpage.xml" boolvalue="true"/>
                <file name="org-netbeans-modules-db-mainpage.xml" url="org-netbeans-modules-db-mainpage.xml">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.db.resources.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/db/resources/helpset.gif"/>
                </file>
                <attr name="org-netbeans-modules-db-mainpage.xml/org-netbeans-modules-usersguide-lower-sep.instance" boolvalue="true"/>
            </folder>
        </folder>
    </folder>

    <folder name="Services">
        <file name="org-netbeans-modules-db-explorer-DatabaseOption.settings" url="DatabaseOption.settings">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.db.resources.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/db/resources/optionIcon.gif"/>
            <attr name="SystemFileSystem.layer" stringvalue="Project"/>
        </file>
        
        <folder name="JavaHelp"> 
            <attr name="org-netbeans-modules-usersguide-above-regular.txt/org-netbeans-modules-db-helpset.xml" boolvalue="true"/> 
            <file name="org-netbeans-modules-db-helpset.xml" url="org-netbeans-modules-db-helpset.xml"/> 
            <attr name="org-netbeans-modules-db-helpset.xml/org-netbeans-modules-usersguide-below-regular.txt" boolvalue="true"/> 
        </folder>
        
    </folder>

    <folder name="Actions">
        <folder name="Help">
            <file name="org-netbeans-modules-db-mainpage.xml" url="org-netbeans-modules-db-mainpage.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.db.resources.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/db/resources/helpset.gif"/>
            </file>
        </folder>
    </folder>
    
    
    <!-- UI -->
    <folder name="UI">
        <folder name="Services">
            <folder name="IDEConfiguration">
                <folder name="ServerAndExternalToolSettings">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.db.resources.Bundle"/>
                    <file name="DatabaseOption.shadow">
<![CDATA[Services/org-netbeans-modules-db-explorer-DatabaseOption.settings
SystemFileSystem
]]>
                    </file>
                </folder> <!-- ServerAndExternalToolSettings -->
            </folder> <!-- IDEConfiguration -->
        </folder> <!-- Services -->
    </folder> <!-- UI -->

</filesystem>
