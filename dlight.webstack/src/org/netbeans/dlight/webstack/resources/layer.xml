      </file>
      <file name="org-netbeans-dlight-core-actions-StopDLightAction.shadow">
        <attr name="originalFile" stringvalue="Actions/DLight/org-netbeans-dlight-core-actions-StopDLightAction.instance"/>
        <attr name="position" intvalue="20"/>
      </file>
    </folder>
  </folder>
  <folder name="DLight">
    <folder name="ToolConfigurationProviders">
      <file name="PhpConfigurationProvider.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.dlight.webstack.PhpConfigurationProvider"/>
      </file>
      <file name="MySQLConfigurationProvider.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.dlight.webstack.MySQLConfigurationProvider"/>
      </file>
    </folder>
    <folder name="Configurations">
      <folder name="Photon">
        <folder name="KnownToolsConfigurationProviders">
          <file name="PhpConfigurationProvider.shadow">
            <attr name="originalFile" stringvalue="DLight/ToolConfigurationProviders/PhpConfigurationProvider.instance"/>
          </file>
          <file name="MySQLConfigurationProvider.shadow">
            <attr name="originalFile" stringvalue="DLight/ToolConfigurationProviders/MySQLConfigurationProvider.instance"/>
          </file>
        </folder>
      </folder>
    </folder>
  </folder>
    
</filesystem>
