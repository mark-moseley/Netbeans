<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
  <folder name="Actions">
    <folder name="DLight">
      <file name="org-netbeans-dlight-core-actions-StopDLightAction.instance"/>
      <file name="org-netbeans-dlight-webstack-actions-StartWebStackAction.instance">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.dlight.core.resources.Bundle"/>
        <attr name="delegate" newvalue="org.netbeans.dlight.webstack.actions.StartWebStackAction"/>
        <attr name="displayName" bundlevalue="org.netbeans.dlight.webstack.actions.Bundle#CTL_StartWebStackAction"/>
        <attr name="iconBase" stringvalue="org/netbeans/dlight/core/resources/gimzo.png"/>
        <attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/>
        <attr name="noIconInMenu" stringvalue="false"/>
      </file>

    </folder>
  </folder>
  <folder name="Toolbars">
    <folder name="Build">
      <file name="org-netbeans-dlight-core-actions-StartWebStackAction.shadow">
        <attr name="originalFile" stringvalue="Actions/DLight/org-netbeans-dlight-webstack-actions-StartWebStackAction.instance"/>
