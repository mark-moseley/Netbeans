<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
      <folder name="org-netbeans-api-project-libraries">
        <folder name="Libraries">
          <file name="j2ee14.xml" url="j2ee14.xml"/>
          <file name="jsf11.xml" url="jsf11.xml"/>
          <file name="jaxrpc11.xml" url="jaxrpc11.xml"/>
          <file name="jaxr10.xml" url="jaxr10.xml"/>
          <file name="saaj12.xml" url="saaj12.xml"/>
          <file name="jaxqname.xml" url="jaxqname.xml"/>
        </folder>
      </folder>
    
    <folder name="Services">
      <folder name="JavaHelp">
         <!-- Merge after Core IDE Help: -->
         <attr name="org-netbeans-modules-usersguide-above-regular.txt/org-netbeans-modules-j2ee-helpset.xml" boolvalue="true"/>
         <file name="org-netbeans-modules-j2ee-helpset.xml" url="helpset-decl.xml"/>
         <!-- Merge before 3rd party help: -->
         <attr name="org-netbeans-modules-j2ee-helpset.xml/org-netbeans-modules-usersguide-below-regular.txt" boolvalue="true"/>
      </folder>
    </folder>
  
      <folder name="Menu">
        <folder name="Tools">
            <attr name="org-netbeans-modules-form-actions-InstallBeanAction.instance/J2eePlatformsCustomizerAction.instance" boolvalue="true"/>
            <attr name="J2eePlatformsCustomizerAction.instance/LibrariesCustomizerAction.instance" boolvalue="true"/>            
            <file name="J2eePlatformsCustomizerAction.instance">
                <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.platform.ui.J2eePlatformsCustomizerAction"/>
            </file>
        </folder>
    </folder>
    <!-- do not show the action at startup
	<folder name="WarmUp">
		<file name="org.netbeans.modules.j2ee.platform.QueryPlatformRoot.instance"/>
	</folder>
	-->
</filesystem>
