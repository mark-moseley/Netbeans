                    <file name="org-netbeans-modules-php-editor-codegen-ConnectionGenerator$Factory.instance">
                        <attr name="position" intvalue="100"/>
                    </file>
                    <file name="org-netbeans-modules-php-editor-codegen-TableGenerator$Factory.instance">
                        <attr name="position" intvalue="200"/>
                    </file>
                </folder>
                <folder name="CodeTemplateProcessorFactories">
                    <file name="org-netbeans-modules-php-editor-codegen-PHPCodeTemplateProcessor$Factory.instance"/>
                </folder>
                <folder name="Keybindings">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="org-netbeans-modules-php-editor-keybindings.xml" url="DefaultKeyBindings.xml"/>
                            <file name="org-netbeans-modules-php-editor-keybindings-mac.xml" url="DefaultKeyBindings-Mac.xml">
                                <attr name="nbeditor-settings-targetOS" stringvalue="OS_MAC"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
                <folder name="CodeTemplateFilterFactories">
                    <file name="org-netbeans-modules-php-editor-PHPCodeTemplateFilter$Factory.instance"/>
                </folder>
                <folder name="CodeTemplates">
                    <folder name="Defaults">
                        <file name="org-netbeans-modules-php-editor-codetemplates.xml" url="code-templates.xml">
                            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.php.editor.resources.Bundle"/>
                        </file>
                    </folder>
                </folder>
                <folder name="CompletionProviders">
                    <file name="org-netbeans-modules-html-editor-HTMLCompletionProvider.instance"/>
                    <file name="org-netbeans-modules-php-editor-FSCompletion.instance"/>
                    <file name="org-netbeans-modules-php-editor-PHPSQLCompletion.instance"/>
                </folder>
                <folder name="FoldManager">
                    <file name="org-netbeans-editor-CustomFoldManager$Factory.instance"/>
                </folder>
                <!--issue #127161:-->
                <folder name="Popup">
                    <folder name="goto">
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.php.editor.resources.Bundle"/>
                        <attr name="position" intvalue="500"/>
                    </folder>
                </folder>
                <!--end of issue #127161-->
                
                <!-- associating platte with the mime type -->
                <file name="PaletteFactory.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.spi.palette.PaletteController"/>
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.php.editor.palette.PHPPaletteFactory.getPalette"/>
                </file>
            </folder>
            <folder name="x-php-doccomment">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.php.editor.resources.Bundle"/>
                <file name="language.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.php.editor.lexer.PHPDocCommentTokenId.language"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
                </file>
            </folder>
        </folder>
