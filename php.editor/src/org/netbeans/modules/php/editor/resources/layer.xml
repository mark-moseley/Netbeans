            </folder>
        </folder>
    </folder>
    <folder name="GsfPlugins">
        <folder name="text">
            <folder name="x-php5">
                <file name="language.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.php.editor.PHPLanguage"/>
