MARK:unpairedStartTag: {
    type:"Warning";
    message:"Unfinished tag.";
}
MARK:unpairedEndTag: {
    type:"Error";
    message:"Unpaired end tag.";
}
MARK:html_ERROR: {
    type:"Error";
    message:"Unpaired end tag.";
}
MARK:error: {
    type:"Error";
    message:"Unexpected character.";
}

