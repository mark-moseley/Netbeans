}
COLOR:gsp_element_name: {
    foreground_color: "blue";
    font_type: "bold";
}
COLOR:gsp_element_end: {
    foreground_color: "blue";
    font_type: "bold";
}
COLOR:gsp_end_element_end: {
    foreground_color: "blue";
    font_type: "bold";
}
COLOR:gsp_attribute_value: {
    default_coloring:"string";
}

########### code folding #######################################################

