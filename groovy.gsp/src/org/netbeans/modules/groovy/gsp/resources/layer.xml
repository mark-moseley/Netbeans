<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Loaders">
        <folder name="text">
            <folder name="x-gsp">
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/groovy/gsp/resources/GspFile16x16.png"/>
                <attr name="iconBase" stringvalue="org/netbeans/modules/groovy/gsp/resources/GspFile16x16.png"/>
                <folder name="Factories">
                    <file name="org-netbeans-modules-groovy-gsp-loaders-GspDataLoader.instance">
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.groovy.gsp.resources.Bundle"/>
                        <attr name="position" intvalue="99999"/>
                    </file>
                </folder>
                <folder name="Actions">
                    <file name="org-openide-actions-OpenAction.instance">
                        <attr name="position" intvalue="100"/>
                    </file>
                    <file name="org-openide-actions-FileSystemAction.instance">
                        <attr name="position" intvalue="200"/>
                    </file>
                    <file name="sep-1.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="300"/>
                    </file>
                    <file name="org-openide-actions-CutAction.instance">
                        <attr name="position" intvalue="400"/>
                    </file>
                    <file name="org-openide-actions-CopyAction.instance">
                        <attr name="position" intvalue="500"/>
                    </file>
                    <file name="sep-2.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="600"/>
                    </file>
                    <file name="org-openide-actions-DeleteAction.instance">
                        <attr name="position" intvalue="700"/>
                    </file>
                    <file name="org-openide-actions-RenameAction.instance">
                        <attr name="position" intvalue="800"/>
                    </file>
                    <file name="sep-3.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="900"/>
                    </file>
                    <file name="org-openide-actions-SaveAsTemplateAction.instance">
                        <attr name="position" intvalue="1000"/>
                    </file>
                    <file name="sep-4.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="1100"/>
                    </file>
                    <file name="org-openide-actions-ToolsAction.instance">
                        <attr name="position" intvalue="1200"/>
                    </file>
                    <file name="org-openide-actions-PropertiesAction.instance">
                        <attr name="position" intvalue="1300"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="GspResolver.xml" url="GspResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.groovy.gsp.resources.Bundle"/>
                <attr name="position" intvalue="255"/>
            </file>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="Groovy">
            <file name="_view.gsp" url="GspTemplate.gsp">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.groovy.gsp.resources.Bundle"/>
                <attr name="position" intvalue="120"/>
                <attr name="templateCategory" stringvalue="groovy"/>
                <attr name="javax.script.ScriptEngine" stringvalue="freemarker"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/groovy/gsp/resources/GspTemplate.html"/>
            </file>
        </folder>
    </folder>

    <folder name="Editors">
        <folder name="text">
            <folder name="x-gsp">
                <file name="org-netbeans-modules-groovy-gsp-GspIndentTaskFactory.instance"/>
                <file name="org-netbeans-modules-groovy-gsp-EmbeddedSectionsHighlighting$Factory.instance"/>
                <file name="org-netbeans-modules-gsfret-editor-semantic-HighlightsLayerFactoryImpl.instance"/>

                <file name="language.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.groovy.gsp.lexer.GspTokenId.language"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
                </file>
                
                <folder name="CompletionProviders">
                    <file name="org-netbeans-modules-html-editor-HTMLCompletionProvider.instance"/>
                    <file name="org-netbeans-modules-gsfret-editor-completion-GsfCompletionProvider.instance"/>
                    <!-- This doesn't work right because most of the templates don't start with characters
                         that are identifiers
                    <file name="org-netbeans-lib-editor-codetemplates-CodeTemplateCompletionProvider.instance"/>
                    -->
                </folder>
   
                <folder name="FoldManager">
                    <file name="org-netbeans-modules-gsfret-editor-fold-GsfFoldManagerFactory.instance"/>
                </folder>
                <folder name="SideBar">
                    <file name="org-netbeans-modules-editor-gsfret-GsfCodeFoldingSideBarFactory.instance">
                        <attr name="position" intvalue="1200"/>
                    </file>
                </folder>
                <!--
                <folder name="CodeTemplateFilterFactories">
                    <file name="org-netbeans-modules-groovy-gsp-editor-GspCodeTemplateFilter$Factory.instance"/>
                    OR:
                    <file name="org-netbeans-modules-gsfret-editor-codetemplates-GsfCodeTemplateFilter$Factory.instance"/>
                </folder>   
                -->

                <folder name="HyperlinkProviders">
                    <file name="GsfHyperlinkProvider.instance">
                        <attr name="instanceClass" stringvalue="org.netbeans.modules.gsfret.editor.hyperlink.GsfHyperlinkProvider"/>
                        <attr name="instanceOf" stringvalue="org.netbeans.lib.editor.hyperlink.spi.HyperlinkProviderExt"/>
                    </file>
                </folder>

                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.groovy.gsp.resources.Bundle"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="coloring.xml" url="fontsColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.groovy.gsp.resources.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>

                <folder name="Preferences">
                    <folder name="Defaults">
                        <file name="org-netbeans-modules-groovy-gsp-editor-preferences.xml" url="editor-preferences.xml"/>
                    </folder>
                </folder>

                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.groovy.gsp.editor.GspKit"/>
                    <attr name="instanceOf" stringvalue="javax.swing.text.EditorKit,org.netbeans.modules.groovy.gsp.editor.GspKit"/>
                    <attr name="beaninfo" boolvalue="false"/>
                </file>
            
                <folder name="CodeTemplates">
                    <folder name="Defaults">
                        <file name="codetemplates.xml" url="codetemplates.xml">
                            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.groovy.gsp.resources.Bundle"/>
                        </file>
                    </folder>
                </folder>

                <folder name="Keybindings">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="org-netbeans-modules-groovy-gsp-keybindings.xml" url="DefaultKeyBindings.xml"/>
                            <file name="org-netbeans-modules-groovy-gsp-keybindings-mac.xml" url="DefaultKeyBindings-Mac.xml">
                                <attr name="nbeditor-settings-targetOS" stringvalue="OS_MAC"/>
                            </file>
                        </folder>
                    </folder>
                </folder>

                <folder name="Popup">
                    <file name="format">
                        <attr name="position" intvalue="1480"/>
                    </file>
                
                    <file name="SeparatorAfterGoToHelp.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="1240"/>
                    </file>
                    
                    <file name="SeparatorBeforeCut.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="1960"/>
                    </file>                    
                </folder>
                
                <folder name="text">
                    <folder name="html">
                        <folder name="CodeTemplates">
                            <folder name="Defaults">
                                <file name="codetemplates.shadow">
                                    <attr name="originalFile" stringvalue="Editors/application/x-httpd-eruby"/>
                                </file>
                            </folder>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-gsp" url="GspExample"/>
            </folder>
        </folder>
    </folder>
    <folder name="GsfPlugins">
        <folder name="text">
            <folder name="x-gsp">
                <attr name="useCustomEditorKit" boolvalue="true"/>
                <file name="language.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.groovy.gsp.GspLanguage"/>
                </file>
                <file name="structure.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.groovy.editor.api.StructureAnalyzer"/>
                </file>
            </folder>
        </folder>
    </folder>

</filesystem>
