<!DOCTYPE codetemplates PUBLIC  "-//NetBeans//DTD Editor Code Templates settings 1.0//EN"
                                "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">
<codetemplates>
    <codetemplate abbreviation="ag">
        <code><![CDATA[application.getAttribute("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="ap">
        <code><![CDATA[application.setAttribute("${cursor}", )]]></code>
    </codetemplate>
    <codetemplate abbreviation="ar">
        <code><![CDATA[application.removeAttribute("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="cfgi">
        <code><![CDATA[config.getInitParameter("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="jspf">
        <code><![CDATA[<jsp:forward page="${cursor}"/>]]></code>
    </codetemplate>
    <codetemplate abbreviation="jspg">
        <code><![CDATA[<jsp:getProperty name="${cursor}" property=""/>]]></code>
    </codetemplate>
    <codetemplate abbreviation="jspi">
        <code><![CDATA[<jsp:include page="${cursor}"/>]]></code>
    </codetemplate>
    <codetemplate abbreviation="jspp">
        <code>
<![CDATA[<jsp:plugin type="${cursor}" code="" codebase="">
</jsp:plugin>]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="jsps">
        <code><![CDATA[<jsp:setProperty name="${cursor}" property=""/>]]></code>
    </codetemplate>
    <codetemplate abbreviation="jspu">
        <code><![CDATA[<jsp:useBean id="${cursor}" type=""/>]]></code>
    </codetemplate>
    <codetemplate abbreviation="oup">
        <code><![CDATA[out.print("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="oupl">
        <code><![CDATA[out.println("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="pcg">
        <code><![CDATA[pageContext.getAttribute("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="pcgn">
        <code><![CDATA[pageContext.getAttributeNamesInScope(${cursor})]]></code>
    </codetemplate>
    <codetemplate abbreviation="pcgs">
        <code><![CDATA[pageContext.getAttributesScope("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="pcr">
        <code><![CDATA[pageContext.removeAttribute("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="pcs">
        <code><![CDATA[pageContext.setAttribute("${cursor}", )]]></code>
    </codetemplate>
    <codetemplate abbreviation="pg">
        <code><![CDATA[<%@page ${cursor}%>]]></code>
    </codetemplate>
        
    <!-- default is true so false is only meaningful value -->
    <codetemplate abbreviation="pga">
        <code><![CDATA[<%@page autoFlush="false"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgb">
        <code><![CDATA[<%@page buffer="${cursor}kb"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgc">
        <code><![CDATA[<%@page contentType="${cursor}"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgerr">
        <code><![CDATA[<%@page errorPage="${cursor}"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgex">
        <code><![CDATA[<%@page extends="${cursor}"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgie">
        <code><![CDATA[<%@page isErrorPage="true"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgim">
        <code><![CDATA[<%@page import="${cursor}"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgin">
        <code><![CDATA[<%@page info="${cursor}"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="pgit">
        <code><![CDATA[<%@page isThreadSafe="false"%>]]></code>
    </codetemplate>
    
    <!-- only defined and required value this attribute is 'java' -->
    <codetemplate abbreviation="pgl">
        <code><![CDATA[<%@page language="java"%>]]></code>
    </codetemplate>
        
    <!-- default is true so false is only meaningful value -->
    <codetemplate abbreviation="pgs">
        <code><![CDATA[<%@page session="false"%>]]></code>
    </codetemplate>
    <codetemplate abbreviation="rg">
        <code><![CDATA[request.getParameter("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="sg">
        <code><![CDATA[session.getAttribute("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="sp">
        <code><![CDATA[session.setAttribute("${cursor}", )]]></code>
    </codetemplate>
    <codetemplate abbreviation="sr">
        <code><![CDATA[session.removeAttribute("${cursor}")]]></code>
    </codetemplate>
    <codetemplate abbreviation="tglb">
        <code><![CDATA[<%@taglib uri="${cursor}"%>]]></code>
    </codetemplate>
</codetemplates>

