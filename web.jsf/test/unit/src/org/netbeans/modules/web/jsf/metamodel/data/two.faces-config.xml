     	<managed-bean-name>twoManagedBean</managed-bean-name>
     	<managed-bean-class>twoManagedBeanClass</managed-bean-class>
     	<managed-bean-scope>some expression</managed-bean-scope>
     </managed-bean>
     <navigation-rule/>
     <render-kit>
     	<renderer>
     		<renderer-class>twoRendererClass</renderer-class>
     		<renderer-type>twoRendererType</renderer-type>
     		<description>twoComponentFamily</description>
     	</renderer>
     </render-kit>
     <validator>
     	<validator-id>twoValidatorId</validator-id>
     	<validator-class>twoValidatorClass</validator-class>
     </validator>
              
