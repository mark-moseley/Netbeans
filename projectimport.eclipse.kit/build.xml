        <ant dir="../projectimport.eclipse.j2se" target="netbeans"/>
        <ant dir="../projectimport.eclipse.web" target="netbeans"/>
        <antcall target="projectized.debug">
            <param name="tryme.args" value="-J-Dnetbeans.exception.report.min.level=999999 -J-Dorg.netbeans.modules.projectimport.eclipse.level=0"/>
        </antcall>
