<filesystem>
	<folder name="org-netbeans-api-project-libraries">
    	<folder name="Libraries">
			<file name="junit.xml" url="junit_lib.xml"/>
		</folder>
	</folder>

    <!-- templates -->
    <folder name="Templates">
    	<attr name="Classes/JUnit" boolvalue="true"/>
    	<attr name="JarPackager/JUnit" boolvalue="true"/>    	
    	<attr name="JUnit/Ant" boolvalue="true"/>
    	<attr name="JUnit/Other" boolvalue="true"/>          
        <folder name="JUnit">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.junit.Bundle"/>
            <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/templates/JUnitFolder.html"/>
            <attr name="EmptyJUnitTest.java/SimpleJUnitTest.java" boolvalue="true"/>
            <attr name="TestSuite.java/Package.shadow" boolvalue="true"/>
            <attr name="SimpleJUnitTest.java/TestSuite.java" boolvalue="true"/>
            <attr name="SimpleJUnitTest.java/Package.shadow" boolvalue="true"/>
            
            <file name="EmptyJUnitTest.java" url="templates/EmptyJUnitTest.java.template">
                <attr name="template" boolvalue="true"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.junit.Bundle"/> 
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/templates/EmptyJUnitTest.html"/>
                <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.junit.wizards.EmptyTestCaseWizardIterator.singleton"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/JUnitLogo.png"/>
                <attr name="templateCategory" stringvalue="junit"/>
            </file>
            
            <file name="SimpleJUnitTest.java" url="templates/SimpleJUnitTest.java.template">
                <attr name="template" boolvalue="true"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.junit.Bundle"/> 
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/templates/SimpleJUnitTest.html"/>
                <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.junit.wizards.SimpleTestCaseWizardIterator.singleton"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/JUnitLogo.png"/>
                <attr name="templateCategory" stringvalue="junit"/>
            </file>
            
            <file name="TestSuite.java" url="templates/TestSuite.java.template">
                <attr name="template" boolvalue="true"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.junit.Bundle"/> 
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/templates/TestSuite.html"/>
                <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.junit.wizards.TestSuiteWizardIterator.singleton"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/junit/resources/JUnitLogo.png"/>
                <attr name="templateCategory" stringvalue="junit"/>
            </file>
            <file name="Package.shadow">
                <attr name="originalFile" stringvalue="Templates/Classes/Package"/>
            </file>
        </folder>
    </folder>

    <!-- editors -->
    <folder name="Editors">
        <folder name="text">
            <folder name="x-java">
                <folder name="Popup">
                    <folder name="goto">
                        <attr name="goto-super-implementation/GoToOppositeAction.shadow" boolvalue="true"/>
                        <file name="GoToOppositeAction.shadow">
                            <attr name="originalFile" stringvalue="Actions/JUnitTests/org-netbeans-modules-junit-GoToOppositeAction.instance"/>
                        </file>
                        <attr name="GoToOppositeAction.shadow/goto" boolvalue="true"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>

    <!-- actions -->
    <folder name="Actions">
        <folder name="JUnitTests">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.junit.Bundle"/>
            <file name="org-netbeans-modules-junit-GoToOppositeAction.instance" />
            <file name="org-netbeans-modules-junit-CreateTestAction.instance" />
        </folder>
       
        <folder name="Window">
            <file name="org-netbeans-modules-junit-output-ResultWindowOpenAction.instance">
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/junit/output/res/testResults.png"/> 
            </file>
        </folder> <!-- Window -->

    </folder>
    
    <folder name="Menu">
        <folder name="GoTo">
            <file name="org-netbeans-modules-junit-GoToOppositeAction.shadow">
                <attr name="originalFile" stringvalue="Actions/JUnitTests/org-netbeans-modules-junit-GoToOppositeAction.instance"/>            
            </file>
            <attr name="org-netbeans-modules-junit-GoToOppositeAction.shadow/org-netbeans-core-windows-actions-RecentViewListAction.shadow" boolvalue="true"/>   

        </folder>
        <folder name="Window">
            <attr name="ViewRuntimeTabAction.shadow/ResultWindowOpenAction.shadow" boolvalue="true"/>
            <attr name="ResultViewOpenAction.shadow/ResultWindowOpenAction.shadow" boolvalue="true"/>
            <attr name="FindUsagesOpenAction.shadow/ResultWindowOpenAction.shadow" boolvalue="true"/>
            <attr name="org-netbeans-core-actions-ViewRuntimeTabAction.instance/ResultWindowOpenAction.shadow" boolvalue="true"/>
            <attr name="org-netbeans-modules-search-ResultViewOpenAction.instance/ResultWindowOpenAction.shadow" boolvalue="true"/>
            <attr name="org-netbeans-modules-refactoring-ui-FindUsagesOpenAction.instance/ResultWindowOpenAction.shadow" boolvalue="true"/>
            <file name="ResultWindowOpenAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-junit-output-ResultWindowOpenAction.instance"/>
            </file> <!-- ResultWindowOpenAction -->
            <attr name="ResultWindowOpenAction.shadow/org-netbeans-modules-refactoring-ui-RefactoringPreviewOpenAction.instance" boolvalue="true"/>
            <attr name="ResultWindowOpenAction.shadow/org-netbeans-modules-versioning-system-cvss-ui-actions-status-OpenVersioningAction.instance" boolvalue="true"/>
            <attr name="ResultWindowOpenAction.shadow/org-netbeans-core-windows-actions-SwitchToRecentDocumentAction.instance" boolvalue="true"/>
            <attr name="ResultWindowOpenAction.shadow/RefactoringPreviewOpenAction.shadow" boolvalue="true"/>
            <attr name="ResultWindowOpenAction.shadow/OpenVersioningAction.shadow" boolvalue="true"/>
            <attr name="ResultWindowOpenAction.shadow/SwitchToRecentDocumentAction.shadow" boolvalue="true"/>
        </folder> <!-- Window -->
    </folder>
    

    <!-- shortcuts -->
    <folder name="Shortcuts">
        <file name="DS-U.instance">
	    <attr name="instanceClass" stringvalue="org.netbeans.modules.junit.CreateTestAction"/>
	</file>
        <file name="OS-E.instance">
	    <attr name="instanceClass" stringvalue="org.netbeans.modules.junit.GoToOppositeAction"/>
	</file>
        <file name="DS-6.shadow">
            <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-junit-output-ResultWindowOpenAction.instance"/>
	</file>
    </folder>
    
    <folder name="UI">
    	<folder name="Services">
                <!-- Display the system option under the "Testing" category in Options. -->
        	<folder name="Testing">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.junit.Bundle"/>
                    <!-- Point to the real settings file (always on the system file system). -->
                    <file name="org-netbeans-modules-junit-JUnitSettingsNode.instance">
                        <attr name="instanceCreate" methodvalue="org.netbeans.modules.junit.JUnitSettings.createViewNode"/>
                    </file>
                </folder>
    	</folder>
    </folder>

    <folder name="Windows2">
        <folder name="Modes">
            <folder name="output">        <!-- "output" is defined in core/ui -->
                <file name="junit-test-results.wstcref" url="../output/junit-test-results.wstcref"/>
            </folder>
        </folder>
        <folder name="Components">
            <file name="junit-test-results.settings" url="../output/junit-test-results.settings"/>
        </folder>
    </folder>

</filesystem>
