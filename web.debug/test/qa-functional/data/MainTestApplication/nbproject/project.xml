<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>MainTestApplication</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <web-module-libraries>
                <library files="1">
                    <file>${reference.TestLibrary.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${reference.TestFreeformLibrary.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${reference.TestTagLibrary.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="2">
                    <file>${libs.jstl11.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.secondaryjava.dir" name="Secondary sources"/>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>TestFreeformLibrary</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>TestLibrary</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>TestTagLibrary</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
