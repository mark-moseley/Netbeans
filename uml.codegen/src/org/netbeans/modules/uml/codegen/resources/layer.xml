<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    
    <folder name="Templates">
        <folder name="UML">
            <folder name="CodeGeneration">            
                <folder name="Java">     
                    
                    <file name="CompilationUnit.java" url="templates/java/CompilationUnit_java">
                        <attr name="template" boolvalue="true" />
                        <attr name="templateCategory" stringvalue="uml-java-templates"/>
                    </file>
                    <file name="NormalClassDeclaration.java" url="templates/java/NormalClassDeclaration_java">
                        <attr name="template" boolvalue="true" />
                        <attr name="templateCategory" stringvalue="uml-java-templates"/>
                    </file>
                    <file name="EnumDeclaration.java" url="templates/java/EnumDeclaration_java">
                        <attr name="template" boolvalue="true" />
                        <attr name="templateCategory" stringvalue="uml-java-templates"/>
                    </file>
                    <file name="InterfaceDeclaration.java" url="templates/java/InterfaceDeclaration_java">
                        <attr name="template" boolvalue="true" />
                        <attr name="templateCategory" stringvalue="uml-java-templates"/>
                    </file>
                    <file name="DeclLib.ftl" url="templates/java/DeclLib_ftl">
                        <attr name="template" boolvalue="true" />
                        <attr name="templateCategory" stringvalue="uml-java-templates"/>
                    </file>
                    
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="UML">
        <folder name="UMLOptions">
            <folder name="Panels">
            <file name="CodeGenOptionsPanel.instance">
                <attr name="instanceClass" stringvalue="org.netbeans.modules.uml.codegen.ui.CodeGenOptionsPanel"/>
            </file>
            </folder>
        </folder>
    </folder>

<folder name="Projects">
        <folder name="org-netbeans-modules-uml-project">
            <folder name="Customizer">
                <file name="Templates.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.uml.codegen.customizer.TemplatesManagerCustomizer.createTemplates"/>
                </file>
            </folder>
        </folder>
    </folder>

</filesystem>
