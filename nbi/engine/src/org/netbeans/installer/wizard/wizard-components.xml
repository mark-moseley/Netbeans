<?xml version="1.0" encoding="UTF-8"?>
<wizard xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="wizard-components.xsd">
    <component class="org.netbeans.installer.wizard.components.panels.TextPanel">
        <properties>
            <property name="text">$P{org.netbeans.installer.wizard.Bundle, WelcomePage.text}</property>
            <property name="content.type">$P{org.netbeans.installer.wizard.Bundle, WelcomePage.content.type}</property>
        </properties>
    </component>
    
    <component class="org.netbeans.installer.wizard.components.panels.ComponentsSelectionPanel"/>
    
    <component class="org.netbeans.installer.wizard.components.panels.PreInstallSummaryPanel"/>
    
    <component class="org.netbeans.installer.wizard.components.sequences.UninstallSequence"/>
    
    <component class="org.netbeans.installer.wizard.components.sequences.InstallSequence"/>
    
    <component class="org.netbeans.installer.wizard.components.panels.PostInstallSummaryPanel"/>
</wizard>
