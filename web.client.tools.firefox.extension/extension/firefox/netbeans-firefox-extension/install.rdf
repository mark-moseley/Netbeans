<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
  
    <em:id>netbeans-firefox-extension@netbeans.org</em:id>
    <em:name>netbeans-firefox-extension</em:name>
    <em:version>0.6</em:version>
    <em:description>NetBeans Firefox Extension</em:description>
    <em:creator>NetBeans</em:creator>
    <!-- optional items -->
    <em:homepageURL>http://www.netbeans.org</em:homepageURL>
    <em:aboutURL>chrome://netbeans-firefox-extension/content/netbeans-firefox-extension-about.xul</em:aboutURL>
    <em:iconURL>chrome://netbeans-firefox-extension/skin/netbeans32x32.png</em:iconURL>
    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.5</em:minVersion>
        <em:maxVersion>2.0.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:requires>
      <Description>
        <!-- Firebug -->
        <em:id>firebug@software.joehewitt.com</em:id>
        <em:minVersion>1.0.99</em:minVersion>
        <em:maxVersion>1.2.*</em:maxVersion>
        <em:homepageURL>http://www.getfirebug.com/</em:homepageURL>
        <em:updateURL>http://www.getfirebug.com/releases/update.rdf</em:updateURL>
      </Description>
    </em:requires>

  </Description>

</RDF>
