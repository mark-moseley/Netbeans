<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

  <folder name="EnvironmentProviders">
    <folder name="ProfileTypes">
       <folder name="Execution">
         <file name="nb-j2ee-deployment.instance">
            <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.deployment.impl.projects.J2eeProfileEnvironment"/>
            <attr name="instanceOf" stringvalue="org.netbeans.spi.projects.ProjectEnvironmentProvider"/>
         </file>
       </folder>
    </folder>
  </folder>
 
  <folder name="Templates">
    <folder name="ProfileTypes">
      <folder name="Execution">
        <file name="nb-j2ee-deployment.profile">
           <attr name="SystemFileSystem.localizingBundle"
                 stringvalue="org.netbeans.modules.j2ee.deployment.execution.Bundle"/>
                 <!-- PENDING do we need an icon here? -->
           <attr name="SystemFileSystem.icon"
                 urlvalue="nbres:/org/netbeans/modules/j2ee/deployment/impl/ui/resources/ServerRegistry.gif"/>
        </file>
      </folder>
    </folder>
  </folder>

  <folder name="Services">
<!-- Is this necessary?  Why do we need an Executor now? -->
    <folder name="Executor">
      <file name="org-netbeans-modules-j2ee-deployment-execution-ServerExecutor.settings" url="../execution/Executor.settings">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.j2ee.deployment.execution.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/core/resources/executionTypes.gif"/>
        <attr name="SystemFileSystem.layer" stringvalue="project"/>
      </file>
    </folder>
    
<!--    <file name="org-netbeans-modules-j2ee-deployment-ServerRegistry.settings" url="ServerRegistry.settings"/> -->
       
  </folder>

  <folder name="J2EE">
    <folder name="ApplicationProviders">
    </folder>
    <folder name="Jsr88Plugins">
    </folder>
    <folder name="InstalledServers">
    </folder>
  </folder>

  <folder name="UI">
    <folder name="Runtime">
       <file name="ServerRegistry2.instance">
<!--          <module name="org.netbeans.modules.j2eeserver/3" spec="1.0"/> -->
          <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.deployment.impl.ui.ServerRegistryNode"/>
       </file>
    </folder>
  </folder>

</filesystem>
