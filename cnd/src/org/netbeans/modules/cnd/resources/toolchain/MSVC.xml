<?xml version="1.0" encoding="UTF-8"?>
<toolchaindefinition xmlns="http://www.netbeans.org/ns/cnd-toolchain-definition/1">
    <toolchain name="MSVC" display="Microsoft Compiler Collection" family="MSVC" qmakespec="win32-msvc"/>
    <platforms stringvalue="windows"/>
    <drive_letter_prefix stringvalue="/"/>
    <base_folder regestry="hklm\software\Microsoft\VisualStudio\SxS\VC7"  pattern=".*REG_SZ(.*)" suffix="bin" path_patern=".*[\\/]VC7[\\/]bin[\\/]?$"/>
    <command_folder regestry="hklm\software\microsoft\windows\currentversion\uninstall\msys-1.0_is1" pattern="Inno Setup: App Path.*REG_SZ(.*)" suffix="bin" path_patern=".*[\\/]msys[\\/]1.0[\\/]bin[\\/]?$"/>
    <c>
        <compiler name="cl"/>
        <recognizer pattern=".*[\\/]VC7[\\/]bin[\\/]?$"/>
        <version flags=""/>
        <system_macros>
            <macro stringvalue="__STDC__"/>
            <macro stringvalue="_ATL_VER"/>
            <macro stringvalue="_CHAR_UNSIGNED" flags="-J"/>
            <macro stringvalue="_CHAR_UNSIGNED" flags="-J"/>
            <macro stringvalue="__CLR_VER"/>
            <macro stringvalue="__cplusplus_cli=200406" flags="-clr -clr:pure -clr:safe"/>
            <macro stringvalue="__COUNTER__"/>
            <macro stringvalue="_CPPRTTI" flags="-GR"/>
            <macro stringvalue="_CPPUNWIND" flags="-GX"/>
            <macro stringvalue="_DEBUG" flags="-LDd -MDd -MTd"/>
            <macro stringvalue="_DLL" flags="-MD -MDd"/>
            <macro stringvalue="__FUNCDNAME__" flags="-EP -P"/>
            <macro stringvalue="__FUNCSIG__" flags="-EP -P"/>
            <macro stringvalue="__FUNCTION__" flags="-EP -P"/>
            <macro stringvalue="_INTEGRAL_MAX_BITS=32"/>
            <macro stringvalue="_M_CEE" flags="-clr -clr:pure -clr:safe -clr:oldSyntax"/>
            <macro stringvalue="_M_CEE_PURE" flags="-clr:pure"/>
            <macro stringvalue="_M_CEE_SAFE" flags="-clr:safe"/>
            <macro stringvalue="_M_IX86=600"/>
            <macro stringvalue="_M_IX86_FP=0" flags="-arch"/>
            <macro stringvalue="_M_IX86_FP=1" flags="-arch:SSE"/>
            <macro stringvalue="_M_IX86_FP=2" flags="-arch:SSE2"/>
            <!--macro stringvalue="_M_X64"/-->
            <macro stringvalue="_MANAGED" flags="-clr"/>
            <macro stringvalue="_MFC_VER=0x0700" flags="-clr"/>
            <macro stringvalue="_MSC_BUILD=1"/>
            <macro stringvalue="_MSC_EXTENSIONS=1" flags="-Ze"/>
            <macro stringvalue="_MSC_FULL_VER=150020706"/>
            <macro stringvalue="_MSC_VER=1500"/>
            <macro stringvalue="__MSVC_RUNTIME_CHECKS" flags="-RTC"/>
            <macro stringvalue="_MT" flags="-MT -MTd"/>
            <macro stringvalue="_NATIVE_WCHAR_T_DEFINED" flags="-Zc:wchar_t"/>
            <macro stringvalue="_OPENMP" flags="-openmp"/>
            <macro stringvalue="_VC_NODEFAULTLIB" flags="-Zl"/>
            <macro stringvalue="_WCHAR_T_DEFINED" flags="-Zc:wchar_t"/>
            <macro stringvalue="_WIN32"/>
            <!--macro stringvalue="_WIN64"/-->
            <macro stringvalue="_Wp64" flags="-Wp64"/>
            <macro stringvalue="_Wp64" flags="-Wp64"/>
        </system_macros>
        <user_include flags="-I"/>
        <user_macro flags="-D"/>
        <development_mode>
            <fast_build flags=""/>
            <debug flags="" default="true"/>
            <performance_debug flags=""/>
            <test_coverage flags=""/>
            <diagnosable_release flags=""/>
            <release flags=""/>
            <performance_release flags=""/>
        </development_mode>
        <warning_level>
            <no_warnings flags=""/>
            <default flags="" default="true"/>
            <more_warnings flags=""/>
            <warning2error flags=""/>
        </warning_level>
        <architecture>
            <default flags="" default="true"/>
            <bits_32 flags=""/>
            <bits_64 flags=""/>
        </architecture>
        <strip flags="-s"/>
        <output_object_file flags="-Fo"/>
        <dependency_generation flags=""/>
        <precompiled_header flags="" suffix="" append="true"/>
    </c>
    <cpp>
        <compiler name="cl"/>
        <recognizer pattern=".*[\\/]VC7[\\/]bin[\\/]?$"/>
        <version flags=""/>
        <system_macros>
            <macro stringvalue="__STDC__"/>
            <macro stringvalue="__cplusplus"/>
            <macro stringvalue="_ATL_VER"/>
            <macro stringvalue="_CHAR_UNSIGNED" flags="-J"/>
            <macro stringvalue="_CHAR_UNSIGNED" flags="-J"/>
            <macro stringvalue="__CLR_VER"/>
            <macro stringvalue="__cplusplus_cli=200406" flags="-clr -clr:pure -clr:safe"/>
            <macro stringvalue="__COUNTER__"/>
            <macro stringvalue="_CPPRTTI" flags="-GR"/>
            <macro stringvalue="_CPPUNWIND" flags="-GX"/>
            <macro stringvalue="_DEBUG" flags="-LDd -MDd -MTd"/>
            <macro stringvalue="_DLL" flags="-MD -MDd"/>
            <macro stringvalue="__FUNCDNAME__" flags="-EP -P"/>
            <macro stringvalue="__FUNCSIG__" flags="-EP -P"/>
            <macro stringvalue="__FUNCTION__" flags="-EP -P"/>
            <macro stringvalue="_INTEGRAL_MAX_BITS=32"/>
            <macro stringvalue="_M_CEE" flags="-clr -clr:pure -clr:safe -clr:oldSyntax"/>
            <macro stringvalue="_M_CEE_PURE" flags="-clr:pure"/>
            <macro stringvalue="_M_CEE_SAFE" flags="-clr:safe"/>
            <macro stringvalue="_M_IX86=600"/>
            <macro stringvalue="_M_IX86_FP=0" flags="-arch"/>
            <macro stringvalue="_M_IX86_FP=1" flags="-arch:SSE"/>
            <macro stringvalue="_M_IX86_FP=2" flags="-arch:SSE2"/>
            <!--macro stringvalue="_M_X64"/-->
            <macro stringvalue="_MANAGED" flags="-clr"/>
            <macro stringvalue="_MFC_VER=0x0700" flags="-clr"/>
            <macro stringvalue="_MSC_BUILD=1"/>
            <macro stringvalue="_MSC_EXTENSIONS=1" flags="-Ze"/>
            <macro stringvalue="_MSC_FULL_VER=150020706"/>
            <macro stringvalue="_MSC_VER=1500"/>
            <macro stringvalue="__MSVC_RUNTIME_CHECKS" flags="-RTC"/>
            <macro stringvalue="_MT" flags="-MT -MTd"/>
            <macro stringvalue="_NATIVE_WCHAR_T_DEFINED" flags="-Zc:wchar_t"/>
            <macro stringvalue="_OPENMP" flags="-openmp"/>
            <macro stringvalue="_VC_NODEFAULTLIB" flags="-Zl"/>
            <macro stringvalue="_WCHAR_T_DEFINED" flags="-Zc:wchar_t"/>
            <macro stringvalue="_WIN32"/>
            <!--macro stringvalue="_WIN64"/-->
            <macro stringvalue="_Wp64" flags="-Wp64"/>
            <macro stringvalue="_Wp64" flags="-Wp64"/>
        </system_macros>
        <user_include flags="-I"/>
        <user_macro flags="-D"/>
        <development_mode>
            <fast_build flags="-Od"/>
            <debug flags="-Od" default="true"/>
            <performance_debug flags="-Ox"/>
            <test_coverage flags=""/>
            <diagnosable_release flags=""/>
            <release flags=""/>
            <performance_release flags=""/>
        </development_mode>
        <warning_level>
            <no_warnings flags="-W0"/>
            <default flags="-W3" default="true"/>
            <more_warnings flags="-W4"/>
            <warning2error flags="-W3 -WX"/>
        </warning_level>
        <architecture>
            <default flags="" default="true"/>
            <bits_32 flags=""/>
            <bits_64 flags=""/>
        </architecture>
        <strip flags=""/>
        <output_object_file flags="-Fo"/>
        <dependency_generation flags=""/>
        <precompiled_header flags="" suffix="" append="true"/>
    </cpp>
    <scanner>
        <error pattern="([a-zA-Z]:[^:\n]*|[^:\n]*):([^:\n]*):([^:\n]*):([^\n]*)" language="c,cpp,fortran,assembler"/>
        <error pattern="([^:\n]*):([0-9]+): ([a-zA-Z]*):*.*" language="c,cpp,fortran,assembler"/>
        <enter_directory pattern="[gd]?make\\[([0-9]+)\\]: Entering[\\w+\\s+]+`([^']*)'"/>
        <leave_directory pattern="[gd]?make\\[([0-9]+)\\]: Leaving[\\w+\\s+]+`([^']*)'"/>
        <change_directory pattern="cd\\s+([\\S]+)[\\s;]"/>
        <stack_header pattern="In file included from ([A-Z]:[^:\n]*|[^:\n]*):([^:^,]*)"/>
        <stack_next pattern="                 from ([A-Z]:[^:\n]*|[^:\n]*):([^:^,]*)"/>
    </scanner>
    <linker>
        <library_prefix stringvalue="lib"/>
        <library_search flags="-L"/>
        <dynamic_library_search flags=""/>
        <library_flag flags="-l"/>
        <PIC flags=""/>
        <static_library flags=""/>
        <dynamic_library flags=""/>
        <dynamic_library_basic flags="-LD "/>
        <output_file flags="-Fe"/>
    </linker>
    <make>
        <tool name="gmake,make" skip="true"/>
        <alternative_path>
            <path directory="$MSYS"/>
            <path toolchain_name="*"/>
            <path directory="$PATH"/>
        </alternative_path>
        <version flags="--version"/>
        <dependency_support code="include $(wildcard $(addsuffix .d, ${OBJECTFILES}))\n"/>
    </make>
    <debugger>
        <tool name="gdb"/>
        <alternative_path>
            <path directory="$MSYS"/>
            <path toolchain_name="*"/>
            <path directory="$PATH"/>
        </alternative_path>
        <version flags="--version"/>
    </debugger>
</toolchaindefinition>
