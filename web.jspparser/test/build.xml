<project name="web/jspparser module test build script" basedir="." default="all">

    <!-- Name of tested module -->
    <property name="xtest.module" value="web/jspparser"/>
    
    <!-- XTest screws up when setting this property, setting it correctly now -->
    <property name="nb_all" location="../../.."/>
    
    <!-- Home of XTest -->
    <property name="xtest.home" location="../../../xtest"/>

    <!-- default testtypes, attributes used when no value is supplied from command line -->
    <property name="xtest.testtype" value="unit"/>
    <property name="xtest.attribs" value="stable"/>

    <!-- Imports buildtests, cleantests, runtest, cleanresults, realclean, printconfig targets. -->
    <import file="${xtest.home}/lib/default-module-build.xml"/>    

</project>

