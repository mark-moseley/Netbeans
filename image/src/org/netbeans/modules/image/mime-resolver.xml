<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
        <ext name="gif"/>
        <ext name="GIF"/>
        <resolver mime="image/gif"/>
    </file>
    <file>
        <ext name="jpg"/>
        <ext name="JPG"/>
        <ext name="jpeg"/>
        <ext name="JPEG"/>
        <resolver mime="image/jpeg"/>
    </file>
    <file>
        <ext name="png"/>
        <ext name="PNG"/>
        <resolver mime="image/png"/>
    </file>
    <file>
        <ext name="bmp"/>
        <ext name="BMP"/>
        <resolver mime="image/bmp"/>
    </file>
</MIME-resolver>
