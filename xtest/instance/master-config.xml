<testconfig>

    <!-- This file is not used to add unit tests for daily runs. It is configured
         directly on Test4U or through binary tests distribution. See
         http://jupiter.czech.sun.com/wiki/view/PlatformQE/DailyTestRunTestSuite.
    -->
    
  <!-- PERFORMANCE -->
 
  <config name="qa-performance">
    <module name="performance"   testtypes="unit"  attributes="measure_ide"/>  
        <module name="performance"   testtypes="qa-functional"  attributes="measure_ui"/>  
  </config>
  
  <config name="qa-performance-ide">
    <module name="performance"   testtypes="unit"  attributes="measure_ide"/>  
  </config>
  
  <config name="qa-performance-ui">
        <module name="performance"   testtypes="qa-functional"  attributes="measure_ui"/>  
  </config>
  
  <config name="qa-web-performance">
    <module name="web"   testtypes="qa-performance"  attributes="measure_ui"/>  
    <module name="j2ee"   testtypes="qa-performance"  attributes="measure_all"/>  
    <module name="performance"   testtypes="qa-functional"  attributes="measure_setup"/>  
  </config>
  

  <!-- Validation tests configurations. Used in NetBeans continuous build process.
       See http://wiki.netbeans.org/wiki/view/UICommitValidation.
  -->
  <config name="commit-validation-nb">
      <module name="o.n.core" testtypes="unit" attributes="commit"/>
      <module name="core.windows" testtypes="unit" attributes="commit" />
      <module name="javahelp" testtypes="unit" attributes="commit" />
      <module name="ide.kit" testtypes="qa-functional" attributes="commit"/>
      <module name="editor" testtypes="qa-functional" attributes="commit" />
  </config>
  
  <config name="commit-validation-enterprise">
      <!--module name="enterprise/bpel" testtypes="qa-functional" attributes="commit"/-->
      <module name="soa.kit" testtypes="qa-functional" attributes="commit"/>
      <module name="xml.schema" testtypes="qa-functional" attributes="commit"/>
  </config>

  <config name="commit-validation-cnd">
      <module name="ide.kit" testtypes="qa-functional" attributes="cnd"/>
  </config>
  
  <config name="commit-validation-profiler">
      <module name="ide.kit" testtypes="qa-functional" attributes="profiler"/>
  </config>
  
  <config name="commit-validation-j2ee">
      <module name="ide.kit" testtypes="qa-functional" attributes="j2ee"/>
  </config>

  <config name="commit-validation-mobility">
      <module name="mobility.project" testtypes="qa-functional" attributes="commit">
          <!-- Propagate wtk.dir property to build-qa-functional.xml. -->
          <property name="wtk.dir" value="${wtk.dir}"/>
      </module>
  </config>

  <config name="commit-validation-uml">
      <module name="uml.kit" testtypes="qa-functional" attributes="commit"/>
  </config>

  <config name="commit-validation-ruby">
      <module name="ide.kit" testtypes="qa-functional" attributes="ruby"/>
  </config>
  
  <!-- All commit configs in one. You can run them using:
       ant -f nbbuild/build.xml commit-validation
           -D"xtest.userdata|com.sun.aas.installRoot"=<glassfish install dir>
           -Dwtk.dir=<path to wtk22_win.zip or wtk22_linux.zip>
           -Dxtest.config=commit-validation-all 
  -->
  <config name="commit-validation-all">
      <module name="o.n.core" testtypes="unit" attributes="commit"/>
      <module name="core.windows" testtypes="unit" attributes="commit" />
      <module name="javahelp" testtypes="unit" attributes="commit" />
      <module name="editor" testtypes="qa-functional" attributes="commit" />
      <!--module name="enterprise/bpel" testtypes="qa-functional" attributes="commit"/-->
      <!--<module name="ide.kit" testtypes="qa-functional" attributes="cnd,commit,j2ee,profiler,ruby"/>-->
      <module name="ide.kit" testtypes="qa-functional" attributes="cnd,commit,profiler,ruby"/>
      <module name="mobility.project" testtypes="qa-functional" attributes="commit">
          <!-- Propagate wtk.dir property to build-qa-functional.xml. -->
          <property name="wtk.dir" value="${wtk.dir}"/>
      </module>
      <module name="soa.kit" testtypes="qa-functional" attributes="commit"/>
      <module name="uml.kit" testtypes="qa-functional" attributes="commit"/>
      <module name="xml.schema" testtypes="qa-functional" attributes="commit"/>
  </config>  
</testconfig>
