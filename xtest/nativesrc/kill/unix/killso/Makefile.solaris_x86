# change to your location of JDK
JAVA_HOME=/usr/local/jdk1.3.1_01

# you might need to change 'sparc' to your architecture when
# compiling on non sparc machines
# you can get your architecture from java system property 'os.arch'
LIBRARY_NAME=lib.jnikill.solaris.x86.so


# you should not change anything below this line

INCLUDES=-I$(JAVA_HOME)/include/ -I$(JAVA_HOME)/include/solaris/
CC=cc
CFLAGS=

# fi


killer: org_netbeans_xtest_util_JNIKill.o
	$(CC) $(CFLAGS) -G  org_netbeans_xtest_util_JNIKill.o -o $(LIBRARY_NAME)

org_netbeans_xtest_util_JNIKill.o: \
	org_netbeans_xtest_util_JNIKill.c \
	org_netbeans_xtest_util_JNIKill.h

	$(CC) $(INCLUDES) $(CFLAGS) -c org_netbeans_xtest_util_JNIKill.c

clean:
	rm org_netbeans_xtest_util_JNIKill.o
	rm $(LIBRARY_NAME)
