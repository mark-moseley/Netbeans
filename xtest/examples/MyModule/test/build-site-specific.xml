<project name="MyModule site specific tests compilers and executors" basedir="." default="all">
    
    <!-- ========= -->
    <!-- Compilers -->
    <!-- ========= -->    
    <target name="site-specific-compiler">
        <buildTests srcDir="site-specific/src"/>
    </target>
    
    <!-- ========= -->
    <!-- Executors -->
    <!-- ========= -->    
    <target name="runcodetest">
        <executeTests pluginName="code" classpath="{jdbc.drivers.jars}"/>
    </target>

</project>

