<XTestPlugin name="base" version="1.0">
    <Dependencies requiredXTestVersion="1.5">
    </Dependencies>

    <AvailableCompilers>
        <Compiler id="compiler" antfile="lib/basic_compiler_targets.xml" target="compile" default="true"/>
    </AvailableCompilers>    

    <AvailablePackagers>
        <Packager id="packager" antfile="lib/basic_packager_targets.xml" target="package" default="true"/>
    </AvailablePackagers>
    
    <AvailableResultProcessors>
        <ResultProcessor id="result_processor" antfile="lib/basic_result_processor_targets.xml" target="report" default="true"/>
    </AvailableResultProcessors>    
</XTestPlugin>
