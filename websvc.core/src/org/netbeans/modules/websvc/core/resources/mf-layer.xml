<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="jax-ws">
        <file name="default-binding-handler.xml" url="default-binding-handler.xml"/>
    </folder>
    <folder name="Templates">
        <attr name="Classes/WebServices" boolvalue="true"/>
        <folder name="WebServices">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.websvc.core.client.wizard.Bundle"/>
            <attr name="WebServiceClient/MessageHandler" boolvalue="true"/>
            <file name="WebServiceClient">
                <attr name="template" boolvalue="true"/>
                <attr name="instantiatingIterator"
                    methodvalue="org.netbeans.modules.websvc.core.client.wizard.WebServiceClientWizardIterator.create" />
                <attr name="templateCategory" stringvalue="web-service-clients"/>
                <attr name="templateWizardURL"
                    urlvalue="nbresloc:/org/netbeans/modules/websvc/core/client/resources/WebServiceClient.html"/>
                <attr name="SystemFileSystem.localizingBundle"
                    stringvalue="org.netbeans.modules.websvc.core.client.wizard.Bundle"/>
                <attr name="SystemFileSystem.icon"
                    urlvalue="nbresloc:/org/netbeans/modules/websvc/core/client/resources/WebServiceClientTemplate.png"/>
            </file>
            <attr name="MessageHandler/WSDL.wsdl" boolvalue="true"/>
            <file name="WSDL.wsdl" url = "WSDL.template">
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardIterator" 
                    newvalue="org.netbeans.modules.websvc.wsdl.wizard.WsdlIterator"/>
                <attr name="templateWizardURL"
                    urlvalue="nbresloc:/org/netbeans/modules/websvc/wsdl/resources/WSDL.html"/>
                <attr name="SystemFileSystem.localizingBundle"
                    stringvalue="org.netbeans.modules.websvc.wsdl.wizard.Bundle"/>
                <attr name="SystemFileSystem.icon"
                    urlvalue="nbresloc:/org/netbeans/modules/websvc/wsdl/resources/wsdl16.gif"/>
                <attr name="templateCategory" stringvalue="wsdl"/>
            </file>
            <file name="WSDL_import.wsdl" url="WSDL_import.template"/>
        </folder>
        
       <folder name="WebServices">
         <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.websvc.core.dev.wizard.Bundle"/>
         <file name="WebService.java" url="nbres:/org/netbeans/modules/websvc/core/dev/resources/WebService.template">         
             <attr name="template" boolvalue="true"/>
             <attr name="instantiatingIterator" methodvalue="org.netbeans.modules.websvc.core.dev.wizard.NewWebServiceWizardIterator.create" />
             <attr name="templateCategory" stringvalue="web-services"/>
             <attr name="templateWizardURL"
                          urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/WebService.html"/>
             <attr name="SystemFileSystem.localizingBundle"
                          stringvalue="org.netbeans.modules.websvc.core.dev.wizard.Bundle"/>
             <attr name="SystemFileSystem.icon"
                urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/XMLServiceDataIcon.png"/> 
         </file>
          <!--  EJB Web Service --> 
          <file name="EjbWebService.java" url="EjbWebService.template"/>

         <file name="WebServiceFromWSDL.java">
             <attr name="template" boolvalue="true"/>
             <attr name="instantiatingIterator" methodvalue="org.netbeans.modules.websvc.core.dev.wizard.NewWebServiceFromWSDLWizardIterator.create" />
             <attr name="templateCategory" stringvalue="web-services"/>
             <attr name="templateWizardURL"
                          urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/WebServiceFromWSDL.html"/>
             <attr name="SystemFileSystem.localizingBundle"
                          stringvalue="org.netbeans.modules.websvc.core.dev.wizard.Bundle"/>
             <attr name="SystemFileSystem.icon"
                urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/XMLServiceDataIcon.png"/> 
         </file>

         <attr name="WebService/MessageHandler" boolvalue="true"/>
         <file name="MessageHandler.java" url="nbres:/org/netbeans/modules/websvc/core/dev/resources/MessageHandler.template">         
             <attr name="template" boolvalue="true"/>
             <attr name="instantiatingIterator" methodvalue="org.netbeans.modules.websvc.core.dev.wizard.MessageHandlerWizard.create" />
             <attr name="templateCategory" stringvalue="web-services"/>
             <attr name="templateWizardURL"
                          urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/MessageHandler.html"/>
             <attr name="SystemFileSystem.localizingBundle"
                          stringvalue="org.netbeans.modules.websvc.core.dev.wizard.Bundle"/>
             <attr name="SystemFileSystem.icon"
                urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/XMLServiceDataIcon.png"/> 
         </file>

         <attr name="WebService/LogicalHandler" boolvalue="true"/>
         <file name="LogicalHandler.java" url="nbres:/org/netbeans/modules/websvc/core/dev/resources/LogicalHandler.template">         
             <attr name="template" boolvalue="true"/>
             <attr name="instantiatingIterator" methodvalue="org.netbeans.modules.websvc.core.dev.wizard.LogicalHandlerWizard.create" />
             <attr name="templateCategory" stringvalue="web-services"/>
             <attr name="templateWizardURL"
                          urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/LogicalHandler.html"/>
             <attr name="SystemFileSystem.localizingBundle"
                          stringvalue="org.netbeans.modules.websvc.core.dev.wizard.Bundle"/>
             <attr name="SystemFileSystem.icon"
                urlvalue="nbresloc:/org/netbeans/modules/websvc/core/dev/resources/XMLServiceDataIcon.png"/> 
         </file>
       </folder><!-- Web Services -->
    </folder>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-java">
                <folder name="CompletionProviders">
                    <file name="org-netbeans-modules-websvc-editor-completion-WSCompletionProvider.instance"/>
                </folder>
            </folder>
        </folder>
    </folder>

    <folder name="Plugins">
        <!-- Register Web Service module catalog plugin -->
        <folder name="XML">
            <folder name="UserCatalogs">
                <file name="org-netbeans-modules-websvc-wsdl-WSSchemaCatalog.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.websvc.wsdl.WSSchemaCatalog"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.xml.catalog.spi.CatalogReader"/>
                </file>
            </folder>
            <folder name="GrammarQueryManagers">
                <!-- register semantics grammar for wscompile-config (1.1) due to code completion -->
                <file name="org-netbeans-modules-websvc-wsdl-WSSchemaGrammarQueryMgr.instance"/>
                <attr name="org-netbeans-modules-websvc-wsdl-WSSchemaGrammarQueryMgr.instance/semantics-grammar-to-generic-gramseparator" boolvalue="true"/>
            </folder> <!-- GrammarQueryManagers -->
        </folder>
    </folder>
    <!--
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="org-netbeans-modules-websvc-core-resources-mime-resolver.xml" url="mime-resolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.websvc.core.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/websvc/core/resources/WebServiceSettings.png"/>
            </file>
        </folder>
    </folder>
    -->
</filesystem>
