<?xml version="1.0" encoding="UTF-8"?>
<project name="_jaxb" default="default" basedir="..">
<import file="build-impl.xml"/>
<target name="xjc-typedef-target" depends="-init-project">
<typedef classname="com.sun.tools.xjc.XJC2Task" name="xjc" xmlns:s="http://xml.netbeans.org/schema/JAXBWizConfig">
<classpath path="${libs.jaxb20.classpath}"/>
</typedef>
</target>
<target name="jaxb-code-generation" depends="xjc-typedef-target">
<mkdir dir="build\generated\jaxb" xmlns:s="http://xml.netbeans.org/schema/JAXBWizConfig"/>
<mkdir dir="build\classes" xmlns:s="http://xml.netbeans.org/schema/JAXBWizConfig"/>
<xjc package="ABC" destdir="build\generated\jaxb">
<schema file="src\META-INF\jaxb\XmlQosPmUtilSchema.xsd"/>
<arg value="-xmlschema"/><arg value="quite"/>
</xjc>
<xjc package="ggg" destdir="build\generated\jaxb">
<schema file="src\META-INF\jaxb\XmlQosPmUtilSchema.xsd"/>
<schema file="src\META-INF\jaxb\XmlVodafoneTroubleTicketSchema.xsd"/>
<arg value="-xmlschema"/><arg value="extension"/>
</xjc>
<javac destdir="build\classes" srcdir="build\generated\jaxb" xmlns:s="http://xml.netbeans.org/schema/JAXBWizConfig">
<classpath path="${libs.jaxb20.classpath}"/>
</javac>
</target>
</project>
