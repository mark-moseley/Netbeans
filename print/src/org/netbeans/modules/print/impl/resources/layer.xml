<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
    <!-- # 100900
    <folder name="Toolbars">
        <folder name="File">
            <file name="org-netbeans-modules-print-impl-action-PreviewAction.instance"/>
            <attr name="org-openide-actions-SaveAllAction.instance/org-netbeans-modules-print-impl-action-PreviewAction.instance" boolvalue="true"/>
        </folder>
    </folder>
    -->

    <folder name="Actions">
        <folder name="File">
            <file name="org-netbeans-modules-print-impl-action-PageSetupAction.instance"/>
            <file name="org-netbeans-modules-print-impl-action-PrintAction.instance"/>
            <file name="org-netbeans-modules-print-impl-action-PreviewAction.instance"/>
        </folder>
    </folder>
    
    <folder name="Menu">
        <folder name="File">
            <file name="org-netbeans-modules-print-impl-action-PageSetupAction.shadow">
                <attr name="originalFile" stringvalue="Actions/File/org-netbeans-modules-print-impl-action-PageSetupAction.instance"/>
            </file>
            
            <file name="org-netbeans-modules-print-impl-action-PrintAction.shadow">
                <attr name="originalFile" stringvalue="Actions/File/org-netbeans-modules-print-impl-action-PrintAction.instance"/>
            </file>

            <file name="org-netbeans-modules-print-impl-action-PreviewAction.shadow">
                <attr name="originalFile" stringvalue="Actions/File/org-netbeans-modules-print-impl-action-PreviewAction.instance"/>
            </file>

            <!-- sorting -->
            <attr name="org-openide-actions-SaveAllAction.instance/org-netbeans-modules-print-impl-action-PageSetupAction.shadow" boolvalue="true"/>
            <attr name="org-netbeans-modules-print-impl-action-PageSetupAction.shadow/org-netbeans-modules-print-impl-action-PrintAction.shadow" boolvalue="true"/>
            <attr name="org-netbeans-modules-print-impl-action-PrintAction.shadow/org-netbeans-modules-print-impl-action-PreviewAction.shadow" boolvalue="true"/>
            <attr name="org-netbeans-modules-print-impl-action-PreviewAction.shadow/org-netbeans-modules-editor-ExportHtmlAction.instance" boolvalue="true"/>

            <!-- hide -->
            <file name="org-openide-actions-PageSetupAction.instance_hidden"/>
            <file name="org-openide-actions-PrintAction.instance_hidden"/>
        </folder>
    </folder>

    <folder name="Keymaps">
        <folder name="NetBeans">
            <file name="D-P.shadow_hidden"/>
        </folder>
    </folder>
    
    <folder name="Shortcuts">
        <file name="D-P.shadow"> <!-- Ctrl+P -->
            <attr name="originalFile" stringvalue="Actions/File/org-netbeans-modules-print-impl-action-PrintAction.instance"/>
        </file>
        <file name="OS-P.shadow"> <!-- Alt+Shift+P -->
            <attr name="originalFile" stringvalue="Actions/File/org-netbeans-modules-print-impl-action-PreviewAction.instance"/>
        </file>
    </folder>
</filesystem>
