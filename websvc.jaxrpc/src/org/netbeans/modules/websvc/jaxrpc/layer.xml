<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="org-netbeans-modules-websvc-jaxrpc-mime-resolver.xml" url="mime-resolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.websvc.jaxrpc.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/websvc/jaxrpc/WebServiceSettings.png"/>
            </file>
        </folder>
    </folder>
    
    <folder name="org-netbeans-modules-websvc-jaxrpc">
        <file name="webservices.xml" url="webservices.xml"/>
    </folder>
    
    <folder name="Projects">
        <!-- Web Service References node in J2SE Project -->
        <folder name="org-netbeans-modules-java-j2seproject">
            <folder name="Nodes">
                <attr name="org-netbeans-modules-java-j2seproject-ui-LibrariesNodeFactory.instance/org-netbeans-modules-websvc-jaxrpc-nodes-JaxRpcNodeFactory.instance" boolvalue="true"/>
                <file name="org-netbeans-modules-websvc-jaxrpc-nodes-JaxRpcNodeFactory.instance"/>
            </folder>
            <folder name="Customizer">
                <attr name="Application/Client.instance" boolvalue="true"/>
                <file name="Client.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.websvc.jaxrpc.project.customizer.JaxRpcCompositePanelProvider.createJaxRpcClients"/>
                </file>
            </folder>
        </folder>
        <folder name="org-netbeans-modules-web-project">
            <folder name="Nodes">
                <attr name="org-netbeans-modules-web-project-ui-DocBaseNodeFactory.instance/org-netbeans-modules-websvc-jaxrpc-nodes-JaxRpcWebServiceNodeFactory.instance" boolvalue="true"/>
                <file name="org-netbeans-modules-websvc-jaxrpc-nodes-JaxRpcWebServiceNodeFactory.instance"/>
                <attr name="org-netbeans-modules-websvc-jaxrpc-nodes-JaxRpcWebServiceNodeFactory.instance/org-netbeans-modules-web-project-ui-ConfFilesNodeFactory.instance" boolvalue="true"/>
            </folder>
        </folder>
    </folder>

</filesystem>
