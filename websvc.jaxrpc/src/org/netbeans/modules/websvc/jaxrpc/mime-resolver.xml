<!--
    Document   : config-mime-resolver.xml
    Created on : November 11, 2004, 2:30 PM
    Author     : peterw99
    Description:
        Mime-resolver for namespace for JAXRPC 1.0 config files so we can add
		it to the NetBeans XML document type registry.
-->

<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">

<MIME-resolver>
	<file>
		<ext name="xml"/>
		<resolver mime="text/jaxrpc-config-1-1">
			<xml-rule>
				<element name="configuration" ns="http://java.sun.com/xml/ns/jax-rpc/ri/config"/>
			</xml-rule>
		</resolver>
	</file>
	<file>
		<ext name="wsdl"/>
		<resolver mime="text/x-wsdl+xml"/>
	</file>
</MIME-resolver> 
