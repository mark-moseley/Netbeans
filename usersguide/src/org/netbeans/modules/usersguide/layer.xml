<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Menu">
        <folder name="Help">
            <!-- Replace normal Contents item with our own which shows UG home page. -->
            <file name="master-help.xml_hidden"/>
            <file name="org-netbeans-modules-usersguide-master.xml" url="master.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/usersguide/help.gif"/>
            </file>
            <attr name="org-netbeans-modules-usersguide-master.xml/HelpShortcuts" boolvalue="true"/>
            <folder name="HelpShortcuts">
                <!-- Shortcut to main page (shows only UG help set). -->
                <file name="org-netbeans-modules-usersguide-mainpage.xml" url="mainpage.xml">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/usersguide/helpset.gif"/>
                </file>
                <!-- Also a separator after that. -->
                <attr name="org-netbeans-modules-usersguide-mainpage.xml/org-netbeans-modules-usersguide-sep.instance" boolvalue="true"/>
                <file name="org-netbeans-modules-usersguide-sep.instance">
                    <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                </file>
                <!-- And another below, for "third-party" helpsets (e.g. Ant's): -->
                <attr name="org-netbeans-modules-usersguide-sep.instance/org-netbeans-modules-usersguide-lower-sep.instance" boolvalue="true"/>
                <file name="org-netbeans-modules-usersguide-lower-sep.instance">
                    <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name="Help">
            <file name="org-netbeans-modules-usersguide-mainpage.xml" url="mainpage.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/usersguide/helpset.gif"/>
            </file>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="JavaHelp">
            <!-- Now the help set. -->
            <file name="org-netbeans-modules-usersguide-helpset.xml" url="helpset.xml"/>
            <attr name="org-netbeans-modules-usersguide-helpset.xml/org-netbeans-modules-usersguide-above-regular.txt" boolvalue="true"/>
            <!-- Only a marker: above regular helpsets, below IDE help. -->
            <file name="org-netbeans-modules-usersguide-above-regular.txt"/>
            <attr name="org-netbeans-modules-usersguide-above-regular.txt/org-netbeans-modules-usersguide-below-regular.txt" boolvalue="true"/>
            <!-- Only a marker: below regular helpsets, above third-party helpsets. -->
            <file name="org-netbeans-modules-usersguide-below-regular.txt"/>
        </folder>
    </folder>
    <!-- #13860: mount the examples dir: -->
    <folder name="Mount">
        <file name="org-netbeans-modules-usersguide-sampledir.settings" url="sampledir.xml">
            <!-- #14403: make it look better: -->
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/openide/resources/localFS.gif"/>
        </file>
        <file name="org-netbeans-modules-usersguide-timerbean.settings" url="timerbean.xml">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/openide/resources/localFS.gif"/>
        </file>
    </folder>
    <folder name="Palette">
        <folder name="Beans">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
            <file name="org-netbeans-examples-lib-timerbean-Timer.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.usersguide.Bundle"/>
            </file>
        </folder>
    </folder>
    
