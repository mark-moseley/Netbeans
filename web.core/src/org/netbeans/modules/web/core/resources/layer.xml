<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

  <folder name="Templates">
    <attr name="Beans/JSP_Servlet" boolvalue="true"/>
    <folder name="JSP_Servlet">
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/context/resources/WebModule.html" />
        <attr name="SystemFileSystem.localizingBundle"
	      stringvalue="org.netbeans.modules.web.core.Bundle"/>

      <attr name="webapp.warImage/JSPs" boolvalue="true"/>
      <attr name="JSPs/Servlet.java" boolvalue="true"/>
      <attr name="Servlet.java/Filters" boolvalue="true"/>
      <attr name="Filters/Listeners" boolvalue="true"/>	    
      <attr name="Listeners/Html.html" boolvalue="true"/>	    
      <attr name="Listeners/TLD.tld" boolvalue="true"/>	    

      <folder name="JSPs">
        <attr name="JSP.jsp/JSPX.jspx" boolvalue="true"/>
        <attr name="JSPX.jspx/JSPF.jspf" boolvalue="true"/>
	<attr name="templateWizardURL" 
              urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/JSPfolder.html"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
	<!-- Basic JSP --> 
        <file name="JSP.jsp" url="templates/JSP.template">
          <attr name="template" boolvalue="true"/>
	  <attr name="templateWizardURL" 
                urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/JSP.html"/>
	  <attr name="SystemFileSystem.localizingBundle"
		      stringvalue="org.netbeans.modules.web.core.Bundle"/>
<!--
	  <attr name="templateWizardIterator" 
                methodvalue="org.netbeans.modules.web.dd.wizards.ServletIterator.createJspIterator"/>
-->
        </file>

	<!--  JSP Document --> 
        <file name="JSPX.jspx" url="templates/JSPDoc.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" 
                urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/JSPDoc.html"/>
          <attr name="SystemFileSystem.localizingBundle" 
                stringvalue="org.netbeans.modules.web.core.Bundle"/>
<!--
          <attr name="templateWizardIterator" 
                methodvalue="org.netbeans.modules.web.dd.wizards.ServletIterator.createJspDocIterator"/>
-->
        </file>

        <!--  JSP Segment --> 
        <file name="JSPF.jspf" url="templates/JSPF.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" 
                urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/JSPF.html"/>
          <attr name="SystemFileSystem.localizingBundle"
                stringvalue="org.netbeans.modules.web.core.Bundle"/>
<!--
          <attr name="templateWizardIterator" 
                methodvalue="org.netbeans.modules.web.dd.wizards.ServletIterator.createJspfIterator"/>
-->
        </file>
      </folder> 
     
      <!-- Servlet --> 
      <file name="Servlet.java" url="templates/Servlet.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/Servlet.html"/>
          <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.web.dd.wizards.ServletIterator.createServletIterator"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
          <attr name="org.netbeans.modules.web.IsServletFile" boolvalue="true"/>
      </file>
      <!-- use template description from HTML module -->
      <file name="Html.html" url="nbresloc:/org/netbeans/modules/html/templates/html.html">
         <attr name="template" boolvalue="true" />
         <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/html/TemplateHelp.html" />
         <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
      </file>
      <!-- Servlet Listener --> 
      <file name="ServletListener.java" url="templates/ServletListener.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/ServletListener.html"/>
          <attr name="templateWizardIterator" newvalue="org.netbeans.modules.web.dd.wizards.ListenerIterator"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
      </file>
      <attr name="webapp.warImage/JSP.jsp" boolvalue="true"/>
      <attr name="webappSample.warImage/JSP.jsp" boolvalue="true"/>
      <attr name="JSP.jsp/Servlet.java" boolvalue="true"/>
      <attr name="Servlet.java/Html.html" boolvalue="true"/>
      
      <!-- Filters -->
      <folder name="Filters">
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/Filters.html" />
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
        
        <file name="SimpleFilter.java" url="templates/SimpleFilter.template">
            <attr name="template" boolvalue="true"/>
            <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/SimpleFilter.html"/>
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
            <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.web.dd.wizards.ServletIterator.createFilterIterator"/>
        </file>        
        <file name="AdvancedFilter.java" url="templates/AdvancedFilter.template">
            <attr name="template" boolvalue="true"/>
            <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/AdvancedFilter.html"/>
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
            <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.web.dd.wizards.ServletIterator.createFilterIterator"/>
        </file>        
      </folder>
    </folder>
  </folder>

  <folder name="Actions">
    <folder name="Build">
      <file name="Set Request Parameters.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.EditQueryStringAction"/>
      </file>
      <file name="Execute (restart server).instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.execution.Execute2Action"/>
      </file>
    </folder>
    <folder name="System">
      <file name="View Servlet.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.jsploader.EditServletAction"/>
      </file>
    </folder>
  </folder>

  <folder name="Services">
    <folder name="Hidden">
	    <attr name="hidden" boolvalue="true" />
	    <file name="org-netbeans-modules-web-core-resources-EntityCatalog.xml" url="entityCatalog.xml"/>
    </folder>

    <folder name="JSP_Servlet">
      <file name="org-netbeans-modules-web-core-ServletSettings.settings" url="servlet.settings">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle" />
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/servletSettings.gif" />
        <attr name="SystemFileSystem.layer" stringvalue="project" />
      </file>
    </folder>

    <!-- Recognize text/x-jsp -->
    <folder name="MIMEResolver">
      <file name="org-netbeans-modules-web-core-resources-mime-resolver.xml" url="mime-resolver.xml">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle" />
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/servletSettings.gif" />
      </file>
    </folder>

  </folder>

  <folder name="J2EE">
    <folder name="JSPParser">
    </folder>
    <folder name="JSPCompilation">
    </folder>
    <folder name="JSPSyntaxColoring">
        <file name="org-netbeans-modules-web-core-jsploader-JspContextInfoImpl.instance">
            <attr name="instanceCreate" newvalue="org.netbeans.modules.web.core.jsploader.JspContextInfoImpl" />
        </file>
    </folder>
  </folder>
  
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="XMLJ2eeAnnotation.xml" url="XMLJ2eeAnnotation.xml"/>
      </folder>
    </folder>
    
  <folder name="Shortcuts">
    <file name="C-Q.instance">
      <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.EditQueryStringAction"/>
    </file>
  </folder>      

  <folder name="Web">
    <folder name="MonitorSupport">
    </folder>
  </folder>
  
    <folder name="Plugins">
      <!-- Register XML module catalog plugin -->
        <folder name="XML">
            <folder name="UserCatalogs">
                <file name="org-netbeans-modules-web-dd-DDCatalogRegistration.instance">
                    <attr name="instanceCreate" 
                        methodvalue="org.netbeans.modules.web.dd.DDDataLoader.getDDCatalog"/>
                    <attr name="instanceOf" 
                        stringvalue="org.netbeans.modules.xml.catalog.spi.CatalogReader"/>
                </file>
            </folder>
            <folder name="GrammarQueryManagers">
                <!-- register semantics grammar for web.xml(2.4) due to code completion -->
                <file name="org-netbeans-modules-web-dd-DDGrammarQueryManager.instance"/>
                <attr name="org-netbeans-modules-web-dd-DDGrammarQueryManager.instance/semantics-grammar-to-generic-grammar-separator" boolvalue="true"/>
            </folder> <!-- GrammarQueryManagers -->
        </folder>
    </folder>

</filesystem>
