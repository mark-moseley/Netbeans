<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

  <folder name="Templates">
    <attr name="JarPackager/JSP_Servlet" boolvalue="true"/>
    <folder name="JSP_Servlet">
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/Servlet_JSP.html" />
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
      <file name="webapp.warImage" url="templates/WebModule.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/context/resources/WebModule.html"/>
          <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.web.context.WebAppWizardIterator.singleton"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.context.Bundle"/>
      </file>
      <file name="JSP.jsp" url="templates/JSP.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/JSP.html"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
      </file>
      <file name="Servlet.java" url="templates/Servlet.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/Servlet.html"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
          <attr name="org.netbeans.modules.web.IsServletFile" boolvalue="true"/>
          <attr name="NetBeansAttrDebuggerType" methodvalue="org.netbeans.modules.j2ee.impl.ServerExecSupport.getServerDebuggerType"/>
          <attr name="NetBeansAttrExecutor" methodvalue="org.netbeans.modules.j2ee.impl.ServerExecSupport.getServerExecutor"/>
      </file>
      <!-- use template description from HTML module -->
      <file name="Html.html" url="nbresloc:/org/netbeans/modules/html/templates/html.html">
         <attr name="template" boolvalue="true" />
         <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/html/TemplateHelp.html" />
         <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.html.Bundle"/>
      </file>
      <attr name="webapp.warImage/JSP.jsp" boolvalue="true"/>
      <attr name="JSP.jsp/Servlet.java" boolvalue="true"/>
      <attr name="Servlet.java/Html.html" boolvalue="true"/>
    </folder>
  </folder>

  <folder name="Actions">
    <folder name="Build">
      <file name="Set Request Parameters.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.EditQueryStringAction"/>
      </file>
      <file name="Execute (restart server).instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.execution.Execute2Action"/>
      </file>
    </folder>
    <folder name="System">
      <file name="View Servlet.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.jsploader.EditServletAction"/>
      </file>
    </folder>
    <folder name="Tools">
      <file name="Convert to Servlet....instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.jsploader.ConvertToServletAction"/>
      </file>
      <file name="Convert to Java....instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.jsploader.ConvertToJavaAction"/>
      </file>
    </folder>
  </folder>

  <folder name="Mount">
      <folder name="java">
          <file name="org-netbeans-modules-web-core-resources-library-servlet.xml">
              <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle" />
              <![CDATA[
                  <!DOCTYPE Library PUBLIC "-//NetBeans IDE//DTD JavaLibrary//EN" "http://www.netbeans.org/dtds/JavaLibrary-1_0.dtd">
                  <Library>
                      <Archive name="modules/ext/servlet-2.3.jar"/>
                  </Library>
              ]]>
          </file>
      </folder>
  </folder>
  <folder name="Services">
    <folder name="Hidden">
	    <attr name="hidden" boolvalue="true" />
	    <file name="org-netbeans-modules-web-core-resources-EntityCatalog.xml" url="entityCatalog.xml"/>
    </folder>

    <folder name="Looks">
        <!-- JSPDefaultLook class defines Basic look -->
        <file name="org.openidex.nodes.looks.NodeProxyLook$Web-Basic.xml">
            <![CDATA[
                <!DOCTYPE Look PUBLIC "-//NetBeans IDE//DTD Looks//EN" "http://www.netbeans.org/dtds/Looks-1_0.dtd" >
                <Look name="JSP/Servlet Basic">
                </Look>
            ]]>
        </file>
        <!-- JSPDefaultLook class defines composite look for servlet, JSP and DD objects 
             it merges available (sub)set of sublooks -->
        <file name="org.openidex.nodes.looks.CompositeLook$Web-Look.xml" url="web-look.xml"/>
    </folder> <!-- Looks -->


    <folder name="JSP_Servlet">
      <file name="org-netbeans-modules-web-core-ServletSettings.settings">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle" />
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/servletSettings.gif" />
        <attr name="SystemFileSystem.layer" stringvalue="project" />
        <![CDATA[<?xml version="1.0"?>
        <!DOCTYPE settings PUBLIC "-//NetBeans//DTD Session settings 1.0//EN" "http://www.netbeans.org/dtds/sessionsettings-1_0.dtd">
            <settings version="1.0">
                <module name="org.netbeans.modules.web.core" spec="1.5"/>
                <instanceof class="java.io.Externalizable"/>
                <instanceof class="org.openide.util.SharedClassObject"/>
                <instanceof class="org.netbeans.modules.web.core.ServletSupportModule$Settings"/>
                <instanceof class="org.netbeans.modules.web.core.WebBrowser$Factory"/>
                <instanceof class="org.netbeans.modules.web.core.ServletSettings"/>
                <instanceof class="org.openide.options.SystemOption"/>
                <instanceof class="java.io.Serializable"/>
                <instanceof class="java.lang.Object"/>
                <instance class="org.netbeans.modules.web.core.ServletSettings"/>
            </settings>
        ]]>
      </file>
    </folder>

    <!-- Recognize text/x-jsp -->
    <folder name="MIMEResolver">
      <file name="org-netbeans-modules-web-core-resources-mime-resolver.xml" url="mime-resolver.xml">
      </file>
    </folder>

  </folder>

    <folder name="UI">
        <folder name="Services">
            <folder name="Building">
                <file name="ServletSettingsNode.instance">
                     <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.core.ServletSettingsNode.getBuildingNode"/>
                     <attr name="instanceClass" stringvalue="org.openide.nodes.Node"/>
                </file>
            </folder>
            
            <folder name="DebuggingAndExecuting">
                <file name="ServletSettingsNode.instance">
                     <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.core.ServletSettingsNode.getExecutionDebuggingNode"/>
                     <attr name="instanceClass" stringvalue="org.openide.nodes.Node"/>
                </file>
            </folder>
            
            <folder name="IDEConfiguration">
              <folder name="LookAndFeel">
                <file name="ServletSettingsNode.instance">
                     <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.core.ServletSettingsNode.getConfigurationNode"/>
                     <attr name="instanceClass" stringvalue="org.openide.nodes.Node"/>
                </file>
              </folder>
            </folder>
        </folder>
    </folder>

  <folder name="J2EE">
    <folder name="ExecPerformersStorage">
        <file name="org-netbeans-modules-web-execution-JspExecPerformer$Factory.instance">
            <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.execution.JspExecPerformer$Factory.getJspFactory" />
        </file>    
        <file name="org-netbeans-modules-web-execution-HtmlExecPerformer$Factory.instance">
            <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.execution.HtmlExecPerformer$Factory.getHtmlFactory" />
        </file>    
        <file name="org-netbeans-modules-web-execution-ServletExecPerformer$Factory.instance">
            <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.execution.ServletExecPerformer$Factory.getJavaFactory" />
        </file>    
        <file name="org-netbeans-modules-web-execution-WebModuleExecPerformer$Factory.instance">
            <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.execution.WebModuleExecPerformer$Factory.getWebModuleFactory" />
        </file>    
        <file name="org-netbeans-modules-web-execution-WebAppExecPerformer$Factory.instance">
            <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.execution.WebAppExecPerformer$Factory.getWebAppFactory" />
        </file>    
    </folder>
    <folder name="JSPParser">
    </folder>
  </folder>
  
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="XMLJ2eeAnnotation.xml" url="XMLJ2eeAnnotation.xml"/>
      </folder>
    </folder>
    
  <folder name="Shortcuts">
    <file name="C-Q.instance">
      <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.EditQueryStringAction"/>
    </file>
  </folder>      

  <folder name="Web">
    <folder name="MonitorSupport">
    </folder>
  </folder>
  
</filesystem>
