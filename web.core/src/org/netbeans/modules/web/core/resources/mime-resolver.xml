<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">

<!-- See #9582. -->

<MIME-resolver>

    <!-- XXX Should these file attributes continue to be honored? -->

    <!-- Resolve according to root element: -->
    <file>
        <ext name="jsp"/>
        <ext name="jspx"/>
        <ext name="jsf"/>
        <ext name="jspf"/>
        <resolver mime="text/x-jsp">
        </resolver>
    </file>
    <file>
        <ext name="tag"/>
        <ext name="tagx"/>
        <ext name="tagf"/>
        <resolver mime="text/x-tag">
        </resolver>
    </file>

    <file>
        <ext name="tld"/>
        <resolver mime="text/x-tld1.2">
            <xml-rule>
            	<doctype public-id="-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="tld"/>
        <resolver mime="text/x-tld1.1">
            <xml-rule>
            	<doctype public-id="-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="tld"/>
        <resolver mime="text/x-tld2.0">
            <xml-rule>
            	<element name="taglib" ns="http://java.sun.com/xml/ns/j2ee">
                    <attr name="version" text="2.0"/>
                </element>
            </xml-rule>
        </resolver>
    </file>  
</MIME-resolver>
