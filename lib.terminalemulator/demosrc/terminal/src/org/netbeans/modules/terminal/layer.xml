<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Other">
        </folder>
        <folder name="Window">
            <file name="org-netbeans-modules.terminal-TermWindowAction.instance"/>
            <file name="org-netbeans-modules.terminal-ShellTermAction.instance"/>
            <file name="org-netbeans-modules.terminal-CmdTermAction.instance"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="BuildProject">    <!-- aka Run -->
            <file name="ShellTermAction.shadow">
                <attr name="originalFile"
                      stringvalue="Actions/Window/org-netbeans-modules.terminal-ShellTermAction.instance"/>
                <attr name="position" intvalue="3"/>
            </file>
            <file name="CmdTermAction.shadow">
                <attr name="originalFile"
                      stringvalue="Actions/Window/org-netbeans-modules.terminal-CmdTermAction.instance"/>
                <attr name="position" intvalue="5"/>
            </file>
            <file name="TermSeparator.instance">
                <attr name="instanceClass"
                      stringvalue="javax.swing.JSeparator"/>
            </file>
        </folder>
        <folder name="Window">
            <folder name="Output">
                <file name="TermWindowAction.shadow">
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules.terminal-TermWindowAction.instance"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="TermTopComponent.settings" url="TermTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="output">
                <file name="TermTopComponent.wstcref" url="TermTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
