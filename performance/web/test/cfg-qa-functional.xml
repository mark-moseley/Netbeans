<mconfig name="QA functional config">

    <!--============================-->
    <!--   P E R F O R M A N C E    -->
    <!--============================-->

    <!-- testbag common for all suites to setup usedir -->
    <testbag testattribs="measure_editor_once or ui_responsiveness or measure_once or measure_ui or measure_setup or measure_openfiles or measure_nodepopup or measure_runtimepopup or measure_editor or measure_webactions" prio="1"
             compiler="qa-perf-compiler" executor="ide_perf_setup" name="Setup IDE for UI responsiveness">
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasuringWebSetup.class"/>
	    </patternset>
	</testset>
    </testbag>

    <!-- 1st round -->    
    <testbag testattribs="measure_once or ui_responsiveness or measure_ui or measure_openfiles" prio="2"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Actions" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebActions.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="measure_once or ui_responsiveness or measure_ui or measure_nodepopup" prio="4"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Dialogs" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebDialogs.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="measure_once or ui_responsiveness or measure_ui or measure_runtimepopup" prio="6"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Menus" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebMenus.class"/>
	    </patternset>
	</testset>
    </testbag>

    <!-- 2nd round -->    
    <testbag testattribs="measure_ui or ui_responsiveness or measure_openfiles" prio="12"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Actions" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebActions.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="measure_ui or ui_responsiveness or measure_nodepopup" prio="14"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Dialogs" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebDialogs.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="measure_ui or ui_responsiveness or measure_runtimepopup" prio="16"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Menus" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebMenus.class"/>
	    </patternset>
	</testset>
    </testbag>
    
    <!-- 3rd round -->    
    <testbag testattribs="measure_ui or ui_responsiveness or measure_openfiles" prio="22"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Actions" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebActions.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="measure_ui or ui_responsiveness or measure_nodepopup" prio="24"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Dialogs" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebDialogs.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="measure_ui or ui_responsiveness or measure_runtimepopup" prio="26"
             compiler="qa-perf-compiler" executor="ide_performance" name="Measure Web Menus" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureWebMenus.class"/>
	    </patternset>
	</testset>
    </testbag>
    
    <compiler name="qa-perf-compiler" antfile="build-qa-functional.xml" target="qa-perf-compiler"/>
    <executor name="ide_perf_setup" antfile="build-qa-functional.xml" target="runidesetup"/>
    <executor name="ide_performance" antfile="build-qa-functional.xml" target="runide_performance"/>
</mconfig>
