<mconfig name="enterprise pack qa-performance test config">

    <!-- testbag common for all suites to setup usedir -->
    <testbag testattribs="all or ui_responsiveness or menus or dialogs or actions" prio="1"
             executor="ide_perf_setup" name="UI responsiveness - setup">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasuringSetup.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 1st round -->    
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions.class"/>
            </patternset>
        </testset>
    </testbag>
<!-- no enteprise specific menus/dialogs tests so far    <testbag testattribs="all or ui_responsiveness or dialogs" prio="3"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or menus" prio="4"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
-->    
    <!-- 2nd round -->    
    <testbag testattribs="(all or ui_responsiveness or actions) and NOT once" prio="5"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions.class"/>
            </patternset>
        </testset>
    </testbag>
<!-- no enteprise specific menus/dialogs tests so far     <testbag testattribs="(all or ui_responsiveness or dialogs) and NOT once" prio="6"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or ui_responsiveness or menus) and NOT once" prio="7"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
-->        
    <!-- 3rd round -->    
    <testbag testattribs="(all or ui_responsiveness or actions) and NOT once" prio="8"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions.class"/>
            </patternset>
        </testset>
    </testbag>
<!-- no enteprise specific menus/dialogs tests so far     <testbag testattribs="(all or ui_responsiveness or dialogs) and NOT once" prio="9"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or ui_responsiveness or menus) and NOT once" prio="10"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
-->    
    <!-- Scan Classpath test bag -->    
    <testbag testattribs="all or classpath_scan" prio="100"
             executor="ide_performance_scan" name="Classpath Scan" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/scanclasspath/EPScanClasspath.class"/>
            </patternset>
        </testset>
    </testbag>

    
<!-- Memory Footprint Testbags -->

    <!-- J2EE Project workflow -->
    <testbag testattribs="all or footprint or footprint_EntepriseProject" prio="330"
             executor="ide_footprint" name="Footprint - J2EE project workflow" >
	<testset dir="footprint/src">
	    <patternset>
		<include name="footprint/J2EEProjectWorkflow.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="(all or footprint or footprint_EntepriseProject) and NOT once" prio="331"
             executor="ide_footprint" name="Footprint - J2EE project workflow" >
	<testset dir="footprint/src">
	    <patternset>
		<include name="footprint/J2EEProjectWorkflow.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="(all or footprint or footprint_EntepriseProject) and NOT once" prio="332"
             executor="ide_footprint" name="Footprint - J2EE project workflow" >
	<testset dir="footprint/src">
	    <patternset>
		<include name="footprint/J2EEProjectWorkflow.class"/>
	    </patternset>
	</testset>
    </testbag>

    <!-- executors & compilers -->
    <executor name="ide_perf_setup" antfile="build-qa-functional.xml" target="runidesetup"/>
    <executor name="ide_performance" antfile="build-qa-functional.xml" target="runidetest" default="true"/>
    <executor name="ide_performance_scan" antfile="build-qa-functional.xml" target="runidetest_scan" />
    <executor name="ide_footprint" antfile="build-qa-functional.xml" target="runidetest_footprint"/>
    
    <compiler name="qa-performance-compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>
    
</mconfig>
