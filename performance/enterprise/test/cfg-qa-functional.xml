    <!-- Scan Classpath test bag -->    
    <testbag testattribs="all or classpath_scan" prio="100"
             executor="ide_performance_scan" name="Classpath Scan" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/scanclasspath/EPScanClasspath.class"/>
            </patternset>
        </testset>
    </testbag>

    
<!-- Memory Footprint Testbags -->

    <!-- J2EE Project workflow -->
    <testbag testattribs="all or footprint or footprint_EntepriseProject" prio="330"
             executor="ide_footprint" name="Footprint - J2EE project workflow" >
	<testset dir="footprint/src">
	    <patternset>
		<include name="footprint/J2EEProjectWorkflow.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="(all or footprint or footprint_EntepriseProject) and NOT once" prio="331"
             executor="ide_footprint" name="Footprint - J2EE project workflow" >
	<testset dir="footprint/src">
	    <patternset>
		<include name="footprint/J2EEProjectWorkflow.class"/>
	    </patternset>
	</testset>
    </testbag>

    <testbag testattribs="(all or footprint or footprint_EntepriseProject) and NOT once" prio="332"
             executor="ide_footprint" name="Footprint - J2EE project workflow" >
	<testset dir="footprint/src">
	    <patternset>
		<include name="footprint/J2EEProjectWorkflow.class"/>
	    </patternset>
	</testset>
    </testbag>

    <!-- executors & compilers -->
    <executor name="ide_perf_setup" antfile="build-qa-functional.xml" target="runidesetup"/>
    <executor name="ide_performance" antfile="build-qa-functional.xml" target="runidetest" default="true"/>
    <executor name="ide_performance_scan" antfile="build-qa-functional.xml" target="runidetest_scan" />
    <executor name="ide_footprint" antfile="build-qa-functional.xml" target="runidetest_footprint"/>
    
    <compiler name="qa-performance-compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>
    
</mconfig>
