            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions4.class/Start Appserver"/>
            </patternset>
        </testset>
    </testbag>
<!-- no enteprise specific menus/dialogs tests so far    <testbag testattribs="all or ui_responsiveness or dialogs" prio="3"
