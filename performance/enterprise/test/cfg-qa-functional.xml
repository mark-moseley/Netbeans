<mconfig name="enterprise pack qa-performance test config">

    <!-- testbag common for all suites to setup usedir -->
    <testbag testattribs="all or ui_responsiveness or menus or dialogs or actions" prio="1"
             executor="ide_perf_setup" name="UI responsiveness - setup">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasuringSetup.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 1st round -->    
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions1.class"/>
                <include name="gui/EPMeasureActions1.class/testGCProjects" expectedFail="128458, 128805"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions2.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions3.class"/>
                <include name="gui/EPMeasureActions3.class/Schema Navigator Schema View" expectedFail="128585"/>
                <include name="gui/EPMeasureActions3.class/Schema Navigator Schema View mode" expectedFail="128596"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/EPMeasureActions4.class/Open Schema View" expectedFail="129789"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
