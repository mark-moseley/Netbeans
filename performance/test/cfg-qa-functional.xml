<mconfig name="Performance funtional tests config">
    
    <!-- testbag common for all suites to setup usedir -->
    <testbag testattribs="all or ui_responsiveness or menus or dialogs or actions or setup or editor or java or apisupport or profiler" prio="1"
             executor="ide_perf_setup" name="UI responsiveness - setup">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasuringSetup.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 1st round -->    
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class"/>
                <exclude name="gui/MeasureActions.class/Create Web Application project"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="2"
             executor="ide_performance_noscan" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class/Create Web Application project"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or dialogs" prio="3"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or menus" prio="4"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 2nd round -->    
    <testbag testattribs="(all or ui_responsiveness or actions) and NOT once" prio="5"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class"/>
                <exclude name="gui/MeasureActions.class/Create Web Application project"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="5"
             executor="ide_performance_noscan" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class/Create Web Application project"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="(all or ui_responsiveness or dialogs) and NOT once" prio="6"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or ui_responsiveness or menus) and NOT once" prio="7"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 3rd round -->    
    <testbag testattribs="(all or ui_responsiveness or actions) and NOT once" prio="8"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class"/>
                <exclude name="gui/MeasureActions.class/Create Web Application project"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or ui_responsiveness or actions" prio="8"
             executor="ide_performance_noscan" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class/Create Web Application project"/>
            </patternset>
        </testset>
    </testbag>

    <testbag testattribs="(all or ui_responsiveness or dialogs) and NOT once" prio="9"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or ui_responsiveness or menus) and NOT once" prio="10"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Scan Classpath test bag -->    
    <testbag testattribs="all or classpath_scan" prio="100"
             executor="ide_performance_scan" name="Classpath Scan" >
        <testset dir="qa-functional/src">
            <patternset>
                
                <include name="gui/scanclasspath/ScanClasspath.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Custom / Module test bags -->    
    <testbag testattribs="java" prio="100"
             executor="ide_performance" name="UI responsiveness of actions, dialogs or menus related to java, editor, refactoring" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureJava.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="profiler" prio="101"
             executor="ide_performance" name="UI responsiveness of actions, dialogs or menus related to profiler" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureProfiler.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="editor" prio="105"
             executor="ide_performance" name="UI responsiveness of actions, dialogs or menus related to editor" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureEditor.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="apisupport" prio="110"
             executor="ide_performance" name="UI responsiveness of actions, dialogs or menus related to apisupport" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureAPIsupport.class"/>
            </patternset>
        </testset>
    </testbag>
    
<!-- Memory Footprint Testbags -->
    <!-- Out Of The Box -->
    <testbag testattribs="all or footprint or footprint_OutOfTheBox" prio="300"
             executor="ide_footprint" name="Footprint - Out Of The Box Startup" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="footprint/OutOfTheBox.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_OutOfTheBox) and NOT once" prio="301"
             executor="ide_footprint" name="Footprint - Out Of The Box Startup" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/OutOfTheBox.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_OutOfTheBox) and NOT once" prio="302"
             executor="ide_footprint" name="Footprint - Out Of The Box Startup" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/OutOfTheBox.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- J2SE Project workflow -->
    <testbag testattribs="all or footprint or footprint_JavaProject" prio="310"
             executor="ide_footprint" name="Footprint - J2SE project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/J2SEProjectWorkflow.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_JavaProject) and NOT once" prio="311"
             executor="ide_footprint" name="Footprint - J2SE project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/J2SEProjectWorkflow.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_JavaProject) and NOT once" prio="312"
             executor="ide_footprint" name="Footprint - J2SE project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/J2SEProjectWorkflow.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Find Usages -->
    <testbag testattribs="all or footprint or footprint_FindUsages" prio="330"
             executor="ide_footprint" name="Footprint - Find usages" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/FindUsages.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_FindUsages) and NOT once" prio="331"
             executor="ide_footprint" name="Footprint - Find usages" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/FindUsages.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_FindUsages) and NOT once" prio="332"
             executor="ide_footprint" name="Footprint - Find usages" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/FindUsages.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Refactor Rename -->
    <testbag testattribs="all or footprint or footprint_RenameClass" prio="340"
             executor="ide_footprint" name="Footprint - Rename class" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/RefactoringRename.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_RenameClass) and NOT once" prio="341"
             executor="ide_footprint" name="Footprint - Rename class" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/RefactoringRename.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(all or footprint or footprint_RenameClass) and NOT once" prio="342"
             executor="ide_footprint" name="Footprint - Rename class" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/RefactoringRename.class"/>
            </patternset>
        </testset>
    </testbag>
    
    
    <!-- executors & compilers -->
    <executor name="ide_perf_setup" antfile="build-qa-functional.xml" target="runidesetup"/>
    <executor name="ide_performance" antfile="build-qa-functional.xml" target="runide_performance_as" default="true"/>
    <executor name="ide_performance_noscan" antfile="build-qa-functional.xml" target="runide_performance_as_noscan"/>
    <executor name="ide_performance_memory_usage" antfile="build-qa-functional.xml" target="runidetest_memory_usage"/>
    <executor name="ide_performance_scan" antfile="build-qa-functional.xml" target="runidetest_scan" />
    <executor name="ide_footprint" antfile="build-qa-functional.xml" target="runidetest_footprint"/>
    
    <compiler name="compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>
    
</mconfig>
