<project name="websphere6" default="netbeans" basedir=".">
    <import file="../../nbbuild/templates/projectized.xml"/>

    <target name="netbeans-extra">
        <locjar jarfile="${netbeans.dest.dir}/${cluster.dir}/${module.jar}">
            <branding name="jse"/>
            <fileset dir="src">
                <patternset>
                    <include name="**/*"/>
                </patternset>
            </fileset>
        </locjar>

        <property name="locale.dir" value="${netbeans.dest.dir}/${cluster.dir}/modules/locale"/>
    </target>
</project>

