  </folder>






  <folder name="Services">

     <!-- Register the HelpSet in lookup: -->
     <folder name="JavaHelp">
        <!-- Merge after Core IDE Help: -->
        <attr name="org-netbeans-modules-usersguide-above-regular.txt/com-sun-enterprise-tools-studio-j2ee-helpset.xml" boolvalue="true"/>
        <!-- Merge it after the Server Plugin Integration online help: -->
        <attr name="org-netbeans-modules-j2ee-server-above-regular.txt/com-sun-enterprise-tools-studio-j2ee-helpset.xml"
                   boolvalue="true"/>
        <!-- The following file supplies the IDE with the name of the helpset: -->
        <file name="com-sun-enterprise-tools-studio-j2ee-helpset.xml" url="com-sun-enterprise-tools-studio-j2ee-helpset.xml"/>
        <!-- Merge it before the last group of helpset TOC entries: -->
        <attr name="com-sun-enterprise-tools-studio-j2ee-helpset.xml/org-netbeans-modules-usersguide-below-regular.txt" 
                   boolvalue="true"/>
     </folder>
     
     <folder name="J2EEServers">
        <file name="PluginOptions.settings" url="PluginOptions.xml">
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.Bundle"/>
          <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/j2ee/sun/ide/resources/sun-cluster_16_pad.gif"/>
        </file>
     </folder>
     
  </folder>
  
  <folder name="Databases">
