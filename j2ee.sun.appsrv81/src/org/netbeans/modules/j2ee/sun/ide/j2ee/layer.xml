<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
	<folder name="J2EE">
	    <folder name="DeploymentPlugins">
		    <folder name="J2EE">
			    <file name="Descriptor" url="nbdep.xml"/>
