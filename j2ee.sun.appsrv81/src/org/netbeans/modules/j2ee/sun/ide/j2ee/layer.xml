					<attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.VerifierSupport"/>
				</file>
				<file name="J2eePlatformFactory.instance">
					<attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.PlatformFactory"/>
					<attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.PlatformFactory"/>
					<attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.J2eePlatformFactory"/>
				</file>
				<file name="OptionalFactory.instance">
					<attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.OptionalFactory"/>
					<attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.OptionalFactory"/>
					<attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.OptionalDeploymentManagerFactory"/>
				</file>
				<file name="AppServerPlanSplitter.instance">
					<attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.incrdeploy.DirectoryDeploymentFacade"/>
					<attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.incrdeploy.DirectoryDeploymentFacade"/>
					<attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.DeploymentPlanSplitter"/>
				</file>
				<file name="DConfigBeanUIManager.instance">
					<attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.DConfigBeanUIFactoryImpl"/>
					<attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.sun.ide.j2ee.DConfigBeanUIFactoryImpl"/>
					<attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.DConfigBeanUIFactory"/>
				</file>
				<folder name="DeploymentFileNames">
					<folder name="WAR">
						<file name="WEB-INF\sun-web.xml"/>
					</folder>
					<folder name="EJB">
						<file name="META-INF\sun-ejb-jar.xml"/>
						<file name="META-INF\sun-cmp-mappings.xml"/>
					</folder>
					<folder name="EAR">
						<file name="META-INF\sun-application.xml"/>
					</folder>
					<folder name="RAR">
						<file name="META-INF\sun-connector.xml"/>
					</folder>

					<folder name="CAR">
						<file name="META-INF\sun-client-application.xml"/>
					</folder>


				</folder>
			</folder>
		</folder>
 
	</folder>
