<!-- 
  These classes are excluded by default (defined in build.xml):
    "**/*Suite.class"
    "**/*$$*.class"
    "**/data/**"
    "**/hidden/**"
    "**/*Hidden.*"
    "**/*Hid.*"
-->

<mconfig name="Apisupport/refactoring qa-functional tests">

    <!--
    Stable tests are enumerated below
    -->
        
    <testbag testattribs="validation" executor="ide" name="basic" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="org/netbeans/modules/apisupport/refactoring/RenameTest.class"/>
            </patternset>
        </testset>
    </testbag>


  <executor name="ide" antfile="build-qa-functional.xml" target="runidetest" default="true"/>    
  <compiler name="qa-functional-compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>

</mconfig>
