|>CLASS:Draggable<|.prototype = {
    |>METHOD:scroll<|: function() {
        var optionTag;
        |>GLOBAL:collection<|.each(function(e,|>UNUSED:i<|) {
                optionTag = |>GLOBAL:document<|.createElement("option");
                optionTag.value = (e instanceof |>CLASS:Array<|) ? e[0] : e;
                if((typeof this.options.value == 'undefined') &&
                        ((e instanceof |>CLASS:Array<|) ? this.element.innerHTML == e[1] : e == optionTag.value)) optionTag.selected = true;
                if(this.options.value==optionTag.value) optionTag.selected = true;
                optionTag.appendChild(|>GLOBAL:document<|.createTextNode((e instanceof |>CLASS:Array<|) ? e[1] : e));
                |>GLOBAL:selectTag<|.appendChild(optionTag);
            }.bind(this));
        this.cached_selectTag = |>GLOBAL:selectTag<|;
    }
}

