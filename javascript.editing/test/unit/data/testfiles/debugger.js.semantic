function |>METHOD:about<|() {
    if (|>CLASS:Element<|.empty('about-content')) {
        new |>CLASS:Ajax<|.Updater('about-content', 'rails/info/properties', {
            method:     'get',
            |>METHOD:onFailure<|:  function() {
                |>CLASS:Element<|.classNames('about-content').add('failure')
                },
            |>METHOD:onComplete<|: function() {
                new |>CLASS:Effect<|.BlindDown('about-content', {
                    duration: 0.25
                })
                }
        });
    } else {
        new |>CLASS:Effect<|[|>CLASS:Element<|.visible('about-content') ? 
        'BlindUp' : 'BlindDown']('about-content', {
            duration: 0.25
        });
        debugger;
    }
}

