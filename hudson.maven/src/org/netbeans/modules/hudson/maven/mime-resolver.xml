<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.1//EN" "http://www.netbeans.org/dtds/mime-resolver-1_1.dtd">
<MIME-resolver>
    <file>
        <ext name="jelly"/>
        <resolver mime="text/x-jelly+xml"/>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-jelly+xml">
            <xml-rule>
                <element name="jelly" ns="jelly:core"/>
            </xml-rule>
        </resolver>
    </file>
</MIME-resolver>
