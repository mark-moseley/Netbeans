<project xmlns="http://maven.apache.org/POM/3.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/3.0.0 http://maven.apache.org/maven-v3_0_0.xsd">
    <pomVersion>3</pomVersion>
    <id>jemmy</id>
    <groupId>jemmy</groupId>
    <artifactId>jemmy</artifactId>
    <name>Jemmy</name>
    <currentVersion>2.2.5-SNAPSHOT</currentVersion>
    <url>http://jemmy.netbeans.org/</url>
    <issueTrackingUrl>http://jemmy.netbeans.org/servlets/ProjectIssues</issueTrackingUrl>
    <siteDirectory>/export/home/www/netbeans/jemmy</siteDirectory>
    <mailingLists>
        <mailingList>
            <name>issues@jemmy.netbeans.org</name>
            <subscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</subscribe>
            <unsubscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</unsubscribe>
            <archive>http://jemmy.netbeans.org/servlets/SummarizeList?listName=issues</archive>
        </mailingList>
        <mailingList>
            <name>announce@jemmy.netbeans.org</name>
            <subscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</subscribe>
            <unsubscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</unsubscribe>
            <archive>http://jemmy.netbeans.org/servlets/SummarizeList?listName=announce</archive>
        </mailingList>
        <mailingList>
            <name>cvs@jemmy.netbeans.org</name>
            <subscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</subscribe>
            <unsubscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</unsubscribe>
            <archive>http://jemmy.netbeans.org/servlets/SummarizeList?listName=cvs</archive>
        </mailingList>
        <mailingList>
            <name>dev@jemmy.netbeans.org</name>
            <subscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</subscribe>
            <unsubscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</unsubscribe>
            <archive>http://jemmy.netbeans.org/servlets/SummarizeList?listName=dev</archive>
        </mailingList>
        <mailingList>
            <name>users@jemmy.netbeans.org</name>
            <subscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</subscribe>
            <unsubscribe>http://jemmy.netbeans.org/servlets/ProjectMailingListList</unsubscribe>
            <archive>http://jemmy.netbeans.org/servlets/SummarizeList?listName=users</archive>
        </mailingList>
    </mailingLists>
    <build>
        <sourceDirectory>src</sourceDirectory>
        <unitTestSourceDirectory>testsuite/src</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
            <excludes>
                <exclude>org/netbeans/jemmy/testing/**</exclude>
            </excludes>
        </unitTest>
    </build>
    <reports>
        <report>maven-changelog-plugin</report>
        <report>maven-changes-plugin</report>
        <report>maven-emma-plugin</report>
        <report>maven-faq-plugin</report>
        <report>maven-file-activity-plugin</report>
        <report>maven-javadoc-plugin</report>
        <report>maven-jdepend-plugin</report>
        <report>maven-junit-report-plugin</report>
        <report>maven-jxr-plugin</report>
        <report>maven-license-plugin</report>
        <report>maven-simian-plugin</report>
        <report>maven-tasklist-plugin</report>
    </reports>
    <repository>
        <connection>scm:cvs:pserver:anoncvs@cvs.netbeans.org:/cvs:jemmy</connection>
    </repository>
    <dependencies>
        <dependency>
            <groupId>ant</groupId>
            <artifactId>ant</artifactId>
            <version>1.6.5</version>
        </dependency>
    </dependencies>
</project>
